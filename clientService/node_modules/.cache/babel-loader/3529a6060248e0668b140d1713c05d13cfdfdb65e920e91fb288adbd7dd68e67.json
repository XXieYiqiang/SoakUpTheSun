olumn label=\"文件名\" min-width=\"250\">\r\n        <template slot-scope=\"scope\">\r\n          <i :class=\"getFileIcon(scope.row.fileType)\" style=\"margin-right: 6px;\"></i>\r\n          {{ scope.row.fileName }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"文件类型\" width=\"180\" prop=\"fileType\" />\r\n      <el-table-column label=\"上传时间\" width=\"180\" prop=\"createTime\" />\r\n      <el-table-column label=\"修改时间\" width=\"180\" prop=\"updateTime\" />\r\n      <el-table-column label=\"大小\" width=\"100\" prop=\"fileSizeStr\" />\r\n\r\n      <el-table-column label=\"状态\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag :type=\"scope.row.statusType === '已处理' ? 'success' : 'warning'\" disable-transitions>\r\n            {{ scope.row.statusType }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"140\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\" icon=\"el-icon-view\" @click=\"handlePreview(scope.row)\"></el-button>\r\n          <el-button type=\"text\" icon=\"el-icon-edit\" @click=\"handleEdit(scope.row)\"></el-button>\r\n          <el-button type=\"text\" icon=\"el-icon-delete\" @click=\"handleDelete(scope.row)\"></el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div class=\"view-more\">\r\n      <el-link type=\"primary\" @click=\"handleViewMore\">查看全部文件</el-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'FileList',\r\n  props: {\r\n    title: {\r\n      type: String,\r\n      default: '业务文档库'\r\n    },\r\n    files: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  methods: {\r\n    getFileIcon(fileType) {\r\n      switch (fileType) {\r\n        case 'pdf':\r\n          return 'el-icon-document';\r\n        case 'docx':\r\n          return 'el-icon-document';\r\n        case 'xlsx