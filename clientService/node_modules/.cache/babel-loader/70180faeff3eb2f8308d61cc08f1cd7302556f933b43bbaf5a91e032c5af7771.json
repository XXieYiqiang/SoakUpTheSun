ox","scrollTop","scrollHeight"],"sources":["src/views/ChatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-app\">\r\n    <!-- 左侧联系人列表 -->\r\n    <div class=\"contact-list\">\r\n      <div\r\n        class=\"contact-item\"\r\n        v-for=\"contact in contacts\"\r\n        :key=\"contact.id\"\r\n        :class=\"{ active: contact.id === activeContact.id }\"\r\n        @click=\"selectContact(contact)\"\r\n      >\r\n        <div class=\"avatar\">{{ contact.name.charAt(0) }}</div>\r\n        <div class=\"name\">{{ contact.name }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 右侧聊天窗口 -->\r\n    <div class=\"chat-window\">\r\n      <div class=\"chat-header\">\r\n        {{ activeContact.name }}\r\n      </div>\r\n\r\n      <div class=\"chat-messages\" ref=\"messageBox\">\r\n        <div\r\n          v-for=\"msg in activeContact.messages\"\r\n          :key=\"msg.id\"\r\n          :class=\"['chat-message', msg.sender === 'user' ? 'user' : 'ai']\"\r\n        >\r\n          <div class=\"bubble\">{{ msg.text }}</div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"chat-input\">\r\n        <el-input\r\n          v-model=\"newMessage\"\r\n          placeholder=\"输入消息...\"\r\n          @keyup.enter.native=\"sendMessage\"\r\n          size=\"small\"\r\n        />\r\n        <el-button type=\"primary\" icon=\"el-icon-sent\" @click=\"sendMessage\">发送</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      contacts: [\r\n        {\r\n          id: 1,\r\n          name: 'AI助手',\r\n          messages: [\r\n            { id: 1, text: '你好，我是AI助手。', sender: 'ai' },\r\n            { id: 2, text: '你可以向我提问！', sender: 'ai' }\r\n          ]\r\n        },\r\n        {\r\n          id: 2,\r\n          name: '小明',\r\n          messages: [\r\n            { id: 1, text: '你好小明。', sender: 'user' },\r\n            { id: 2, text: '早上好！', sender: 'ai' }\r\n          ]\r\n        }\r\n      ],\r\n      activeContact: {},\r\n      newMessage: ''\r\n    };\r\n  },\r\n  mounted() {\r\n    this.activeContact = this.contacts[0];\r\n  },\r\n  methods: {\r\n    selectContact(contact) {\r\n      this.activeContact = contact;\r\n      this.scrollToBottom();\r\n    },\r\n    sendMessage() {\r\n      if (!this.newMessage.trim()) return;\r\n\r\n      const message = {\r\n        id: Date.now(),\r\n        text: this.newMessage,\r\n        sender: 'user'\r\n      };\r\n      this.activeContact.messages.push(message);\r\n\r\n      // 模拟 AI 回复\r\n      setTimeout(() => {\r\n        this.activeContact.messages.push({\r\n          id: Date.now() + 1,\r\n          text: '已收到你的消息：“' + this.newMessage + '”',\r\n          sender: 'ai'\r\n        });\r\n        this.scrollToBottom();\r\n      }, 800);\r\n\r\n      this.newMessage = '';\r\n      this.scrollToBottom();\r\n    },\r\n    scrollToBottom() {\r\n      this.$nextTick(() => {\r\n        const box = this.$refs.messageBox;\r\n        if (box) box.scrollTop = box.scrollHeight;\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.chat-app {\r\n  display: flex;\r\n  height: 100vh;\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n.contact-list {\r\n  width: 240px;\r\n  background-color: #fff;\r\n  border-right: 1px solid #dcdfe6;\r\n  overflow-y: auto;\r\n}\r\n\r\n.contact-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 12px;\r\n  cursor: pointer;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.contact-item.active {\r\n  background-color: #e6f7ff;\r\n}\r\n\r\n.avatar {\r\n  width: 36px;\r\n  height: 36px;\r\n  background-color: #409EFF;\r\n  color: #fff;\r\n  font-weight: bold;\r\n  border-radius: 50%;\r\n  text-align: center;\r\n  line-height: 36px;\r\n  margin-right: 12px;\r\n}\r\n\r\n.chat-window {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.chat-header {\r\n  height: 60px;\r\n  padding: 16px;\r\n  background-color: #fff;\r\n  font-weight: bold;\r\n  font-size: 16px;\r\n  border-bottom: 1px solid #dcdfe6;\r\n}\r\n\r\n.chat-messages {\r\n  flex: 1;\r\n  padding: 16px;\r\n  overflow-y: auto;\r\n  background-color: #f7f8fa;\r\n}\r\n\r\n.chat-message {\r\n  margin-bottom: 12px;\r\n  display: flex;\r\n}\r\n\r\n.chat-message.user {\r\n  justify-content: flex-end;\r\n}\r\n\r\n.chat-message.ai {\r\n  justify-content: flex-start;\r\n}\r\n\r\n.bubble {\r\n  max-width: 60%;\r\n  padding: 10px 14px;\r\n  border-radius: 16px;\r\n  line-height: 1.4;\r\n  font-size: 14px;\r\n  word-break: break-word;\r\n  background-color: #409EFF;\r\n  color: white;\r\n}\r\n\r\n.chat-message.ai .bubble {\r\n  background-color: #e4e7ed;\r\n  color: #333;\r\n}\r\n\r\n.chat-input {\r\n  padding: 12px 16px;\r\n  background-color: #fff;\r\n  border-top: 1px solid #dcdfe6;\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;iCA8CA;EACAA,KAAA;IACA;MACAC,QAAA,GACA;QACAC,EAAA;QACAC,IAAA;QACAC,QAAA,GACA;UAAAF,EAAA;UAAAG,IAAA;UAAAC,MAAA;QAAA,GACA;UAAAJ,EAAA;UAAAG,IAAA;UAAAC,MAAA;QAAA;MAEA,GACA;QACAJ,EAAA;QACAC,IAAA;QACAC,QAAA,GACA;UAAAF,EAAA;UAAAG,IAAA;UAAAC,MAAA;QAAA,GACA;UAAAJ,EAAA;UAAAG,IAAA;UAAAC,MAAA;QAAA;MAEA,EACA;MACAC,aAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAF,aAAA,QAAAN,QAAA;EACA;EACAS,OAAA;IACAC,cAAAC,OAAA;MACA,KAAAL,aAAA,GAAAK,OAAA;MACA,KAAAC,cAAA;IACA;IACAC,YAAA;MACA,UAAAN,UAAA,CAAAO,IAAA;MAEA,MAAAC,OAAA;QACAd,EAAA,EAAAe,IAAA,CAAAC,GAAA;QACAb,IAAA,OAAAG,UAAA;QACAF,MAAA;MACA;MACA,KAAAC,aAAA,CAAAH,QAAA,CAAAe,IAAA,CAAAH,OAAA;;MAEA;MACAI,UAAA;QACA,KAAAb,aAAA,CAAAH,QAAA,CAAAe,IAAA;UACAjB,EAAA,EAAAe,IAAA,CAAAC,GAAA;UACAb,IAAA,qBAAAG,UAAA;UACAF,MAAA;QACA;QACA,KAAAO,cAAA;MACA;MAEA,KAAAL,UAAA;MACA,KAAAK,cAAA;IACA;IACAA,eAAA;MACA,KAAAQ,SAAA;QACA,MAAAC,GAAA,QAAAC,KAAA,CAAAC,UAAA;QACA,IAAAF,GAAA,EAAAA,GAAA,CAAAG,SAAA,GAAAH,GAAA,CAAAI,YAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"script","externalDependencies":[]}