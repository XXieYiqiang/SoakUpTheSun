v>\r\n        <div class=\"msg-content\" v-html=\"msg.renderedContent\"></div>\r\n        <div class=\"avatar\" v-if=\"msg.role === 'user'\">üßë</div>\r\n      </div>\r\n\r\n      <!-- AI ÊâìÂ≠ó‰∏≠... -->\r\n      <div class=\"ai-loading\" v-if=\"loading\">\r\n        <span style=\"margin-right: 10px;\">AI Ê≠£Âú®ËæìÂÖ•</span>\r\n        <div class=\"dot-flash\">\r\n          <div class=\"dot\" v-for=\"i in 3\" :key=\"i\" :style=\"{ animationDelay: i * 0.2 + 's' }\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Â∫ïÈÉ®ËæìÂÖ•Ê†è -->\r\n    <div class=\"input-bar\" :class=\"{ 'input-bottom': chatMessages.length > 0 }\">\r\n      <div class=\"input-wrapper\">\r\n        <input v-model=\"userInput\" @keyup.enter=\"handleSend\" placeholder=\"ËØ∑ËæìÂÖ•ÈóÆÈ¢ò...\" class=\"input-box\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<!-- <script>\r\nimport { marked } from 'marked'\r\nimport hljs from 'highlight.js'\r\nimport 'highlight.js/styles/github.css' // ‰Ω†ÂèØ‰ª•Êç¢ÊàêÂÖ∂‰ªñ‰∏ªÈ¢òÊ†∑Âºè\r\n\r\nconst renderer = new marked.Renderer()\r\n\r\nrenderer.code = function (code, infostring, escaped) {\r\n  // 1. Á°Æ‰øù code ÊòØÂ≠óÁ¨¶‰∏≤ÔºåÈùûÂ≠óÁ¨¶‰∏≤Áî® JSON.stringify ÁæéÂåñ\r\n  if (typeof code !== 'string') {\r\n    try {\r\n      code = JSON.stringify(code, null, 2)\r\n    } catch (e) {\r\n      code = String(code || '')\r\n    }\r\n  }\r\n\r\n  const lang = (infostring || '').trim()\r\n  let highlighted;\r\n  let displayLang = lang && hljs.getLanguage(lang) ? lang : ''\r\n\r\n  if (displayLang) {\r\n    highlighted = hljs.highlight(code, { language: displayLang }).value\r\n  } else {\r\n    const autoResult = hljs.highlightAuto(code)\r\n    highlighted = autoResult.value\r\n    displayLang = autoResult.language || 'plaintext'\r\n  }\r\n\r\n  return `\r\n    <div class=\"code-block\">\r\n      <div class=\"code-lang\">${displayLang}</div>\r\n      <pre><code class=\"hljs language-${displayLang}\">${highlighted}</code></pre>\r\n    </div>\r\n  `\r\n}\r\n\r\nmarked.setOptions({\r\n  breaks: true,\r\n  gfm: true,\r\n  renderer\r\n  // highlight(code, lang) {\r\n  //   return lang && hljs.getLanguage(lang)\r\n  //     ? hljs.highlight(code, { language: lang }).value\r\n  //     : hljs.highlightAuto(code).value;\r\n  // }\r\n})\r\n\r\n// ‚úÖ Áªü‰∏ÄÊ∏≤ÊüìÂáΩÊï∞ÔºàÂÖºÂÆπ Markdown Â≠óÁ¨¶‰∏≤Âíå code JSON ÂùóÔºâ\r\nfunction renderMarkdownOrCode(content) {\r\n  if (typeof content === 'object' && content.type === 'code') {\r\n    const lang = content.lang || ''\r\n    const code = content.text || ''\r\n    return renderer.code(code, lang)\r\n  } else if (typeof content === 'string') {\r\n    return marked.parse(content)\r\n  } else {\r\n    return String(content)\r\n  }\r\n}\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      chatMessages: [\r\n        {\r\n          id: Date.now(),\r\n          role: 'ai',\r\n          content: 'ÂòøÔºÅÊàëÊòØ **AI Â∞èÂä©Êâã**ÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû',\r\n          renderedContent: '',\r\n        },\r\n      ],\r\n      userInput: '',\r\n      socket: null,\r\n      loading: false,\r\n      currentAiMessageId: null,\r\n      typingQueue: [],\r\n      typingTimer: null,\r\n      isTyping: false,\r\n    }\r\n  },\r\n  mounted() {\r\n    // ÂàùÂßãÂåñ Markdown Ê∏≤Êüì\r\n    this.chatMessages.forEach((msg) => {\r\n      msg.renderedContent = marked.parse(msg.content)\r\n    })\r\n    this.initSocket()\r\n  },\r\n  beforeDestroy() {\r\n    if (this.typingTimer) {\r\n      clearTimeout(this.typingTimer)\r\n    }\r\n  },\r\n  methods: {\r\n    generateId() {\r\n      return Date.now() + Math.random()\r\n    },\r\n    initSocket() {\r\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws')\r\n\r\n      this.socket.addEventListener('open', () => {\r\n        console.log('WebSocket Â∑≤ËøûÊé•')\r\n      })\r\n\r\n      this.socket.addEventListener('message', (event) => {\r\n        if (event.data === '[DONE]') {\r\n          this.loading = false\r\n          this.currentAiMessageId = null\r\n          return\r\n        }\r\n        this.queueTextForTyping(event.data)\r\n      })\r\n\r\n      this.socket.addEventListener('close', () => {\r\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠')\r\n      })\r\n\r\n      this.socket.addEventListener('error', (error) => {\r\n        console.error('WebSocket ÈîôËØØ:', error)\r\n      })\r\n    },\r\n    handleSend() {\r\n      const msg = this.userInput.trim()\r\n      if (!msg || this.socket.readyState !== WebSocket.OPEN) return\r\n\r\n      this.chatMessages.push({\r\n        id: this.generateId(),\r\n        role: 'user',\r\n        content: msg,\r\n        renderedContent: marked.parse(msg),\r\n      })\r\n\r\n      this.userInput = ''\r\n      this.loading = true\r\n      this.scrollToBottom()\r\n\r\n      const context = this.chatMessages\r\n        .map((m) => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content)\r\n        .join('\\n')\r\n\r\n      this.socket.send(context)\r\n    },\r\n    queueTextForTyping(text) {\r\n      const chars = Array.from(text)\r\n      this.typingQueue.push(...chars)\r\n      if (!this.isTyping) {\r\n        this.startTyping()\r\n      }\r\n    },\r\n    startTyping() {\r\n      this.isTyping = true\r\n\r\n      let last = this.chatMessages.find((msg) => msg.id === this.currentAiMessageId)\r\n      if (!last) {\r\n        last = { id: this.generateId(), role: 'ai', content: '', renderedContent: '' }\r\n        this.chatMessages.push(last)\r\n        this.currentAiMessageId = last.id\r\n      }\r\n\r\n      const typeNext = () => {\r\n        if (this.typingQueue.length === 0) {\r\n          this.isTyping = false\r\n          return\r\n        }\r\n\r\n        const nextChar = this.typingQueue.shift()\r\n        last.content += nextChar\r\n        last.renderedContent = marked.parse(last.content)\r\n        this.$forceUpdate()\r\n        this.scrollToBottom()\r\n\r\n        this.$nextTick(() => {\r\n          hljs.highlightAll() // ËØ≠Ê≥ïÈ´ò‰∫ÆÈáçÊñ∞ÊâßË°å\r\n        })\r\n\r\n        this.typingTimer = setTimeout(typeNext, 20)\r\n      }\r\n\r\n      typeNext()\r\n    },\r\n    scrollToBottom() {\r\n      this.$nextTick(() => {\r\n        const el = this.$refs.chatList\r\n        if (el) el.scrollTop = el.scrollHeight\r\n      })\r\n    },\r\n  },\r\n}\r\n</script> -->\r\n\r\n<script>\r\nimport { marked } from 'marked'\r\nimport hljs from 'highlight.js'\r\nimport 'highlight.js/styles/github.css' // ÂèØÊõøÊç¢Êàê‰Ω†ÂñúÊ¨¢ÁöÑ‰∏ªÈ¢ò\r\n\r\n// Ëá™ÂÆö‰πâÊ∏≤ÊüìÂô®\r\nconst renderer = new marked.Renderer()\r\n\r\nrenderer.code = function (code, infostring, escaped) {\r\n  // ÈùûÂ≠óÁ¨¶‰∏≤ÊÉÖÂÜµÔºåËΩ¨Êç¢‰∏∫ JSON Â≠óÁ¨¶‰∏≤\r\n  if (typeof code !== 'string') {\r\n    try {\r\n      code = JSON.stringify(code, null, 2)\r\n    } catch {\r\n      code = String(code || '')\r\n    }\r\n  }\r\n\r\n  const lang = (infostring || '').trim()\r\n  let highlighted;\r\n  let displayLang = lang && hljs.getLanguage(lang) ? lang : ''\r\n\r\n  if (displayLang) {\r\n    highlighted = hljs.highlight(code, { language: displayLang }).value\r\n  } else {\r\n    const autoResult = hljs.highlightAuto(code)\r\n    highlighted = autoResult.value\r\n    displayLang = autoResult.language || 'plaintext'\r\n  }\r\n\r\n  return `\r\n    <div class=\"code-block\">\r\n      <div class=\"code-lang\">${displayLang}</div>\r\n      <pre><code class=\"hljs language-${displayLang}\">${highlighted}</code></pre>\r\n    </div>\r\n  `\r\n}\r\n\r\nmarked.setOptions({\r\n  breaks: true,\r\n  gfm: true,\r\n  renderer\r\n})\r\n\r\n// ‚úÖ Ê∏≤ÊüìÂáΩÊï∞ÔºöÂÖºÂÆπ Markdown Â≠óÁ¨¶‰∏≤Âíå code ÂØπË±°\r\nfunction renderMarkdownOrCode(content) {\r\n  if (content && typeof content === 'object' && content.type === 'code') {\r\n    return renderer.code(content.text || '', content.lang || '')\r\n  }\r\n  if (typeof content === 'string') {\r\n    return marked.parse(content)\r\n  }\r\n  return String(content)\r\n}\r\n\r\nexport default {\r\n  data() {\r\n    return {\