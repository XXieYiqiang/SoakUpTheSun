{"ast":null,"code":"\"use strict\";\n\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\n// JavaScript ÈÄªËæë‰øùÊåÅ‰∏çÂèò\nvar _default = exports.default = {\n  name: 'FileList',\n  props: {\n    title: {\n      type: String,\n      default: '‰∏öÂä°ÊñáÊ°£Â∫ì'\n    },\n    files: {\n      type: Array,\n      required: true\n    },\n    knowledgeCode: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {\n      fileList: [],\n      allowedTypes: ['application/pdf', 'application/msword', 'application/vnd.openxmlformats-officedocument.wordprocessingml.document', 'application/vnd.ms-excel', 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet', 'text/plain'],\n      loadingInstance: null\n    };\n  },\n  methods: {\n    beforeUpload(file) {\n      const isAllowed = this.allowedTypes.includes(file.type);\n      const isLt5M = file.size / 1024 / 1024 < 5;\n      if (!isAllowed) {\n        this.$message.error('‚ùå ‰ªÖÊîØÊåÅ PDF„ÄÅWord„ÄÅExcel„ÄÅTXT ÊñáÊ°£ÔºåÁ¶ÅÊ≠¢‰∏ä‰º†ÂõæÁâá„ÄÅÈü≥ËßÜÈ¢ëÔºÅ');\n        return false;\n      }\n      if (!isLt5M) {\n        this.$message.error('‚ùå Êñá‰ª∂Â§ßÂ∞è‰∏çËÉΩË∂ÖËøá 5MBÔºÅ');\n        return false;\n      }\n\n      // ‚úÖ ÂºÄÂêØÂÖ®Â±Ä loading\n      this.loadingInstance = this.$loading({\n        lock: true,\n        text: 'Êñá‰ª∂Â∑≤‰∏ä‰º†ÔºåÊ≠£Âú®ËøõË°åÊô∫ËÉΩËß£ÊûêÔºåËØ∑ËÄêÂøÉÁ≠âÂæÖÁ∫¶ 5~10 Áßí‚Ä¶',\n        spinner: 'el-icon-loading',\n        background: 'rgba(0, 0, 0, 0.7)' // Â¢ûÂä†ËÉåÊôØÈÄèÊòéÂ∫¶ÔºåÂåπÈÖçÁßëÊäÄ‰∏ªÈ¢ò\n      });\n      return true;\n    },\n    handleSuccess(response, file) {\n      this.closeLoading();\n      this.$emit('dataLoadding');\n      this.$message.success(`‚úÖ \"${file.name}\" ‰∏ä‰º†ÊàêÂäü`);\n      this.fileList = [];\n    },\n    handleError(err, file) {\n      this.closeLoading();\n      this.$message.error(`‚ùå \"${file.name}\" ‰∏ä‰º†Â§±Ë¥•`);\n      this.fileList = [];\n    },\n    handleExceed(files, fileList) {\n      this.$message.warning('‚ö†Ô∏è ÊúÄÂ§öÂè™ËÉΩ‰∏ä‰º† 1 ‰∏™Êñá‰ª∂');\n    },\n    // ÂÖ≥Èó≠ loadingÔºàÁ°Æ‰øù‰∏ÄÂÆö‰ºöË¢´Ë∞ÉÁî®Ôºâ\n    closeLoading() {\n      if (this.loadingInstance) {\n        this.loadingInstance.close();\n        this.loadingInstance = null;\n      }\n    },\n    /**\r\n     * * @param fileType Ëé∑ÂèñÊñá‰ª∂ icon ÂõæÊ†á\r\n     */\n    getFileIcon(fileType) {\n      switch (fileType) {\n        case 'pdf':\n          return 'el-icon-document';\n        case 'docx':\n          return 'el-icon-document';\n        case 'xlsx':\n          return 'el-icon-s-data';\n        case 'pptx':\n          return 'el-icon-s-marketing';\n        default:\n          return 'el-icon-document';\n      }\n    },\n    /** Êñá‰ª∂È¢ÑËßà */\n    handlePreview(file) {\n      this.$message(`È¢ÑËßàÊñá‰ª∂Ôºö${file.fileName}`);\n      const fileSuffix = (file.extendName || '').toLowerCase();\n\n      // Êã¶Êà™ÂõæÁâá„ÄÅÈü≥È¢ë„ÄÅËßÜÈ¢ëÁ±ªÂûãÔºå‰∏çÂ§ÑÁêÜ\n      const IMAGE_TYPES = ['png', 'jpg', 'jpeg', 'gif', 'svg'];\n      const VIDEO_TYPES = ['mp4'];\n      const AUDIO_TYPES = ['mp3', 'flac'];\n      if (IMAGE_TYPES.includes(fileSuffix) || VIDEO_TYPES.includes(fileSuffix) || AUDIO_TYPES.includes(fileSuffix)) {\n        this.$message.warning('ËØ•Êñá‰ª∂Á±ªÂûã‰∏çÊîØÊåÅÈ¢ÑËßà');\n        return;\n      }\n\n      // Office Êñá‰ª∂Âú®Á∫øÈ¢ÑËßà\n      if ([...officeFileType].includes(fileSuffix)) {\n        this.getFileOnlineViewPathByOffice(file);\n        return;\n      }\n\n      // PDF Êñá‰ª∂Êñ∞Ê†áÁ≠æÈ°µÊâìÂºÄ\n      if (fileSuffix === 'pdf') {\n        window.open(this.getViewFilePath(file), '_blank');\n        return;\n      }\n\n      // Áªü‰∏Ä yaml => yml\n      let codeSuffix = fileSuffix === 'yaml' ? 'yml' : fileSuffix;\n\n      // Markdown ÊñáÊ°£\n      if (markdownFileType.includes(codeSuffix)) {\n        Vue.prototype.$openBox.markdownPreview({\n          fileInfo: file,\n          editable: false\n        });\n        return;\n      }\n\n      // ÊñáÊú¨/‰ª£Á†ÅÁ±ªÊñá‰ª∂ Êàñ Êó†Êâ©Â±ïÂêçÊñá‰ª∂\n      if (fileSuffixCodeModeMap.has(codeSuffix) || file.isDir === 0 && !file.extendName) {\n        Vue.prototype.$openBox.codePreview({\n          fileInfo: file,\n          isEdit: false\n        });\n        return;\n      }\n\n      // ÂÖ∂‰ªñ‰∏çÊîØÊåÅÈ¢ÑËßà\n      this.$message.warning('ÊöÇ‰∏çÊîØÊåÅËØ•Á±ªÂûãÊñá‰ª∂È¢ÑËßà');\n    },\n    /**\r\n     * * @param file Êñá‰ª∂Âà†Èô§Êìç‰Ωú\r\n     */\n    handleDelete(file) {\n      this.$confirm(`Á°ÆÂÆöÂà†Èô§ ${file.fileName} ÂêóÔºü`, 'Âà†Èô§Á°ÆËÆ§').then(() => {\n        this.$emit('fileDelete', file);\n      }).catch(() => {});\n    },\n    handleViewMore() {\n      this.$message('Âä†ËΩΩÊõ¥Â§öÊñá‰ª∂...');\n    }\n  }\n};","map":{"version":3,"names":["_default","exports","default","name","props","title","type","String","files","Array","required","knowledgeCode","data","fileList","allowedTypes","loadingInstance","methods","beforeUpload","file","isAllowed","includes","isLt5M","size","$message","error","$loading","lock","text","spinner","background","handleSuccess","response","closeLoading","$emit","success","handleError","err","handleExceed","warning","close","getFileIcon","fileType","handlePreview","fileName","fileSuffix","extendName","toLowerCase","IMAGE_TYPES","VIDEO_TYPES","AUDIO_TYPES","officeFileType","getFileOnlineViewPathByOffice","window","open","getViewFilePath","codeSuffix","markdownFileType","Vue","prototype","$openBox","markdownPreview","fileInfo","editable","fileSuffixCodeModeMap","has","isDir","codePreview","isEdit","handleDelete","$confirm","then","catch","handleViewMore"],"sources":["src/views/knowledge/components/FileList.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-list tech-card\">\r\n    <div class=\"file-list-header\">\r\n      <div>\r\n        <h3 class=\"neon-title-small\">{{ title }} <span class=\"sub-text\">// DOCUMENT LOG</span></h3>\r\n        <p class=\"sub-text\">ÊúÄËøëËÆøÈóÆ ¬∑ {{ files.length }} ‰∏™Êñá‰ª∂</p>\r\n      </div>\r\n      <div class=\"btn-group\">\r\n        <el-upload class=\"upload-demo\" action=\"/api/knowledge/upload\" :file-list=\"fileList\"\r\n          :data=\"{ knowledgeCode: knowledgeCode }\" :before-upload=\"beforeUpload\" :on-success=\"handleSuccess\"\r\n          :on-error=\"handleError\" :show-file-list=\"false\" :limit=\"1\" :on-exceed=\"handleExceed\" multiple auto-upload\r\n          list-type=\"text\">\r\n          <el-button type=\"primary\" size=\"small\" class=\"tech-button primary-btn\">\r\n            <i class=\"el-icon-upload2\"></i> üì§ ÈÄâÊã©Âπ∂‰∏ä‰º†Êñá‰ª∂\r\n          </el-button>\r\n        </el-upload>\r\n      </div>\r\n    </div>\r\n\r\n    <el-table :data=\"files\" border stripe class=\"tech-table\">\r\n      <el-table-column label=\"Êñá‰ª∂Âêç\" min-width=\"250\">\r\n        <template slot-scope=\"scope\">\r\n          <i :class=\"getFileIcon(scope.row.fileType)\" class=\"tech-icon-small\"></i>\r\n          <span class=\"file-name-text\">{{ scope.row.fileName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"Êñá‰ª∂Á±ªÂûã\" width=\"100\" prop=\"fileType\" />\r\n      <el-table-column label=\"‰∏ä‰º†Êó∂Èó¥\" width=\"180\" prop=\"createTime\" class=\"monospace-text\"/>\r\n      <el-table-column label=\"‰øÆÊîπÊó∂Èó¥\" width=\"180\" prop=\"updateTime\" class=\"monospace-text\"/>\r\n      <el-table-column label=\"Â§ßÂ∞è\" width=\"100\" prop=\"fileSizeStr\" class=\"monospace-text\"/>\r\n\r\n      <el-table-column label=\"Áä∂ÊÄÅ\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag :class=\"scope.row.statusType === 'Â∑≤Â§ÑÁêÜ' ? 'tech-tag-success' : 'tech-tag-warning'\" disable-transitions>\r\n            {{ scope.row.statusType }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"Êìç‰Ωú\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\" icon=\"el-icon-view\" class=\"tech-text-btn\"\r\n            @click=\"$file.handleKnowledgeFileNameClick(scope.row)\"></el-button>\r\n          <el-button type=\"text\" icon=\"el-icon-delete\" class=\"tech-text-btn delete-btn\"\r\n            @click=\"handleDelete(scope.row)\"></el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div class=\"view-more\">\r\n      <el-link class=\"tech-link\" @click=\"handleViewMore\">Êü•ÁúãÂÖ®ÈÉ®Êñá‰ª∂ >></el-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// JavaScript ÈÄªËæë‰øùÊåÅ‰∏çÂèò\r\nexport default {\r\n  name: 'FileList',\r\n  props: {\r\n    title: {\r\n      type: String,\r\n      default: '‰∏öÂä°ÊñáÊ°£Â∫ì'\r\n    },\r\n    files: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    knowledgeCode: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      fileList: [],\r\n      allowedTypes: [\r\n        'application/pdf',\r\n        'application/msword',\r\n        'application/vnd.openxmlformats-officedocument.wordprocessingml.document',\r\n        'application/vnd.ms-excel',\r\n        'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet',\r\n        'text/plain',\r\n      ],\r\n      loadingInstance: null,\r\n    }\r\n  },\r\n  methods: {\r\n    beforeUpload(file) {\r\n      const isAllowed = this.allowedTypes.includes(file.type);\r\n      const isLt5M = file.size / 1024 / 1024 < 5;\r\n\r\n      if (!isAllowed) {\r\n        this.$message.error('‚ùå ‰ªÖÊîØÊåÅ PDF„ÄÅWord„ÄÅExcel„ÄÅTXT ÊñáÊ°£ÔºåÁ¶ÅÊ≠¢‰∏ä‰º†ÂõæÁâá„ÄÅÈü≥ËßÜÈ¢ëÔºÅ');\r\n        return false;\r\n      }\r\n      if (!isLt5M) {\r\n        this.$message.error('‚ùå Êñá‰ª∂Â§ßÂ∞è‰∏çËÉΩË∂ÖËøá 5MBÔºÅ');\r\n        return false;\r\n      }\r\n\r\n      // ‚úÖ ÂºÄÂêØÂÖ®Â±Ä loading\r\n      this.loadingInstance = this.$loading({\r\n        lock: true,\r\n        text: 'Êñá‰ª∂Â∑≤‰∏ä‰º†ÔºåÊ≠£Âú®ËøõË°åÊô∫ËÉΩËß£ÊûêÔºåËØ∑ËÄêÂøÉÁ≠âÂæÖÁ∫¶ 5~10 Áßí‚Ä¶',\r\n        spinner: 'el-icon-loading',\r\n        background: 'rgba(0, 0, 0, 0.7)', // Â¢ûÂä†ËÉåÊôØÈÄèÊòéÂ∫¶ÔºåÂåπÈÖçÁßëÊäÄ‰∏ªÈ¢ò\r\n      });\r\n      return true;\r\n    },\r\n    handleSuccess(response, file) {\r\n      this.closeLoading();\r\n      this.$emit('dataLoadding');\r\n      this.$message.success(`‚úÖ \"${file.name}\" ‰∏ä‰º†ÊàêÂäü`);\r\n      this.fileList = []\r\n    },\r\n    handleError(err, file) {\r\n      this.closeLoading();\r\n      this.$message.error(`‚ùå \"${file.name}\" ‰∏ä‰º†Â§±Ë¥•`);\r\n      this.fileList = []\r\n    },\r\n    handleExceed(files, fileList) {\r\n      this.$message.warning('‚ö†Ô∏è ÊúÄÂ§öÂè™ËÉΩ‰∏ä‰º† 1 ‰∏™Êñá‰ª∂');\r\n    },\r\n    // ÂÖ≥Èó≠ loadingÔºàÁ°Æ‰øù‰∏ÄÂÆö‰ºöË¢´Ë∞ÉÁî®Ôºâ\r\n    closeLoading() {\r\n      if (this.loadingInstance) {\r\n        this.loadingInstance.close();\r\n        this.loadingInstance = null;\r\n      }\r\n    },\r\n    /**\r\n     * * @param fileType Ëé∑ÂèñÊñá‰ª∂ icon ÂõæÊ†á\r\n     */\r\n    getFileIcon(fileType) {\r\n      switch (fileType) {\r\n        case 'pdf':\r\n          return 'el-icon-document';\r\n        case 'docx':\r\n          return 'el-icon-document';\r\n        case 'xlsx':\r\n          return 'el-icon-s-data';\r\n        case 'pptx':\r\n          return 'el-icon-s-marketing';\r\n        default:\r\n          return 'el-icon-document';\r\n      }\r\n    },\r\n    /** Êñá‰ª∂È¢ÑËßà */\r\n    handlePreview(file) {\r\n      this.$message(`È¢ÑËßàÊñá‰ª∂Ôºö${file.fileName}`)\r\n\r\n      const fileSuffix = (file.extendName || '').toLowerCase()\r\n\r\n      // Êã¶Êà™ÂõæÁâá„ÄÅÈü≥È¢ë„ÄÅËßÜÈ¢ëÁ±ªÂûãÔºå‰∏çÂ§ÑÁêÜ\r\n      const IMAGE_TYPES = ['png', 'jpg', 'jpeg', 'gif', 'svg']\r\n      const VIDEO_TYPES = ['mp4']\r\n      const AUDIO_TYPES = ['mp3', 'flac']\r\n\r\n      if (\r\n        IMAGE_TYPES.includes(fileSuffix) ||\r\n        VIDEO_TYPES.includes(fileSuffix) ||\r\n        AUDIO_TYPES.includes(fileSuffix)\r\n      ) {\r\n        this.$message.warning('ËØ•Êñá‰ª∂Á±ªÂûã‰∏çÊîØÊåÅÈ¢ÑËßà')\r\n        return\r\n      }\r\n\r\n      // Office Êñá‰ª∂Âú®Á∫øÈ¢ÑËßà\r\n      if ([...officeFileType].includes(fileSuffix)) {\r\n        this.getFileOnlineViewPathByOffice(file)\r\n        return\r\n      }\r\n\r\n      // PDF Êñá‰ª∂Êñ∞Ê†áÁ≠æÈ°µÊâìÂºÄ\r\n      if (fileSuffix === 'pdf') {\r\n        window.open(this.getViewFilePath(file), '_blank')\r\n        return\r\n      }\r\n\r\n      // Áªü‰∏Ä yaml => yml\r\n      let codeSuffix = fileSuffix === 'yaml' ? 'yml' : fileSuffix\r\n\r\n      // Markdown ÊñáÊ°£\r\n      if (markdownFileType.includes(codeSuffix)) {\r\n        Vue.prototype.$openBox.markdownPreview({\r\n          fileInfo: file,\r\n          editable: false\r\n        })\r\n        return\r\n      }\r\n\r\n      // ÊñáÊú¨/‰ª£Á†ÅÁ±ªÊñá‰ª∂ Êàñ Êó†Êâ©Â±ïÂêçÊñá‰ª∂\r\n      if (\r\n        fileSuffixCodeModeMap.has(codeSuffix) ||\r\n        (file.isDir === 0 && !file.extendName)\r\n      ) {\r\n        Vue.prototype.$openBox.codePreview({ fileInfo: file, isEdit: false })\r\n        return\r\n      }\r\n\r\n      // ÂÖ∂‰ªñ‰∏çÊîØÊåÅÈ¢ÑËßà\r\n      this.$message.warning('ÊöÇ‰∏çÊîØÊåÅËØ•Á±ªÂûãÊñá‰ª∂È¢ÑËßà')\r\n    },\r\n    /**\r\n     * * @param file Êñá‰ª∂Âà†Èô§Êìç‰Ωú\r\n     */\r\n    handleDelete(file) {\r\n      this.$confirm(`Á°ÆÂÆöÂà†Èô§ ${file.fileName} ÂêóÔºü`, 'Âà†Èô§Á°ÆËÆ§')\r\n        .then(() => {\r\n          this.$emit('fileDelete', file);\r\n        })\r\n        .catch(() => { });\r\n    },\r\n    handleViewMore() {\r\n      this.$message('Âä†ËΩΩÊõ¥Â§öÊñá‰ª∂...');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n/* ================= ËµõÂçöÊúãÂÖã‰∏ªÈ¢òÂèòÈáè (ÁªßÊâøËá™Áà∂ÁªÑ‰ª∂ÔºåËøôÈáåÈáçÂ§ç‰ª•‰æøÁã¨Á´ãË∞ÉËØï) ================= */\r\n@bg-dark: #0f1219;\r\n@bg-panel: rgba(26, 31, 44, 0.7);\r\n@primary-color: #00f2ff; /* Cyber Cyan */\r\n@accent-color: #7d2ae8;  /* Neon Purple */\r\n@text-main: #e2e8f0; // ÊµÖËâ≤‰∏ªÊñáÊú¨\r\n@text-sub: #94a3b8;  // ËæÖÂä©ÊñáÊú¨\r\n@border-color: rgba(255, 255, 255, 0.08);\r\n\r\n/* ================= ÂÆπÂô® (Card) Ê†∑ÂºèÈáçÂÜô ===========