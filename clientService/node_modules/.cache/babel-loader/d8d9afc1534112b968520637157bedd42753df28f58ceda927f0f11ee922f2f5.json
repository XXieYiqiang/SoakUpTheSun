iv> -->\r\n                <div class=\"msg-content\" v-html=\"formatToHtml(msg.content)\"></div>\r\n                <div class=\"avatar\" v-if=\"msg.role === 'user'\">üßë</div>\r\n            </div>\r\n\r\n            <div class=\"ai-loading\" v-if=\"loading\">\r\n                <span style=\"margin-right: 10px;\">AI Ê≠£Âú®ËæìÂÖ•</span>\r\n                <div class=\"dot-flash\">\r\n                    <div class=\"dot\" v-for=\"i in 3\" :key=\"i\" :style=\"{ animationDelay: (i * 0.2) + 's' }\"></div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Â∫ïÈÉ®ËæìÂÖ•Ê†è -->\r\n        <div class=\"input-bar\" :class=\"{ 'input-bottom': chatMessages.length > 0 }\">\r\n            <div class=\"input-wrapper\">\r\n                <input v-model=\"userInput\" @keyup.enter=\"handleSend\" placeholder=\"ËØ∑ËæìÂÖ•ÈóÆÈ¢ò...\" class=\"input-box\" />\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            chatMessages: [\r\n                { id: Date.now(), role: \"ai\", content: \"ÂòøÔºÅÊàëÊòØ AI Â∞èÂä©ÊâãÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû\" },\r\n            ],\r\n            userInput: '',\r\n            socket: null,\r\n            loading: false,\r\n            currentAiMessageId: null, // üîß ÂΩìÂâçÊ≠£Âú®Êé•Êî∂ÁöÑ AI Ê∂àÊÅØ\r\n            typingQueue: [],       // ÊâìÂ≠óÊú∫ÁºìÂÜ≤ÈòüÂàó\r\n            typingTimer: null,     // ÂÆöÊó∂Âô®ÊéßÂà∂Âô®\r\n            isTyping: false,       // ÊòØÂê¶Ê≠£Âú®ÊâìÂ≠ó\r\n        };\r\n    },\r\n    mounted() {\r\n        this.initSocket();\r\n    },\r\n    beforeDestroy() {\r\n        if (this.typingTimer) {\r\n            clearTimeout(this.typingTimer);\r\n        }\r\n    },\r\n    methods: {\r\n        formatToHtml(content) {\r\n            marked.setOptions({\r\n                breaks: true, // ÊîØÊåÅÊç¢Ë°å\r\n                highlight: function (code, lang) {\r\n                    return hljs.highlightAuto(code).value;\r\n                }\r\n            });\r\n            return marked.parse(content);\r\n        },\r\n        generateId() {\r\n            return Date.now() + Math.random(); // ÁÆÄÂçïÂîØ‰∏Ä ID\r\n        },\r\n        initSocket() {\r\n            this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws');\r\n\r\n            this.socket.addEventListener('open', () => {\r\n                console.log('WebSocket Â∑≤ËøûÊé•');\r\n            });\r\n\r\n            this.socket.addEventListener('message', (event) => {\r\n                if (event.data === '[DONE]') {\r\n                    this.loading = false;\r\n                    this.currentAiMessageId = null; // ‚úÖ ÁªìÊùü AI Ê∂àÊÅØ\r\n                    return;\r\n                }\r\n                // this.appendCharByChar(event.data);\r\n                this.queueTextForTyping(event.data); // ËøõÂÖ•Áªü‰∏ÄÁºìÂÜ≤Êú∫Âà∂\r\n            });\r\n\r\n            this.socket.addEventListener('close', () => {\r\n                console.log('WebSocket Â∑≤ÂÖ≥Èó≠');\r\n            });\r\n\r\n            this.socket.addEventListener('error', (error) => {\r\n                console.error('WebSocket ÈîôËØØ:', error);\r\n            });\r\n        },\r\n\r\n        handleSend() {\r\n            const msg = this.userInput.trim();\r\n            if (!msg || this.socket.readyState !== WebSocket.OPEN) return;\r\n\r\n            this.chatMessages.push({\r\n                id: this.generateId(),\r\n                role: 'user',\r\n                content: msg\r\n            });\r\n\r\n            this.userInput = '';\r\n            this.loading = true;\r\n            this.scrollToBottom();\r\n\r\n            const context = this.chatMessages\r\n                .map((m) => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content)\r\n                .join('\\n');\r\n\r\n            this.socket.send(context);\r\n        },\r\n\r\n        queueTextForTyping(text) {\r\n            const chars = Array.from(text);\r\n            this.typingQueue.push(...chars); // Âä†ÂÖ•ÈòüÂàó\r\n            if (!this.isTyping) {\r\n                this.startTyping(); // Â¶ÇÊûúÊ≤°Âú®ÊâìÂ≠óÔºåÂ∞±ÂêØÂä®ÊâìÂ≠óÊú∫\r\n            }\r\n        },\r\n\r\n        startTyping() {\r\n            this.isTyping = true;\r\n\r\n            let last = this.chatMessages.find(msg => msg.id === this.currentAiMessageId);\r\n            if (!last) {\r\n                last = { id: this.generateId(), role: 'ai', content: '' };\r\n                this.chatMessages.push(last);\r\n                this.currentAiMessageId = last.id;\r\n            }\r\n\r\n            const typeNext = () => {\r\n                if (this.typingQueue.length === 0) {\r\n                    this.isTyping = false;\r\n                    return;\r\n                }\r\n\r\n                const nextChar = this.typingQueue.shift();\r\n                last.content += nextChar;\r\n                this.$forceUpdate();\r\n                this.scrollToBottom();\r\n\r\n                this.typingTimer = setTimeout(typeNext, 20);\r\n            };\r\n\r\n            typeNext();\r\n        },\r\n\r\n        // appendCharByChar(text) {\r\n\r\n        //     let last = this.chatMessages.find(msg => msg.id === this.currentAiMessageId);\r\n\r\n        //     if (!last) {\r\n        //         last = { id: this.generateId(), role: 'ai', content: '' };\r\n        //         this.chatMessages.push(last);\r\n        //         this.currentAiMessageId = last.id;\r\n        //     }\r\n\r\n        //     const chars = Array.from(text);\r\n        //     let index = 0;\r\n\r\n        //     const typeNext = () => {\r\n        //         if (index < chars.length) {\r\n        //             last.content += chars[index++];\r\n        //             this.$forceUpdate(); // Âº∫Âà∂Âà∑Êñ∞ËßÜÂõæ\r\n        //             this.scrollToBottom();\r\n        //             setTimeout(typeNext, 20); // ÊâìÂ≠óÈÄüÂ∫¶ÂèØË∞É\r\n        //         }\r\n        //     };\r\