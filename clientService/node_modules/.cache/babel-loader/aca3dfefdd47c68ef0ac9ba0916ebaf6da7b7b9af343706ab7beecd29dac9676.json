grid\">\r\n      <div v-for=\"stat in stats\" :key=\"stat.title\" class=\"stat-card\" :style=\"{ '--bg-color': stat.bgColor }\">\r\n        <span class=\"stat-icon\">{{ stat.icon }}</span>\r\n        <div class=\"stat-info\">\r\n          <p class=\"stat-title\">{{ stat.title }}</p>\r\n          <h2 class=\"stat-value\">{{ stat.value }}</h2>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"content-split-grid\">\r\n\r\n      <div class=\"model-section\">\r\n        <h3>ü§ñ Âπ≥Âè∞Êé•ÂÖ•Ê†∏ÂøÉÊ®°Âûã (Á¨¨ {{ modelCurrentPage }} È°µ / ÂÖ± {{ modelTotalPages }} È°µ)</h3>\r\n        <div class=\"model-cards-container\">\r\n          <div v-for=\"model in paginatedModels\" :key=\"model.name\" class=\"model-card\">\r\n            <div class=\"model-header\">\r\n              <h4 class=\"model-name\">{{ model.icon }} {{ model.name }}</h4>\r\n              <span :class=\"['model-type-tag', `tag-${model.type}`]\">{{ model.typeText }}</span>\r\n            </div>\r\n            <p class=\"model-advantage\">{{ model.advantage }}</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"pagination-controls model-pagination\">\r\n          <button \r\n            @click=\"prevModelPage\" \r\n            :disabled=\"modelCurrentPage === 1\"\r\n          >\r\n            &lt; ‰∏ä‰∏ÄÈ°µ\r\n          </button>\r\n          <button \r\n            @click=\"nextModelPage\" \r\n            :disabled=\"modelCurrentPage === modelTotalPages\"\r\n          >\r\n            ‰∏ã‰∏ÄÈ°µ &gt;\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"log-section\">\r\n        <h3>üì¢ ÊÇ®ÁöÑÊúÄÊñ∞Êìç‰ΩúÊó•Âøó (Á¨¨ {{ logCurrentPage }} È°µ / ÂÖ± {{ logTotalPages }} È°µ)</h3>\r\n        <ul class=\"activity-logs\">\r\n          <li v-for=\"log in activityLogs\" :key=\"log.time + log.message\" class=\"log-item\">\r\n            <div class=\"log-indicator\"></div>\r\n            <span class=\"log-time\">{{ log.time }}</span>\r\n            <span class=\"log-message\">{{ log.message }}</span>\r\n          </li>\r\n        </ul>\r\n        \r\n        <div class=\"pagination-controls log-pagination\">\r\n          <button \r\n            @click=\"prevLogPage\" \r\n            :disabled=\"logCurrentPage === 1 || logLoading\"\r\n          >\r\n            &lt; ‰∏ä‰∏ÄÈ°µ\r\n          </button>\r\n          <button \r\n            @click=\"nextLogPage\" \r\n            :disabled=\"logCurrentPage === logTotalPages || logLoading\"\r\n          >\r\n            <span v-if=\"logLoading\" class=\"loading-spinner\"></span>\r\n            {{ logLoading ? 'Âä†ËΩΩ‰∏≠...' : '‰∏ã‰∏ÄÈ°µ >' }}\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { setTimeout } from 'timers'; // Node.js ÁéØÂ¢É‰∏ãÁöÑÂØºÂÖ•ÔºåWebÁéØÂ¢ÉÂèØÁúÅÁï•\r\n\r\nexport default {\r\n  name: 'UserDashboard',\r\n\r\n  // 3. ËÆ°ÁÆóÂ±ûÊÄß (Computed Properties)\r\n  // Áî®‰∫éËÆ°ÁÆóÂΩìÂâçÈ°µÂ∫îÂ±ïÁ§∫ÁöÑÊ®°ÂûãÂíåÊó•ÂøóÊï∞ÊçÆ\r\n  computed: {\r\n    modelTotalPages() {\r\n      return Math.ceil(this.modelTotalList.length / this.pageSize);\r\n    },\r\n    // ÂΩìÂâçÈ°µÁöÑÊ®°ÂûãÂàóË°®\r\n    paginatedModels() {\r\n      const start = (this.modelCurrentPage - 1) * this.pageSize;\r\n      const end = start + this.pageSize;\r\n      return this.modelTotalList.slice(start, end);\r\n    },\r\n    logTotalPages() {\r\n      return Math.ceil(this.logTotalList.length / this.pageSize);\r\n    }\r\n    // activityLogs Êï∞ÁªÑÂú® methods ‰∏≠Âä®ÊÄÅÊõ¥Êñ∞ÔºåÊïÖ‰∏çÂÜçÈúÄË¶Å log ÁöÑ computed Â±ûÊÄß\r\n  },\r\n\r\n  data() {\r\n    // Ê®°ÊãüÂÆåÊï¥‰∏îË∂ÖËøá 20 Êù°ÁöÑÊï∞ÊçÆ\r\n    const fullModelList = [\r\n      { name: 'GPT-4o', icon: 'üöÄ', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'Â§öÊ®°ÊÄÅ„ÄÅÈ´òÈÄü„ÄÅÈ°∂Á∫ßÊé®ÁêÜËÉΩÂäõ„ÄÇ' },\r\n      { name: 'Claude 3 Opus', icon: 'üåü', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'ÈïøÊñáÊú¨‰∏ä‰∏ãÊñá„ÄÅÊÉÖÊÑüÁêÜËß£„ÄÅÂÆâÂÖ®ÂêàËßÑ„ÄÇ' },\r\n      { name: 'Gemini 1.5 Pro', icon: 'üåå', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'Êµ∑Èáè‰∏ä‰∏ãÊñáÁ™óÂè£„ÄÅÈ´òÊïàË∑®Ê®°ÊÄÅÊé®ÁêÜ„ÄÇ' },\r\n      { name: 'LLaMA-3 70B', icon: 'üß†', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'ÂºÄÊ∫êÁïåÊ†áÊùÜ„ÄÅÈ´òÊÄßËÉΩ„ÄÅÂèØÁßÅÊúâÂåñÈÉ®ÁΩ≤„ÄÇ' },\r\n      { name: 'Mistral Large', icon: 'üí°', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'Ê¨ßÊ¥≤AIÂÖàÈîã„ÄÅÂ§öËØ≠ÁßçÊîØÊåÅ„ÄÅÊàêÊú¨‰ºòÂåñ„ÄÇ' },\r\n      { name: 'Cohere Command', icon: 'üåê', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'RAG‰ºòÂåñ„ÄÅÂïÜ‰∏öÂ∫îÁî®ÂèãÂ•Ω„ÄÅÂ∑•ÂÖ∑‰ΩøÁî®ËÉΩÂäõÂº∫„ÄÇ' },\r\n      { name: 'GPT-3.5 Turbo', icon: '‚ö°', type: 'llm', typeText: 'Â§ßËØ≠Ë®ÄÊ®°Âûã', advantage: 'ÈÄöÁî®ËØ≠Ë®ÄÁêÜËß£Âíå‰ª£Á†ÅÁîüÊàêÔºåÂìçÂ∫îÈÄüÂ∫¶Âø´ÔºåÊàêÊú¨ÊïàÁõäÈ´ò„ÄÇ' },\r\n      { name: 'LLaMA-3 8B', icon: 'üí°', type: 'embedding', typeText: 'Áü•ËØÜÂ∫ìÂêëÈáè', advantage: '‰∏ì‰∏∫Êú¨Âú∞Áü•ËØÜÂ∫ìÊ£ÄÁ¥¢‰ºòÂåñÔºå‰øùÈöúÊï∞ÊçÆÈöêÁßÅÂíåÂÆâÂÖ®ÊÄß„ÄÇ' },\r\n      { name: 'DALL¬∑E 3', icon: 'üé®', type: 'image', typeText: 'ÂõæÂÉèÁîüÊàê', advantage: 'È´òË¥®ÈáèÂõæÂÉèÁîüÊàêÂíåÁºñËæëÔºåÈÄÇÂêàËÆæËÆ°ÂíåÂàõÊÑèÂ∑•‰ΩúÊµÅ„ÄÇ' },\r\n      { name: 'Stable Diffusion XL', icon: 'üñºÔ∏è', type: 'image', typeText: 'ÂõæÂÉèÁîüÊàê', advantage: 'ÂºÄÊ∫êÂõæÂÉèÊ®°Âûã„ÄÅÈ´òÂ∫¶ÂÆöÂà∂Âåñ„ÄÅÁ§æÂå∫ÁîüÊÄÅ‰∏∞ÂØå„ÄÇ' },\r\n      { name: 'Midjourney V6', icon: '‚ú®', type: 'image', typeText: 'ÂõæÂÉèÁîüÊàê', advantage: 'Ëâ∫ÊúØÈ£éÊ†ºË°®Áé∞ÂäõÂº∫„ÄÅËâ≤ÂΩ©ÂíåÊûÑÂõæ‰ºòÁßÄ„ÄÇ' },\r\n      { name: 'CLIP', icon: 'üëÅÔ∏è', type: 'vision', typeText: 'ËßÜËßâÊ®°Âûã', advantage: 'ÂõæÂÉè-ÊñáÊú¨ÂØπÈΩêÔºåÂ∏∏Áî®‰∫éÈõ∂Ê†∑Êú¨ËØÜÂà´ÂíåÊêúÁ¥¢„ÄÇ' },\r\n      { name: 'Whisper V3', icon: 'üéß', type: 'audio', typeText: 'Èü≥È¢ëÂ§ÑÁêÜ', advantage: 'Â§öËØ≠Áßç„ÄÅÈ´òÁ≤æÂ∫¶ËØ≠Èü≥ËØÜÂà´ÂíåÁøªËØë„ÄÇ' },\r\n      { name: 'Code LLaMA', icon: 'üíª', type: 'code', typeText: '‰ª£Á†ÅÊ®°Âûã', advantage: '‰∏ìÊ≥®‰∫é‰ª£Á†ÅÁîüÊàêÂíåË°•ÂÖ®„ÄÇ' },\r\n      { name: 'T5', icon: '‚úçÔ∏è', type: 'seq2seq', typeText: