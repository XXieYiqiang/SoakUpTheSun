{"ast":null,"code":"\"use strict\";\n\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\nvar _vuex = require(\"vuex\");\nvar _default = exports.default = {\n  name: 'FileTable',\n  props: {\n    // 文件类型\n    fileType: {\n      required: true,\n      type: Number\n    },\n    // 文件路径\n    filePath: {\n      required: true,\n      type: String\n    },\n    // 文件列表\n    fileList: {\n      required: true,\n      type: Array\n    },\n    // 文件加载状态\n    loading: {\n      required: true,\n      type: Boolean\n    }\n  },\n  data() {\n    return {\n      officeFileType: ['ppt', 'pptx', 'doc', 'docx', 'xls', 'xlsx'],\n      sortedFileList: [] //  排序后的表格数据\n    };\n  },\n  computed: {\n    //  selectedColumnList: 判断当前用户设置的左侧栏是否折叠\n    ...(0, _vuex.mapGetters)(['selectedColumnList']),\n    // 路由名称\n    routeName() {\n      return this.$route.name;\n    },\n    // 屏幕宽度\n    screenWidth() {\n      return this.$store.state.common.screenWidth;\n    }\n  },\n  watch: {\n    /**\r\n     * 文件路径变化时清空表格已选行\r\n     */\n    filePath() {\n      this.clearSelectedTable();\n      this.$refs.multipleTable.clearSort();\n    },\n    /**\r\n     * 文件类型变化时清空表格已选行\r\n     */\n    fileType() {\n      this.clearSelectedTable();\n      this.$refs.multipleTable.clearSort();\n    },\n    /**\r\n     * 文件列表变化时清空表格已选行\r\n     */\n    fileList() {\n      this.clearSelectedTable();\n      this.$refs.multipleTable.clearSort();\n      this.sortedFileList = this.fileList;\n    }\n  },\n  methods: {\n    /**\r\n     * 当表格的排序条件发生变化的时候会触发该事件\r\n     */\n    handleSortChange() {\n      this.sortedFileList = this.$refs.multipleTable.tableData;\n    },\n    /**\r\n     * 表格某一行右键事件\r\n     * @description 打开右键菜单\r\n     * @param {object} row 当前行数据\r\n     * @param {object} column 当前列数据\r\n     * @param {object} event 当前右键元素\r\n     */\n    handleContextMenu(row, column, event) {\n      // 阻止右键事件冒泡\n      event.cancelBubble = true;\n      // xs 以上的屏幕\n      if (this.screenWidth > 768) {\n        event.preventDefault();\n        this.$refs.multipleTable.setCurrentRow(row); //  选中当前行\n        this.$openBox.contextMenu({\n          selectedFile: row,\n          domEvent: event\n        }).then(res => {\n          this.$refs.multipleTable.setCurrentRow(); //  取消当前选中行\n          if (res === 'confirm') {\n            this.$emit('getTableDataByType'); //  刷新文件列表\n            // this.$store.dispatch('showStorage') //  刷新存储容量\n          }\n        });\n      }\n    },\n    /**\r\n     * 清空表格已选行\r\n     * @description 用于父组件调用 | 本组件调用，请勿删除\r\n     */\n    clearSelectedTable() {\n      this.$refs.multipleTable.clearSelection();\n    },\n    /**\r\n     * 表格选择项发生变化时的回调函数\r\n     * @param {[]} selection 勾选的行数据\r\n     */\n    handleSelectRow(selection) {\n      this.$store.commit('changeSelectedFiles', selection);\n      this.$store.commit('changeIsBatchOperation', selection.length !== 0);\n    },\n    /**\r\n     * 更多图标点击事件\r\n     * @description 打开右键菜单\r\n     * @param {object} row 当前行数据\r\n     * @param {object} event 当前右键元素\r\n     */\n    handleClickMore(row, event) {\n      this.$refs.multipleTable.setCurrentRow(row); //  选中当前行\n      this.$openBox.contextMenu({\n        selectedFile: row,\n        domEvent: event\n      }).then(res => {\n        this.$refs.multipleTable.setCurrentRow(); //  取消当前选中行\n        if (res === 'confirm') {\n          //外部方法\n          this.$emit('getTableDataByType'); //  刷新文件列表\n          // 外层依赖\n          // this.$store.dispatch('showStorage') //  刷新存储容量\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["_vuex","require","_default","exports","default","name","props","fileType","required","type","Number","filePath","String","fileList","Array","loading","Boolean","data","officeFileType","sortedFileList","computed","mapGetters","routeName","$route","screenWidth","$store","state","common","watch","clearSelectedTable","$refs","multipleTable","clearSort","methods","handleSortChange","tableData","handleContextMenu","row","column","event","cancelBubble","preventDefault","setCurrentRow","$openBox","contextMenu","selectedFile","domEvent","then","res","$emit","clearSelection","handleSelectRow","selection","commit","length","handleClickMore"],"sources":["src/components/common/FileTable.vue"],"sourcesContent":["<template>\r\n    <div class=\"file-table-wrapper tech-table-wrapper\">\r\n        <el-table class=\"file-table tech-table\" :class=\"['file-type-' + fileType, routeName === 'Share' ? 'share' : '']\"\r\n            ref=\"multipleTable\" fit v-loading=\"loading\" element-loading-text=\"文件加载中……\" tooltip-effect=\"dark\"\r\n            :data=\"fileList\" :highlight-current-row=\"true\" @selection-change=\"handleSelectRow\"\r\n            @sort-change=\"handleSortChange\" @row-contextmenu=\"handleContextMenu\">\r\n            <el-table-column type=\"selection\" key=\"selection\" width=\"56\" align=\"center\"\r\n                v-if=\"fileType !== 8\"></el-table-column>\r\n            <el-table-column label prop=\"isDir\" key=\"isDir\" :width=\"screenWidth <= 768 ? 40 : 56\" align=\"center\"\r\n                class-name=\"file-icon-column tech-icon-column\">\r\n                <template slot-scope=\"scope\">\r\n                    <video style=\"width: 30px; max-height: 30px; cursor: pointer\" v-if=\"$file.isVideoFile(scope.row)\"\r\n                        :src=\"$file.setFileImg(scope.row)\"></video>\r\n                    <img :src=\"$file.setFileImg(scope.row)\" :title=\"`${scope.row.isDir ? '' : '点击预览'}`\"\r\n                        style=\"width: 30px; max-height: 30px; cursor: pointer\" @click=\"\r\n                            $file.handleFileNameClick(scope.row, scope.$index, sortedFileList)\r\n                            \" v-else />\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"fileName\" key=\"fileName\" :sort-by=\"['isDir', 'fileName']\" sortable\r\n                show-overflow-tooltip>\r\n                <template slot=\"header\">\r\n                    <span class=\"tech-header-text\">文件名</span>\r\n                </template>\r\n                <template slot-scope=\"scope\">\r\n                    <span @click=\"\r\n                        $file.handleFileNameClick(scope.row, scope.$index, sortedFileList)\r\n                        \">\r\n                        <span class=\"file-name tech-filename-text\" style=\"cursor: pointer\" :title=\"`${scope.row.isDir ? '' : '点击预览'}`\"\r\n                            v-html=\"$file.getFileNameComplete(scope.row, true)\"></span>\r\n                        <div class=\"file-info\" v-if=\"screenWidth <= 768\">\r\n                            {{ scope.row.uploadTime }}\r\n                            <span class=\"file-size tech-size-text\">\r\n                                {{\r\n                                    scope.row.isDir === 0\r\n                                        ? $file.calculateFileSize(scope.row.fileSize)\r\n                                        : ''\r\n                                }}\r\n                            </span>\r\n                        </div>\r\n                    </span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"fileType === 6 ? '原路径' : '路径'\" prop=\"filePath\" key=\"filePath\" show-overflow-tooltip\r\n                v-if=\"\r\n                    ![0, 8].includes(Number($route.query.fileType)) &&\r\n                    routeName !== 'Share' &&\r\n                    screenWidth > 768\r\n                \">\r\n                <template slot-scope=\"scope\">\r\n                    <span style=\"cursor: pointer\" title=\"点击跳转\" @click=\"\r\n                        $router.push({\r\n                            query: { filePath: scope.row.filePath, fileType: 0 }\r\n                        })\r\n                        \">{{ scope.row.filePath }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"类型\" width=\"80\" prop=\"extendName\" key=\"extendName\" :sort-by=\"['isDir', 'extendName']\"\r\n                sortable show-overflow-tooltip v-if=\"selectedColumnList.includes('extendName') && screenWidth > 768\">\r\n                <template slot-scope=\"scope\">\r\n                    <span class=\"tech-text-sub\">{{ $file.getFileType(scope.row) }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"大小\" width=\"100\" prop=\"fileSize\" key=\"fileSize\" :sort-by=\"['isDir', 'fileSize']\"\r\n                sortable align=\"right\" v-if=\"selectedColumnList.includes('fileSize') && screenWidth > 768\">\r\n                <template slot-scope=\"scope\">\r\n                    <span class=\"tech-text-sub\">{{\r\n                        scope.row.isDir === 0\r\n                            ? $file.calculateFileSize(scope.row.fileSize)\r\n                            : ''\r\n                    }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"修改日期\" prop=\"uploadTime\" key=\"uploadTime\" width=\"160\"\r\n                :sort-by=\"['isDir', 'uploadTime']\" sortable align=\"center\" v-if=\"\r\n                    selectedColumnList.includes('uploadTime') &&\r\n                    ![7, 8].includes(fileType) &&\r\n                    !['Share'].includes(this.routeName) &&\r\n                    screenWidth > 768\r\n                \"></el-table-column>\r\n            <el-table-column label=\"删除日期\" prop=\"deleteTime\" key=\"deleteTime\" width=\"160\"\r\n                :sort-by=\"['isDir', 'deleteTime']\" sortable align=\"center\" v-if=\"\r\n                    fileType === 6 &&\r\n                    selectedColumnList.includes('deleteTime') &&\r\n                    screenWidth > 768\r\n                \"></el-table-column>\r\n            <el-table-column label=\"分享类型\" prop=\"shareType\" key=\"shareType\" width=\"100\" align=\"center\"\r\n                v-if=\"fileType === 8 && screenWidth > 768\">\r\n                <template slot-scope=\"scope\">\r\n                    <span :class=\"scope.row.shareType === 1 ? 'tech-private-share' : 'tech-public-share'\">{{ scope.row.shareType === 1 ? '私密' : '公共' }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"分享时间\" prop=\"shareTime\" key=\"shareTime\" width=\"160\" :sort-by=\"['isDir', 'shareTime']\"\r\n                show-overflow-tooltip sortable align=\"center\"\r\n                v-if=\"fileType === 8 && screenWidth > 768\"></el-table-column>\r\n            <el-table-column label=\"过期时间\" prop=\"endTime\" key=\"endTime\" width=\"190\" :sort-by=\"['isDir', 'endTime']\"\r\n                show-overflow-tooltip sortable align=\"center\" v-if=\"fileType === 8 && screenWidth > 768\">\r\n                <template slot-scope=\"scope\">\r\n                    <div :class=\"{ 'tech-expired-warning': $file.getFileShareStatus(scope.row.endTime) }\">\r\n                        <i class=\"el-icon-warning tech-icon-warning\" v-if=\"$file.getFileShareStatus(scope.row.endTime)\"></i>\r\n                        <i class=\"el-icon-time tech-icon-time\" v-else></i>\r\n                        {{ scope.row.endTime }}\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"\" key=\"operation\" width=\"48\" v-if=\"screenWidth <= 768\">\r\n                <template slot-scope=\"scope\">\r\n                    <i class=\"file-operate el-icon-more tech-icon-more\" :class=\"`operate-more-${scope.$index}`\"\r\n                        @click=\"handleClickMore(scope.row, $event)\"></i>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\n\r\nexport default {\r\n    name: 'FileTable',\r\n    props: {\r\n        // 文件类型\r\n        fileType: {\r\n            required: true,\r\n            type: Number\r\n        },\r\n        // 文件路径\r\n        filePath: {\r\n            required: true,\r\n            type: String\r\n        },\r\n        // 文件列表\r\n        fileList: {\r\n            required: true,\r\n            type: Array\r\n        },\r\n        // 文件加载状态\r\n        loading: {\r\n            required: true,\r\n            type: Boolean\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            officeFileType: ['ppt', 'pptx', 'doc', 'docx', 'xls', 'xlsx'],\r\n            sortedFileList: [] //  排序后的表格数据\r\n        }\r\n    },\r\n    computed: {\r\n        //  selectedColumnList: 判断当前用户设置的左侧栏是否折叠\r\n        ...mapGetters(['selectedColumnList']),\r\n        // 路由名称\r\n        routeName() {\r\n            return this.$route.name\r\n        },\r\n        // 屏幕宽度\r\n        screenWidth() {\r\n            return this.$store.state.common.screenWidth\r\n        }\r\n    },\r\n    watch: {\r\n        /**\r\n         * 文件路径变化时清空表格已选行\r\n         */\r\n        filePath() {\r\n            this.clearSelectedTable()\r\n            this.$refs.multipleTable.clearSort()\r\n        },\r\n        /**\r\n         * 文件类型变化时清空表格已选行\r\n         */\r\n        fileType() {\r\n            this.clearSelectedTable()\r\n            this.$refs.multipleTable.clearSort()\r\n        },\r\n        /**\r\n         * 文件列表变化时清空表格已选行\r\n         */\r\n        fileList() {\r\n            this.clearSelectedTable()\r\n            this.$refs.multipleTable.clearSort()\r\n            this.sortedFileList = this.fileList\r\n        }\r\n    },\r\n    methods: {\r\n        /**\r\n         * 当表格的排序条件发生变化的时候会触发该事件\r\n         */\r\n        handleSortChange() {\r\n            this.sortedFileList = this.$refs.multipleTable.tableData\r\n        },\r\n        /**\r\n         * 表格某一行右键事件\r\n         * @description 打开右键菜单\r\n         * @param {object} row 当前行数据\r\n         * @param {object} column 当前列数据\r\n         * @param {object} event 当前右键元素\r\n         */\r\n        handleContextMenu(row, column, event) {\r\n            // 阻止右键事件冒泡\r\n            event.cancelBubble = true\r\n            // xs 以上的屏幕\r\n            if (this.screenWidth > 768) {\r\n                event.preventDefault()\r\n                this.$refs.multipleTable.setCurrentRow(row) //  选中当前行\r\n                this.$openBox\r\n                    .contextMenu({\r\n                        selectedFile: row,\r\n                        domEvent: event\r\n                    })\r\n                    .then((res) => {\r\n                        this.$refs.multipleTable.setCurrentRow() //  取消当前选中行\r\n                        if (res === 'confirm') {\r\n                            this.$emit('getTableDataByType') //  刷新文件列表\r\n                            // this.$store.dispatch('showStorage') //  刷新存储容量\r\n                        }\r\n                    })\r\n            }\r\n        },\r\n        /**\r\n         * 清空表格已选行\r\n         * @description 用于父组件调用 | 本组件调用，请勿删除\r\n         */\r\n        clearSelectedTable() {\r\n            this.$refs.multipleTable.clearSelection()\r\n        },\r\n        /**\r\n         * 表格选择项发生变化时的回调函数\r\n         * @param {[]} selection 勾选的行数据\r\n         */\r\n        handleSelectRow(selection) {\r\n            this.$store.commit('changeSelectedFiles', selection)\r\n            this.$store.commit('changeIsBatchOperation', selection.length !== 0)\r\n        },\r\n        /**\r\n         * 更多图标点击事件\r\n         * @description 打开右键菜单\r\n         * @param {object} row 当前行数据\r\n         * @param {object} event 当前右键元素\r\n         */\r\n        handleClickMore(row, event) {\r\n            this.$refs.multipleTable.setCurrentRow(row) //  选中当前行\r\n            this.$openBox\r\n                .contextMenu({\r\n                    selectedFile: row,\r\n                    domEvent: event\r\n                })\r\n                .then((res) => {\r\n                    this.$refs.multipleTable.setCurrentRow() //  取消当前选中行\r\n                    if (res === 'confirm') {\r\n                        //外部方法\r\n                        this.$emit('getTableDataByType') //  刷新文件列表\r\n                        // 外层依赖\r\n                        // this.$store.dispatch('showStorage') //  刷新存储容量\r\n                    }\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n/* ================= 赛博朋克主题变量 ================= */\r\n@bg-dark: #0f1219;\r\n@bg-panel: rgba(26, 31, 44, 0.7);\r\n@primary-color: #00f2ff; /* Cyber Cyan - 霓虹青色 */\r\n@accent-color: #7d2ae8;  /* Neon Purple */\r\n@warning-color: #ff073a; /* Neon Red for warnings */\r\n@text-main: #e2e8f0; // 浅色主文本\r\n@text-sub: #94a3b8;  // 辅助文本\r\n@border-color-dark: rgba(255, 255, 255, 0.08); // 用于主要边框\r\n@header-bg: rgba(0, 242, 255, 0.05); // 表头背景\r\n@hover-bg: rgba(0, 242, 255, 0.1); // 行悬停背景\r\n@select-bg: rgba(125, 42, 232, 0.1); // 行选中背景\r\n\r\n.file-table-wrapper.tech-table-wrapper {\r\n    background-color: @bg-dark; // 使用深色背景\r\n    border-radius: 8px;\r\n    padding: 16px;\r\n    border: 1px solid @border-color-dark; // 浅色科技边框\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.5); // 增加阴影深度\r\n\r\n    .el-table.tech-table {\r\n        width: 100%;\r\n        color: @text-main; // 表格文本主色\r\n        background-color: transparent; // 背景透明\r\n        \r\n        // 移除或修改默认的表格边框和线条\r\n        &::before {\r\n            background-color: transparent !important;\r\n        }\r\n\r\n        /deep/ .el-table__header-wrapper {\r\n            border-bottom: 2px solid @primary-color; // 霓虹青色底部线\r\n            box-shadow: 0 1px 3px rgba(0, 242, 255, 0.3); // 霓虹光晕\r\n\r\n            th {\r\n                background-color: @header-bg !important; // 表头使用半透明深色背景\r\n                color: @primary-color !important; // 表头文本使用霓虹青色\r\n                border-bottom: none !important; // 移除默认的底部边框\r\n                \r\n                .cell {\r\n                    font-weight: bold;\r\n                    text-shadow: 0 0 3px rgba(0, 242, 255, 0.5); // 文本发光\r\n                }\r\n\r\n                // 排序图标\r\n                .sort-caret.ascending {\r\n                    border-bottom-color: @primary-color !important;\r\n                }\r\n                .sort-caret.descending {\r\n                    border-top-color: @primary-color !important;\r\n                }\r\n            }\r\n        }\r\n\r\n        /deep/ .el-table__body-wrapper {\r\n            td {\r\n                background-color: transparent;\r\n                border-bottom: 1px dashed @border-color-dark; // 使用虚线分隔行\r\n                color: @text-main;\r\n\r\n                // 文件名文字样式\r\n                .tech-filename-text {\r\n                    color: @text-main;\r\n\r\n                    &:hover {\r\n                        color: @primary-color;\r\n                        text-shadow: 0 0 5px rgba(0, 242, 255, 0.5);\r\n                    }\r\n                }\r\n\r\n                // 移动端文件信息\r\n                .file-info {\r\n                    color: @text-sub;\r\n                    font-size: 12px;\r\n                    \r\n                    .tech-size-text {\r\n                        margin-left: 8px;\r\n                        color: @accent-color; // 大小使用紫色\r\n                    }\r\n                }\r\n                \r\n                // 其他辅助文本\r\n                .tech-text-sub {\r\n                    color: @text-sub;\r\n                }\r\n\r\n                // 分享类型\r\n                .tech-private-share {\r\n                    color: @accent-color; // 私密：紫色\r\n                }\r\n                .tech-public-share {\r\n                    color: @primary-color; // 公共：青色\r\n                }\r\n\r\n                // 过期时间警告\r\n                .tech-expired-warning {\r\n                    color: @warning-color; // 警告：红色\r\n                    .tech-icon-warning {\r\n                        color: @warning-color;\r\n    