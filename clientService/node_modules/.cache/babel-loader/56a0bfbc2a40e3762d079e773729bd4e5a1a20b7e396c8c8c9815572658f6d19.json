   this.initSocket()\r\n  },\r\n  beforeDestroy() {\r\n    if (this.typingTimer) {\r\n      clearTimeout(this.typingTimer)\r\n    }\r\n    if (this.reconnectTimer) {\r\n      clearTimeout(this.reconnectTimer)\r\n    }\r\n    if (this.socket) {\r\n      this.socket.close()\r\n    }\r\n  },\r\n  methods: {\r\n    /** ÈôÑ‰ª∂È¢ÑËßà ÈÄâ‰∏≠‰∫ã‰ª∂*/\r\n    handleFileSelect(e) {\r\n\r\n      const file = e.target.files[0]\r\n      if (!file) return\r\n      this.selectedFile = file\r\n\r\n    },\r\n    /**\r\n     * Ê∂àÊÅØId ÁîüÊàê\r\n     */\r\n    generateId() {\r\n      return Date.now() + Math.random()\r\n    },\r\n    /**\r\n     * ÂàùÂßãÂåñ Web Socket ÊúçÂä°\r\n     */\r\n    initSocket() {\r\n\r\n      if (this.socket) {\r\n        this.socket.close()\r\n        this.socket = null\r\n      }\r\n\r\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws')\r\n\r\n      this.socketStatus = 'connecting'\r\n\r\n      this.socket.addEventListener('open', () => {\r\n        console.log('WebSocket Â∑≤ËøûÊé•')\r\n        this.socketStatus = 'open'\r\n        this.reconnectAttempts = 0  // ÊàêÂäüËøûÊé•ÂêéÈáçÁΩÆËÆ°Êï∞\r\n        if (this.reconnectTimer) {\r\n          clearTimeout(this.reconnectTimer)\r\n          this.reconnectTimer = null\r\n        }\r\n      })\r\n\r\n      this.socket.addEventListener('message', (event) => {\r\n        if (event.data === '[DONE]') {\r\n          this.loading = false\r\n          this.currentAiMessageId = null\r\n          return\r\n        }\r\n        this.queueTextForTyping(event.data)\r\n      })\r\n\r\n      this.socket.addEventListener('close', () => {\r\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠')\r\n        this.socketStatus = 'closed'\r\n        this.tryReconnect()\r\n      })\r\n\r\n      this.socket.addEventListener('error', (error) => {\r\n        console.error('WebSocket ÈîôËØØ:', error)\r\n        this.socketStatus = 'error'\r\n        this.tryReconnect()\r\n      })\r\n    },\r\n    /** WebSocket ÈáçËøû ÈÄªËæë */\r\n  