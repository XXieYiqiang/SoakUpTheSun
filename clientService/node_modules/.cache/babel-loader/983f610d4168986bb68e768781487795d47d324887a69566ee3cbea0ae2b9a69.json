{"ast":null,"code":"\"use strict\";\n\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\nrequire(\"core-js/modules/es.array.push.js\");\nrequire(\"core-js/modules/es.iterator.constructor.js\");\nrequire(\"core-js/modules/es.iterator.filter.js\");\nrequire(\"core-js/modules/es.iterator.find.js\");\nrequire(\"core-js/modules/es.iterator.reduce.js\");\nvar _default = exports.default = {\n  name: 'ChatView',\n  data() {\n    return {\n      sidebarCollapsed: false,\n      isMobile: false,\n      search: '',\n      activeMenu: 'message',\n      account: {\n        user_id: 'myid',\n        username: '我的账户',\n        avatar_url: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/1cec2ca1d9a7cf4e5e06c75370e677e7.png'\n      },\n      contacts: [{\n        contact_id: '1',\n        contact_name: '张三',\n        contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/e2f88577a8fde6d7ed70dd51df8d35aa.png',\n        contact_status: 'online',\n        last_contact_time: '14:32',\n        lastMessage: '好的，我明白了，明天见！',\n        lastMessageType: 'text',\n        unread_count: 0\n      }, {\n        contact_id: '2',\n        contact_name: '李四',\n        contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/dcdbda4e30edf6c9f34341a51ca4376c.png',\n        contact_status: 'online',\n        last_contact_time: '昨天',\n        lastMessage: '项目方案已经发送到您邮箱，请查收',\n        lastMessageType: 'text',\n        unread_count: 3\n      }, {\n        contact_id: '3',\n        contact_name: '王经理',\n        contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/8666b7c384360229440b4d45b6ed8cbc.png',\n        contact_status: 'offline',\n        last_contact_time: '周一',\n        lastMessage: '季度报告.pdf',\n        lastMessageType: 'file',\n        unread_count: 0\n      }, {\n        contact_id: '4',\n        contact_name: '技术支持',\n        contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/dfdd661625852bbf34fded8ae159a1cd.png',\n        contact_status: 'online',\n        last_contact_time: '上周',\n        lastMessage: '问题已经修复，请测试确认',\n        lastMessageType: 'text',\n        unread_count: 0\n      }],\n      groups: [{\n        group_id: '5',\n        group_name: '产品开发组',\n        group_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/15bd703716eb575eb049d376ef18625e.png',\n        last_message_time: '10:25',\n        last_message_author: '赵工程师',\n        last_message: '后端API已经准备好了'\n      }, {\n        group_id: '6',\n        group_name: '市场部',\n        group_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/2f7e3cc5e3e5a3d0e681096b58281846.png',\n        last_message_time: '周五',\n        last_message_author: '李营销',\n        last_message: '新活动方案已更新'\n      }],\n      currentContactId: '1',\n      messages: [{\n        message_id: 'm1',\n        sender_id: '1',\n        receiver_id: 'myid',\n        content: '你好！关于昨天讨论的项目，我有一些新的想法想和你分享。',\n        message_type: '文本',\n        send_time: '2025-08-04 14:25',\n        read_status: 'read'\n      }, {\n        message_id: 'm2',\n        sender_id: '1',\n        receiver_id: 'myid',\n        content: '我们可以考虑增加一个新的功能模块，提升用户体验。',\n        message_type: '文本',\n        send_time: '2025-08-04 14:26',\n        read_status: 'read'\n      }, {\n        message_id: 'm3',\n        sender_id: 'myid',\n        receiver_id: '1',\n        content: '听起来不错！具体是什么样的功能模块呢？',\n        message_type: '文本',\n        send_time: '2025-08-04 14:28',\n        read_status: 'read'\n      }, {\n        message_id: 'm4',\n        sender_id: '1',\n        receiver_id: 'myid',\n        content: '类似于智能推荐系统，根据用户的使用习惯推荐相关内容。',\n        message_type: '文本',\n        send_time: '2025-08-04 14:30',\n        read_status: 'read'\n      }, {\n        message_id: 'm5',\n        sender_id: '1',\n        receiver_id: 'myid',\n        content: '',\n        message_type: '图片',\n        attachment_url: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/3bfc5386989d115c611663488c75f1d7.png',\n        attachment_name: '推荐系统设计图.png',\n        send_time: '2025-08-04 14:31',\n        read_status: 'read'\n      }, {\n        message_id: 'm6',\n        sender_id: 'myid',\n        receiver_id: '1',\n        content: '这个设计看起来很棒！我们可以安排一个会议详细讨论一下吗？',\n        message_type: '文本',\n        send_time: '2025-08-04 14:32',\n        read_status: 'read'\n      }],\n      inputMessage: '',\n      isTyping: false\n    };\n  },\n  computed: {\n    filteredContacts() {\n      if (!this.search) return this.contacts;\n      return this.contacts.filter(c => c.contact_name.includes(this.search) || c.lastMessage.includes(this.search));\n    },\n    filteredGroups() {\n      if (!this.search) return this.groups;\n      return this.groups.filter(g => g.group_name.includes(this.search) || g.last_message.includes(this.search));\n    },\n    currentContact() {\n      return this.contacts.find(c => c.contact_id === this.currentContactId) || this.groups.find(g => g.group_id === this.currentContactId) || {};\n    },\n    currentContactName() {\n      return this.currentContact.contact_name || this.currentContact.group_name || '';\n    },\n    currentContactAvatar() {\n      return this.currentContact.contact_avatar || this.currentContact.group_avatar || '';\n    },\n    currentContactStatus() {\n      return this.currentContact.contact_status || 'online';\n    },\n    unreadTotal() {\n      return this.contacts.reduce((sum, c) => sum + (c.unread_count || 0), 0);\n    }\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarCollapsed = !this.sidebarCollapsed;\n    },\n    showSidebar() {\n      this.sidebarCollapsed = false;\n    },\n    handleMenuSelect(index) {\n      this.activeMenu = index;\n    },\n    selectContact(id) {\n      this.currentContactId = id;\n      // TODO: 加载对应聊天记录\n    },\n    formatTime(time) {\n      if (!time) return '';\n      if (typeof time === 'string' && time.length > 5) return time.slice(-5);\n      return time;\n    },\n    handleInputKeydown(e) {\n      if (e.key === 'Enter' && !e.shiftKey) {\n        e.preventDefault();\n        this.sendMessage();\n      }\n    },\n    sendMessage() {\n      if (!this.inputMessage.trim()) return;\n      this.messages.push({\n        message_id: 'm' + (this.messages.length + 1),\n        sender_id: this.account.user_id,\n        receiver_id: this.currentContactId,\n        content: this.inputMessage,\n        message_type: '文本',\n        send_time: new Date().toLocaleString(),\n        read_status: 'sent'\n      });\n      this.inputMessage = '';\n      this.$nextTick(() => {\n        if (this.$refs.chatBody) {\n          this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\n        }\n      });\n      // 模拟对方正在输入\n      this.isTyping = true;\n      setTimeout(() => {\n        this.isTyping = false;\n        this.messages.push({\n          message_id: 'm' + (this.messages.length + 1),\n          sender_id: this.currentContactId,\n          receiver_id: this.account.user_id,\n          content: '收到，我会仔细考虑你的建议。',\n          message_type: '文本',\n          send_time: new Date().toLocaleString(),\n          read_status: 'read'\n        });\n        this.$nextTick(() => {\n          if (this.$refs.chatBody) {\n            this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\n          }\n        });\n      }, 2000);\n    },\n    logout() {\n      // TODO: 退出登录逻辑\n      this.$message.success('已退出登录');\n    },\n    handleResize() {\n      this.isMobile = true;\n      if (this.isMobile) {\n        this.sidebarCollapsed = true;\n      } else {\n        this.sidebarCollapsed = false;\n      }\n    }\n  },\n  mounted() {\n    this.handleResize();\n    window.addEventListener('resize', this.handleResize);\n    this.$nextTick(() => {\n      if (this.$refs.chatBody) {\n        this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\n      }\n    });\n  },\n  beforeDestroy() {\n    window.removeEventListener('resize', this.handleResize);\n  }\n};","map":{"version":3,"names":["name","data","sidebarCollapsed","isMobile","search","activeMenu","account","user_id","username","avatar_url","contacts","contact_id","contact_name","contact_avatar","contact_status","last_contact_time","lastMessage","lastMessageType","unread_count","groups","group_id","group_name","group_avatar","last_message_time","last_message_author","last_message","currentContactId","messages","message_id","sender_id","receiver_id","content","message_type","send_time","read_status","attachment_url","attachment_name","inputMessage","isTyping","computed","filteredContacts","filter","c","includes","filteredGroups","g","currentContact","find","currentContactName","currentContactAvatar","currentContactStatus","unreadTotal","reduce","sum","methods","toggleSidebar","showSidebar","handleMenuSelect","index","selectContact","id","formatTime","time","length","slice","handleInputKeydown","e","key","shiftKey","preventDefault","sendMessage","trim","push","Date","toLocaleString","$nextTick","$refs","chatBody","scrollTop","scrollHeight","setTimeout","logout","$message","success","handleResize","mounted","window","addEventListener","beforeDestroy","removeEventListener"],"sources":["src/views/ChatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-app\">\r\n    <!-- 侧边导航栏 -->\r\n    <aside :class=\"['sidebar', { 'sidebar-collapsed': sidebarCollapsed }]\" ref=\"sidebar\">\r\n      <!-- 应用标题 -->\r\n      <div class=\"sidebar-title-bar\">\r\n        <span class=\"sidebar-title\">\r\n          <i class=\"el-icon-message\"></i>\r\n          站内信\r\n        </span>\r\n        <el-button\r\n          icon=\"el-icon-arrow-left\"\r\n          @click=\"toggleSidebar\"\r\n          circle\r\n          class=\"sidebar-toggle-btn\"\r\n          v-show=\"isMobile\"\r\n        />\r\n      </div>\r\n      <!-- 搜索框 -->\r\n      <div class=\"sidebar-search\">\r\n        <el-input\r\n          v-model=\"search\"\r\n          placeholder=\"搜索对话...\"\r\n          prefix-icon=\"el-icon-search\"\r\n          size=\"small\"\r\n          clearable\r\n        />\r\n      </div>\r\n      <!-- 导航菜单 -->\r\n      <el-menu\r\n        :default-active=\"activeMenu\"\r\n        class=\"sidebar-menu\"\r\n        :collapse=\"sidebarCollapsed\"\r\n        @select=\"handleMenuSelect\"\r\n      >\r\n        <el-menu-item index=\"message\">\r\n          <i class=\"el-icon-message\"></i>\r\n          <span slot=\"title\">消息</span>\r\n          <el-badge :value=\"unreadTotal\" class=\"menu-badge\" v-if=\"unreadTotal > 0\"/>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"group\">\r\n          <i class=\"el-icon-user-solid\"></i>\r\n          <span slot=\"title\">群组</span>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"favorite\">\r\n          <i class=\"el-icon-star-on\"></i>\r\n          <span slot=\"title\">收藏</span>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"history\">\r\n          <i class=\"el-icon-time\"></i>\r\n          <span slot=\"title\">历史记录</span>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"setting\">\r\n          <i class=\"el-icon-setting\"></i>\r\n          <span slot=\"title\">设置</span>\r\n        </el-menu-item>\r\n      </el-menu>\r\n      <!-- 联系人列表 -->\r\n      <div class=\"sidebar-list\">\r\n        <div class=\"sidebar-list-title\">最近联系人</div>\r\n        <div class=\"sidebar-list-scroll\">\r\n          <div\r\n            v-for=\"contact in filteredContacts\"\r\n            :key=\"contact.contact_id\"\r\n            :class=\"['contact-item', { active: contact.contact_id === currentContactId }]\"\r\n            @click=\"selectContact(contact.contact_id)\"\r\n          >\r\n            <div class=\"avatar-wrap\">\r\n              <img :src=\"contact.contact_avatar\" class=\"avatar\" />\r\n              <span\r\n                class=\"status-dot\"\r\n                :class=\"contact.contact_status === 'online' ? 'online' : 'offline'\"\r\n              ></span>\r\n            </div>\r\n            <div class=\"contact-info\">\r\n              <div class=\"contact-top\">\r\n                <span class=\"contact-name\">{{ contact.contact_name }}</span>\r\n                <span class=\"contact-time\">{{ contact.last_contact_time }}</span>\r\n              </div>\r\n              <div class=\"contact-bottom\">\r\n                <span v-if=\"contact.lastMessageType === 'file'\">\r\n                  <i class=\"el-icon-document\"></i>\r\n                  {{ contact.lastMessage }}\r\n                </span>\r\n                <span v-else>{{ contact.lastMessage }}</span>\r\n              </div>\r\n            </div>\r\n            <el-badge\r\n              :value=\"contact.unread_count\"\r\n              class=\"contact-badge\"\r\n              v-if=\"contact.unread_count > 0\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"sidebar-list-title\">群组</div>\r\n        <div class=\"sidebar-list-scroll\">\r\n          <div\r\n            v-for=\"group in filteredGroups\"\r\n            :key=\"group.group_id\"\r\n            :class=\"['contact-item', { active: group.group_id === currentContactId }]\"\r\n            @click=\"selectContact(group.group_id)\"\r\n          >\r\n            <div class=\"avatar-wrap\">\r\n              <img :src=\"group.group_avatar\" class=\"avatar\" />\r\n            </div>\r\n            <div class=\"contact-info\">\r\n              <div class=\"contact-top\">\r\n                <span class=\"contact-name\">{{ group.group_name }}</span>\r\n                <span class=\"contact-time\">{{ group.last_message_time }}</span>\r\n              </div>\r\n              <div class=\"contact-bottom\">\r\n                <span>\r\n                  <span class=\"contact-group-author\">{{ group.last_message_author }}: </span>\r\n                  {{ group.last_message }}\r\n                </span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 用户信息 -->\r\n      <div class=\"sidebar-account\">\r\n        <img :src=\"account.avatar_url\" class=\"avatar\" />\r\n        <div class=\"account-info\">\r\n          <div class=\"account-name\">{{ account.username }}</div>\r\n          <div class=\"account-status\">\r\n            <span class=\"status-dot online\"></span>\r\n            在线\r\n          </div>\r\n        </div>\r\n        <el-button icon=\"el-icon-switch-button\" type=\"text\" @click=\"logout\"></el-button>\r\n      </div>\r\n    </aside>\r\n    <!-- 主聊天区域 -->\r\n    <main class=\"chat-main\">\r\n      <!-- 顶部聊天信息栏 -->\r\n      <header class=\"chat-header\">\r\n        <div class=\"chat-header-left\">\r\n          <el-button\r\n            icon=\"el-icon-menu\"\r\n            @click=\"showSidebar\"\r\n            circle\r\n            class=\"sidebar-show-btn\"\r\n            v-show=\"isMobile\"\r\n          />\r\n          <div class=\"avatar-wrap\">\r\n            <img :src=\"currentContactAvatar\" class=\"avatar\" />\r\n            <span\r\n              class=\"status-dot\"\r\n              :class=\"currentContactStatus === 'online' ? 'online' : 'offline'\"\r\n            ></span>\r\n          </div>\r\n          <div>\r\n            <div class=\"chat-header-name\">{{ currentContactName }}</div>\r\n            <div class=\"chat-header-status\">\r\n              <span class=\"status-dot online\"></span>\r\n              在线\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"chat-header-actions\">\r\n          <el-button icon=\"el-icon-search\" circle />\r\n          <el-button icon=\"el-icon-phone\" circle />\r\n          <el-button icon=\"el-icon-video-camera\" circle />\r\n          <el-button icon=\"el-icon-more\" circle />\r\n        </div>\r\n      </header>\r\n      <!-- 聊天消息区域 -->\r\n      <div class=\"chat-body\" ref=\"chatBody\">\r\n        <div class=\"chat-date-divider\">\r\n          今天 14:25\r\n        </div>\r\n        <div v-for=\"msg in messages\" :key=\"msg.message_id\" :class=\"['chat-message', msg.sender_id === account.user_id ? 'self' : 'other']\">\r\n          <img v-if=\"msg.sender_id !== account.user_id\" :src=\"currentContactAvatar\" class=\"avatar msg-avatar\" />\r\n          <div class=\"msg-content-wrap\">\r\n            <div class=\"msg-meta\" v-if=\"msg.sender_id !== account.user_id\">\r\n              <span class=\"msg-name\">{{ currentContactName }}</span>\r\n              <span class=\"msg-time\">{{ formatTime(msg.send_time) }}</span>\r\n            </div>\r\n            <div\r\n              v-if=\"msg.message_type === '文本'\"\r\n              :class=\"['msg-bubble', msg.sender_id === account.user_id ? 'right' : 'left']\"\r\n            >\r\n              <span class=\"msg-text\">{{ msg.content }}</span>\r\n            </div>\r\n            <div\r\n              v-else-if=\"msg.message_type === '图片'\"\r\n              :class=\"['msg-bubble', msg.sender_id === account.user_id ? 'right' : 'left']\"\r\n            >\r\n              <img :src=\"msg.attachment_url\" class=\"msg-img\" />\r\n              <div class=\"msg-file-meta\">\r\n                <span>{{ msg.attachment_name }}</span>\r\n              </div>\r\n            </div>\r\n            <div\r\n              v-else-if=\"msg.message_type === '文件'\"\r\n              :class=\"['msg-bubble', msg.sender_id === account.user_id ? 'right' : 'left']\"\r\n            >\r\n              <i class=\"el-icon-document\"></i>\r\n              <span>{{ msg.attachment_name }}</span>\r\n            </div>\r\n            <div\r\n              v-else-if=\"msg.message_type === '语音'\"\r\n              :class=\"['msg-bubble', msg.sender_id === account.user_id ? 'right' : 'left']\"\r\n            >\r\n              <i class=\"el-icon-microphone\"></i>\r\n              <span>语音消息</span>\r\n            </div>\r\n            <div class=\"msg-meta-bottom\" v-if=\"msg.sender_id === account.user_id\">\r\n              <span class=\"msg-time\">{{ formatTime(msg.send_time) }}</span>\r\n              <i v-if=\"msg.read_status === 'read'\" class=\"el-icon-check\"></i>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- 对方正在输入 -->\r\n        <div v-if=\"isTyping\" class=\"chat-message other\">\r\n          <img :src=\"currentContactAvatar\" class=\"avatar msg-avatar\" />\r\n          <div class=\"msg-content-wrap\">\r\n            <div class=\"msg-bubble left\">\r\n              <span class=\"typing-dot\"></span>\r\n              <span class=\"typing-dot\"></span>\r\n              <span class=\"typing-dot\"></span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 底部输入区域 -->\r\n      <footer class=\"chat-footer\">\r\n        <div class=\"chat-footer-actions\">\r\n          <el-button icon=\"el-icon-smile\" circle />\r\n          <el-button icon=\"el-icon-paperclip\" circle />\r\n          <el-button icon=\"el-icon-picture\" circle />\r\n          <el-button icon=\"el-icon-microphone\" circle />\r\n          <div style=\"flex:1\"></div>\r\n          <el-button icon=\"el-icon-more\" circle />\r\n        </div>\r\n        <div class=\"chat-footer-input\">\r\n          <el-input\r\n            type=\"textarea\"\r\n            v-model=\"inputMessage\"\r\n            :autosize=\"{ minRows: 1, maxRows: 4, maxHeight: 100 }\"\r\n            placeholder=\"输入消息...\"\r\n            ref=\"input\"\r\n            @keydown.native=\"handleInputKeydown\"\r\n            class=\"msg-input\"\r\n          />\r\n          <el-button\r\n            icon=\"el-icon-caret-right\"\r\n            type=\"primary\"\r\n            circle\r\n            @click=\"sendMessage\"\r\n            class=\"send-btn\"\r\n          />\r\n        </div>\r\n      </footer>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ChatView',\r\n  data() {\r\n    return {\r\n      sidebarCollapsed: false,\r\n      isMobile: false,\r\n      search: '',\r\n      activeMenu: 'message',\r\n      account: {\r\n        user_id: 'myid',\r\n        username: '我的账户',\r\n        avatar_url: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/1cec2ca1d9a7cf4e5e06c75370e677e7.png'\r\n      },\r\n      contacts: [\r\n        {\r\n          contact_id: '1',\r\n          contact_name: '张三',\r\n          contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/e2f88577a8fde6d7ed70dd51df8d35aa.png',\r\n          contact_status: 'online',\r\n          last_contact_time: '14:32',\r\n          lastMessage: '好的，我明白了，明天见！',\r\n          lastMessageType: 'text',\r\n          unread_count: 0\r\n        },\r\n        {\r\n          contact_id: '2',\r\n          contact_name: '李四',\r\n          contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/dcdbda4e30edf6c9f34341a51ca4376c.png',\r\n          contact_status: 'online',\r\n          last_contact_time: '昨天',\r\n          lastMessage: '项目方案已经发送到您邮箱，请查收',\r\n          lastMessageType: 'text',\r\n          unread_count: 3\r\n        },\r\n        {\r\n          contact_id: '3',\r\n          contact_name: '王经理',\r\n          contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/8666b7c384360229440b4d45b6ed8cbc.png',\r\n          contact_status: 'offline',\r\n          last_contact_time: '周一',\r\n          lastMessage: '季度报告.pdf',\r\n          lastMessageType: 'file',\r\n          unread_count: 0\r\n        },\r\n        {\r\n          contact_id: '4',\r\n          contact_name: '技术支持',\r\n          contact_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/dfdd661625852bbf34fded8ae159a1cd.png',\r\n          contact_status: 'online',\r\n          last_contact_time: '上周',\r\n          lastMessage: '问题已经修复，请测试确认',\r\n          lastMessageType: 'text',\r\n          unread_count: 0\r\n        }\r\n      ],\r\n      groups: [\r\n        {\r\n          group_id: '5',\r\n          group_name: '产品开发组',\r\n          group_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/15bd703716eb575eb049d376ef18625e.png',\r\n          last_message_time: '10:25',\r\n          last_message_author: '赵工程师',\r\n          last_message: '后端API已经准备好了'\r\n        },\r\n        {\r\n          group_id: '6',\r\n          group_name: '市场部',\r\n          group_avatar: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/2f7e3cc5e3e5a3d0e681096b58281846.png',\r\n          last_message_time: '周五',\r\n          last_message_author: '李营销',\r\n          last_message: '新活动方案已更新'\r\n        }\r\n      ],\r\n      currentContactId: '1',\r\n      messages: [\r\n        {\r\n          message_id: 'm1',\r\n          sender_id: '1',\r\n          receiver_id: 'myid',\r\n          content: '你好！关于昨天讨论的项目，我有一些新的想法想和你分享。',\r\n          message_type: '文本',\r\n          send_time: '2025-08-04 14:25',\r\n          read_status: 'read'\r\n        },\r\n        {\r\n          message_id: 'm2',\r\n          sender_id: '1',\r\n          receiver_id: 'myid',\r\n          content: '我们可以考虑增加一个新的功能模块，提升用户体验。',\r\n          message_type: '文本',\r\n          send_time: '2025-08-04 14:26',\r\n          read_status: 'read'\r\n        },\r\n        {\r\n          message_id: 'm3',\r\n          sender_id: 'myid',\r\n          receiver_id: '1',\r\n          content: '听起来不错！具体是什么样的功能模块呢？',\r\n          message_type: '文本',\r\n          send_time: '2025-08-04 14:28',\r\n          read_status: 'read'\r\n        },\r\n        {\r\n          message_id: 'm4',\r\n          sender_id: '1',\r\n          receiver_id: 'myid',\r\n          content: '类似于智能推荐系统，根据用户的使用习惯推荐相关内容。',\r\n          message_type: '文本',\r\n          send_time: '2025-08-04 14:30',\r\n          read_status: 'read'\r\n        },\r\n        {\r\n          message_id: 'm5',\r\n          sender_id: '1',\r\n          receiver_id: 'myid',\r\n          content: '',\r\n          message_type: '图片',\r\n          attachment_url: 'https://design.gemcoder.com/staticResource/echoAiSystemImages/3bfc5386989d115c611663488c75f1d7.png',\r\n          attachment_name: '推荐系统设计图.png',\r\n          send_time: '2025-08-04 14:31',\r\n          read_status: 'read'\r\n        },\r\n        {\r\n          message_id: 'm6',\r\n          sender_id: 'myid',\r\n          receiver_id: '1',\r\n          content: '这个设计看起来很棒！我们可以安排一个会议详细讨论一下吗？',\r\n          message_type: '文本',\r\n          send_time: '2025-08-04 14:32',\r\n          read_status: 'read'\r\n        }\r\n      ],\r\n      inputMessage: '',\r\n      isTyping: false\r\n    };\r\n  },\r\n  computed: {\r\n    filteredContacts() {\r\n      if (!this.search) return this.contacts;\r\n      return this.contacts.filter(c => c.contact_name.includes(this.search) || c.lastMessage.includes(this.search));\r\n    },\r\n    filteredGroups() {\r\n      if (!this.search) return this.groups;\r\n      return this.groups.filter(g => g.group_name.includes(this.search) || g.last_message.includes(this.search));\r\n    },\r\n    currentContact() {\r\n      return (\r\n        this.contacts.find(c => c.contact_id === this.currentContactId) ||\r\n        this.groups.find(g => g.group_id === this.currentContactId) ||\r\n        {}\r\n      );\r\n    },\r\n    currentContactName() {\r\n      return this.currentContact.contact_name || this.currentContact.group_name || '';\r\n    },\r\n    currentContactAvatar() {\r\n      return this.currentContact.contact_avatar || this.currentContact.group_avatar || '';\r\n    },\r\n    currentContactStatus() {\r\n      return this.currentContact.contact_status || 'online';\r\n    },\r\n    unreadTotal() {\r\n      return this.contacts.reduce((sum, c) => sum + (c.unread_count || 0), 0);\r\n    }\r\n  },\r\n  methods: {\r\n    toggleSidebar() {\r\n      this.sidebarCollapsed = !this.sidebarCollapsed;\r\n    },\r\n    showSidebar() {\r\n      this.sidebarCollapsed = false;\r\n    },\r\n    handleMenuSelect(index) {\r\n      this.activeMenu = index;\r\n    },\r\n    selectContact(id) {\r\n      this.currentContactId = id;\r\n      // TODO: 加载对应聊天记录\r\n    },\r\n    formatTime(time) {\r\n      if (!time) return '';\r\n      if (typeof time === 'string' && time.length > 5) return time.slice(-5);\r\n      return time;\r\n    },\r\n    handleInputKeydown(e) {\r\n      if (e.key === 'Enter' && !e.shiftKey) {\r\n        e.preventDefault();\r\n        this.sendMessage();\r\n      }\r\n    },\r\n    sendMessage() {\r\n      if (!this.inputMessage.trim()) return;\r\n      this.messages.push({\r\n        message_id: 'm' + (this.messages.length + 1),\r\n        sender_id: this.account.user_id,\r\n        receiver_id: this.currentContactId,\r\n        content: this.inputMessage,\r\n        message_type: '文本',\r\n        send_time: new Date().toLocaleString(),\r\n        read_status: 'sent'\r\n      });\r\n      this.inputMessage = '';\r\n      this.$nextTick(() => {\r\n        if (this.$refs.chatBody) {\r\n          this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\r\n        }\r\n      });\r\n      // 模拟对方正在输入\r\n      this.isTyping = true;\r\n      setTimeout(() => {\r\n        this.isTyping = false;\r\n        this.messages.push({\r\n          message_id: 'm' + (this.messages.length + 1),\r\n          sender_id: this.currentContactId,\r\n          receiver_id: this.account.user_id,\r\n          content: '收到，我会仔细考虑你的建议。',\r\n          message_type: '文本',\r\n          send_time: new Date().toLocaleString(),\r\n          read_status: 'read'\r\n        });\r\n        this.$nextTick(() => {\r\n          if (this.$refs.chatBody) {\r\n            this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\r\n          }\r\n        });\r\n      }, 2000);\r\n    },\r\n    logout() {\r\n      // TODO: 退出登录逻辑\r\n      this.$message.success('已退出登录');\r\n    },\r\n    handleResize() {\r\n      this.isMobile  = true\r\n      if (this.isMobile) {\r\n        this.sidebarCollapsed = true;\r\n      } else {\r\n        this.sidebarCollapsed = false;\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.handleResize();\r\n    window.addEventListener('resize', this.handleResize);\r\n    this.$nextTick(() => {\r\n      if (this.$refs.chatBody) {\r\n        this.$refs.chatBody.scrollTop = this.$refs.chatBody.scrollHeight;\r\n      }\r\n    });\r\n  },\r\n  beforeDestroy() {\r\n    window.removeEventListener('resize', this.handleResize);\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.chat-app {\r\n  display: flex;\r\n  height: 83vh;\r\n  background: #f1f2f6;\r\n  font-family: 'Inter', 'Segoe UI', 'sans-serif';\r\n  overflow: hidden;\r\n}\r\n.sidebar {\r\n  width: 260px;\r\n  min-width: 220px;\r\n  background: #fff;\r\n  box-shadow: 0 4px 20px rgba(0,0,0,0.08);\r\n  display: flex;\r\n  flex-direction: column;\r\n  transition: width 0.3s;\r\n  z-index: 10;\r\n}\r\n.sidebar-collapsed {\r\n  width: 0 !important;\r\n  min-width: 0 !important;\r\n  overflow: hidden;\r\n}\r\n.sidebar-title-bar {\r\n  padding: 2px;\r\n  border-bottom: 1px solid #e0e0e0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n.sidebar-title {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  color: #6C5CE7;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.sidebar-toggle-btn {\r\n  margin-left: 8px;\r\n}\r\n.sidebar-search {\r\n  padding: 12px 16px;\r\n  border-bottom: 1px solid #e0e0e0;\r\n}\r\n.sidebar-menu {\r\n  border: none;\r\n  margin-bottom: 8px;\r\n}\r\n.menu-badge {\r\n  float: right;\r\n  margin-left: 8px;\r\n}\r\n.sidebar-list {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding-bottom: 8px;\r\n}\r\n.sidebar-list-title {\r\n  font-size: 13px;\r\n  font-weight: bold;\r\n  color: #888;\r\n  padding: 10px 16px 4px 16px;\r\n  background: #fff;\r\n}\r\n.sidebar-list-scroll {\r\n  max-height: 180px;\r\n  overflow-y: auto;\r\n  padding-bottom: 4px;\r\n}\r\n.contact-item