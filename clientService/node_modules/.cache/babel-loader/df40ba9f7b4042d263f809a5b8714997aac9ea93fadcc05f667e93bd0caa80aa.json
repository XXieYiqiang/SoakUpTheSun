{"ast":null,"code":"\"use strict\";\n\nvar _interopRequireDefault = require(\"C:/Users/Administrator/Desktop/GimChat/gimChatVue/node_modules/@babel/runtime/helpers/interopRequireDefault.js\").default;\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\nrequire(\"core-js/modules/es.array.push.js\");\nrequire(\"core-js/modules/es.iterator.constructor.js\");\nrequire(\"core-js/modules/es.iterator.find.js\");\nrequire(\"core-js/modules/es.iterator.for-each.js\");\nrequire(\"core-js/modules/es.iterator.map.js\");\nvar _marked = require(\"marked\");\nvar _highlight = _interopRequireDefault(require(\"highlight.js\"));\nrequire(\"highlight.js/styles/github.css\");\n// ‰Ω†ÂèØ‰ª•Êç¢ÊàêÂÖ∂‰ªñ‰∏ªÈ¢òÊ†∑Âºè\n\n_marked.marked.setOptions({\n  breaks: true,\n  gfm: true,\n  highlight(code, lang) {\n    return lang && _highlight.default.getLanguage(lang) ? _highlight.default.highlight(code, {\n      language: lang\n    }).value : _highlight.default.highlightAuto(code).value;\n  }\n});\nvar _default = exports.default = {\n  data() {\n    return {\n      selectedFile: null,\n      // Êñ∞Â¢ûÔºöÂ∑≤ÈÄâÊñá‰ª∂\n      chatMessages: [{\n        id: Date.now(),\n        role: 'ai',\n        content: 'ÂòøÔºÅÊàëÊòØ **AI Â∞èÂä©Êâã**ÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû',\n        renderedContent: ''\n      }],\n      userInput: '',\n      socket: null,\n      loading: false,\n      currentAiMessageId: null,\n      typingQueue: [],\n      typingTimer: null,\n      isTyping: false\n    };\n  },\n  mounted() {\n    // ÂàùÂßãÂåñ Markdown Ê∏≤Êüì\n    this.chatMessages.forEach(msg => {\n      msg.renderedContent = _marked.marked.parse(msg.content);\n    });\n    this.initSocket();\n  },\n  beforeDestroy() {\n    if (this.typingTimer) {\n      clearTimeout(this.typingTimer);\n    }\n  },\n  methods: {\n    /** ÈôÑ‰ª∂È¢ÑËßà ÈÄâ‰∏≠‰∫ã‰ª∂*/\n    handleFileSelect(e) {\n      const file = e.target.files[0];\n      if (!file) return;\n      this.selectedFile = file;\n    },\n    /**\r\n     * Ê∂àÊÅØId ÁîüÊàê\r\n     */\n    generateId() {\n      return Date.now() + Math.random();\n    },\n    /**\r\n     * ÂàùÂßãÂåñ Web Socket ÊúçÂä°\r\n     */\n    initSocket() {\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws');\n      this.socket.addEventListener('open', () => {\n        console.log('WebSocket Â∑≤ËøûÊé•');\n      });\n      this.socket.addEventListener('message', event => {\n        if (event.data === '[DONE]') {\n          this.loading = false;\n          this.currentAiMessageId = null;\n          return;\n        }\n        this.queueTextForTyping(event.data);\n      });\n      this.socket.addEventListener('close', () => {\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠');\n      });\n      this.socket.addEventListener('error', error => {\n        console.error('WebSocket ÈîôËØØ:', error);\n      });\n    },\n    // Â§ÑÁêÜÊ∂àÊÅØÂèëÈÄÅ\n    async handleSend() {\n      const msg = this.userInput.trim();\n\n      // ‚úÖ Ê†°È™åËæìÂÖ•Âíå WebSocket Áä∂ÊÄÅ\n      if (!msg && !this.selectedFile) return;\n      if (!this.socket || this.socket.readyState !== WebSocket.OPEN) {\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÂèëÈÄÅÂ§±Ë¥•');\n        return;\n      }\n\n      // 1. ÊòæÁ§∫Áî®Êà∑Ê∂àÊÅØÔºàÊ∏≤Êüì markdownÔºåÂè™Ê∏≤ÊüìÊñáÊú¨ÈÉ®ÂàÜÔºâ\n      let displayContent = msg;\n      if (this.selectedFile) {\n        displayContent += `\\nüìé ÈôÑ‰ª∂: ${this.selectedFile.name}`;\n      }\n      this.chatMessages.push({\n        id: this.generateId(),\n        role: 'user',\n        content: displayContent,\n        renderedContent: _marked.marked.parse(msg || '') // ÈôÑ‰ª∂‰∏çÂèÇ‰∏é markdown Ê∏≤Êüì\n      });\n\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°ÜÔºåÂºÄÂêØÂä†ËΩΩ\n      this.userInput = '';\n      this.loading = true;\n      this.scrollToBottom();\n\n      // 2. ÁªÑË£Ö‰∏ä‰∏ãÊñáÔºàÊñπ‰æøÂêéÁ´Ø‰øùÊåÅ‰ºöËØùÔºâ\n      const context = this.chatMessages.map(m => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content).join('\\n');\n      try {\n        // 3. Âà§Êñ≠ÊòØÂèëÈÄÅÈôÑ‰ª∂ËøòÊòØÁ∫ØÊñáÊú¨\n        if (this.selectedFile) {\n          console.log('[Chat] üìé ÂèëÈÄÅÈôÑ‰ª∂:', this.selectedFile.name);\n          const base64 = await this.fileToBase64(this.selectedFile);\n          this.safeSend({\n            type: 'file',\n            name: this.selectedFile.name,\n            data: base64,\n            text: msg || '',\n            context\n          });\n          this.selectedFile = null;\n        } else {\n          console.log('[Chat] üí¨ ÂèëÈÄÅÊñáÊú¨:', msg);\n          this.safeSend({\n            type: 'text',\n            text: msg,\n            context\n          });\n        }\n      } catch (err) {\n        console.error('[Chat] ‚ùå Ê∂àÊÅØÂèëÈÄÅÂ§±Ë¥•:', err);\n      }\n    },\n    /**\r\n     * ‚úÖ Â∞ÅË£ÖÂÆâÂÖ®ÁöÑ WebSocket ÂèëÈÄÅÊñπÊ≥ïÔºåÈÅøÂÖç TEXT_PARTIAL_WRITING ÈîôËØØ\r\n     */\n    safeSend(data) {\n      if (this.socket && this.socket.readyState === WebSocket.OPEN) {\n        this.socket.send(typeof data === 'string' ? data : JSON.stringify(data));\n      } else {\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÊú™ÂèëÈÄÅ');\n      }\n    },\n    // Êñá‰ª∂Â§ÑÁêÜ‰∏∫Base64\n    fileToBase64(file) {\n      return new Promise((resolve, reject) => {\n        const reader = new FileReader();\n        reader.readAsDataURL(file);\n        reader.onload = () => resolve(reader.result);\n        reader.onerror = reject;\n      });\n    },\n    queueTextForTyping(text) {\n      const chars = Array.from(text);\n      this.typingQueue.push(...chars);\n      if (!this.isTyping) {\n        this.startTyping();\n      }\n    },\n    startTyping() {\n      this.isTyping = true;\n      let last = this.chatMessages.find(msg => msg.id === this.currentAiMessageId);\n      if (!last) {\n        last = {\n          id: this.generateId(),\n          role: 'ai',\n          content: '',\n          renderedContent: ''\n        };\n        this.chatMessages.push(last);\n        this.currentAiMessageId = last.id;\n      }\n      const typeNext = () => {\n        if (this.typingQueue.length === 0) {\n          this.isTyping = false;\n          return;\n        }\n        const nextChar = this.typingQueue.shift();\n        last.content += nextChar;\n        last.renderedContent = _marked.marked.parse(last.content);\n        this.$forceUpdate();\n        this.scrollToBottom();\n        this.$nextTick(() => {\n          _highlight.default.highlightAll(); // ËØ≠Ê≥ïÈ´ò‰∫ÆÈáçÊñ∞ÊâßË°å\n        });\n        this.typingTimer = setTimeout(typeNext, 20);\n      };\n      typeNext();\n    },\n    scrollToBottom() {\n      this.$nextTick(() => {\n        const el = this.$refs.chatList;\n        if (el) el.scrollTop = el.scrollHeight;\n      });\n    }\n  }\n};","map":{"version":3,"names":["_marked","require","_highlight","_interopRequireDefault","marked","setOptions","breaks","gfm","highlight","code","lang","hljs","getLanguage","language","value","highlightAuto","_default","exports","default","data","selectedFile","chatMessages","id","Date","now","role","content","renderedContent","userInput","socket","loading","currentAiMessageId","typingQueue","typingTimer","isTyping","mounted","forEach","msg","parse","initSocket","beforeDestroy","clearTimeout","methods","handleFileSelect","e","file","target","files","generateId","Math","random","WebSocket","addEventListener","console","log","event","queueTextForTyping","error","handleSend","trim","readyState","OPEN","warn","displayContent","name","push","scrollToBottom","context","map","m","join","base64","fileToBase64","safeSend","type","text","err","send","JSON","stringify","Promise","resolve","reject","reader","FileReader","readAsDataURL","onload","result","onerror","chars","Array","from","startTyping","last","find","typeNext","length","nextChar","shift","$forceUpdate","$nextTick","highlightAll","setTimeout","el","$refs","chatList","scrollTop","scrollHeight"],"sources":["src/views/AIView.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-container\">\r\n    <!-- ËÅäÂ§©Ê∂àÊÅØÂå∫ -->\r\n    <div class=\"msg-list\" ref=\"chatList\">\r\n      <div v-for=\"msg in chatMessages\" :key=\"msg.id\" :class=\"['msg', msg.role === 'user' ? 'msg-user' : 'msg-ai']\">\r\n        <!-- <div class=\"avatar\" v-if=\"msg.role === 'ai'\">üí°</div> -->\r\n        <div class=\"avatar\" v-if=\"msg.role === 'ai'\">\r\n          <img src=\"@/assets/images/avatar/openai.svg\" alt=\"AI Avatar\" class=\"avatar-img\" />\r\n        </div>\r\n        <div class=\"msg-content\" v-html=\"msg.renderedContent\"></div>\r\n        <div class=\"avatar\" v-if=\"msg.role === 'user'\">üßë</div>\r\n      </div>\r\n\r\n      <!-- AI ÊâìÂ≠ó‰∏≠... -->\r\n      <div class=\"ai-loading\" v-if=\"loading\">\r\n        <span style=\"margin-right: 10px;\">AI Ê≠£Âú®ËæìÂÖ•</span>\r\n        <div class=\"dot-flash\">\r\n          <div class=\"dot\" v-for=\"i in 3\" :key=\"i\" :style=\"{ animationDelay: i * 0.2 + 's' }\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Â∫ïÈÉ®ËæìÂÖ•Ê†è -->\r\n    <div class=\"input-bar\" :class=\"{ 'input-bottom': chatMessages.length > 0 }\">\r\n      <!-- ÈôÑ‰ª∂ÊåâÈíÆ -->\r\n      <input ref=\"fileInput\" type=\"file\" accept=\".txt,.pdf,.doc,.docx\" style=\"display:none\"\r\n        @change=\"handleFileSelect\" />\r\n      <button class=\"attach-btn\" @click=\"$refs.fileInput.click()\">üìé</button>\r\n      <div class=\"input-wrapper\">\r\n        <input v-model=\"userInput\" @keyup.enter=\"handleSend\" placeholder=\"ËØ∑ËæìÂÖ•ÈóÆÈ¢ò...\" class=\"input-box\" />\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÈôÑ‰ª∂È¢ÑËßà -->\r\n    <div v-if=\"selectedFile\" class=\"file-preview\">\r\n      <span>{{ selectedFile.name }}</span>\r\n      <button class=\"remove-file\" @click=\"selectedFile = null\">‚ùå</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { marked } from 'marked'\r\nimport hljs from 'highlight.js'\r\nimport 'highlight.js/styles/github.css' // ‰Ω†ÂèØ‰ª•Êç¢ÊàêÂÖ∂‰ªñ‰∏ªÈ¢òÊ†∑Âºè\r\n\r\nmarked.setOptions({\r\n  breaks: true,\r\n  gfm: true,\r\n  highlight(code, lang) {\r\n    return lang && hljs.getLanguage(lang)\r\n      ? hljs.highlight(code, { language: lang }).value\r\n      : hljs.highlightAuto(code).value;\r\n  }\r\n})\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedFile: null, // Êñ∞Â¢ûÔºöÂ∑≤ÈÄâÊñá‰ª∂\r\n      chatMessages: [\r\n        {\r\n          id: Date.now(),\r\n          role: 'ai',\r\n          content: 'ÂòøÔºÅÊàëÊòØ **AI Â∞èÂä©Êâã**ÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû',\r\n          renderedContent: '',\r\n        },\r\n      ],\r\n      userInput: '',\r\n      socket: null,\r\n      loading: false,\r\n      currentAiMessageId: null,\r\n      typingQueue: [],\r\n      typingTimer: null,\r\n      isTyping: false,\r\n    }\r\n  },\r\n  mounted() {\r\n    // ÂàùÂßãÂåñ Markdown Ê∏≤Êüì\r\n    this.chatMessages.forEach((msg) => {\r\n      msg.renderedContent = marked.parse(msg.content)\r\n    })\r\n    this.initSocket()\r\n  },\r\n  beforeDestroy() {\r\n    if (this.typingTimer) {\r\n      clearTimeout(this.typingTimer)\r\n    }\r\n  },\r\n  methods: {\r\n    /** ÈôÑ‰ª∂È¢ÑËßà ÈÄâ‰∏≠‰∫ã‰ª∂*/\r\n    handleFileSelect(e) {\r\n\r\n      const file = e.target.files[0]\r\n      if (!file) return\r\n      this.selectedFile = file\r\n\r\n    },\r\n    /**\r\n     * Ê∂àÊÅØId ÁîüÊàê\r\n     */\r\n    generateId() {\r\n      return Date.now() + Math.random()\r\n    },\r\n    /**\r\n     * ÂàùÂßãÂåñ Web Socket ÊúçÂä°\r\n     */\r\n    initSocket() {\r\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws')\r\n\r\n      this.socket.addEventListener('open', () => {\r\n        console.log('WebSocket Â∑≤ËøûÊé•')\r\n      })\r\n\r\n      this.socket.addEventListener('message', (event) => {\r\n        if (event.data === '[DONE]') {\r\n          this.loading = false\r\n          this.currentAiMessageId = null\r\n          return\r\n        }\r\n        this.queueTextForTyping(event.data)\r\n      })\r\n\r\n      this.socket.addEventListener('close', () => {\r\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠')\r\n      })\r\n\r\n      this.socket.addEventListener('error', (error) => {\r\n        console.error('WebSocket ÈîôËØØ:', error)\r\n      })\r\n    },\r\n    // Â§ÑÁêÜÊ∂àÊÅØÂèëÈÄÅ\r\n    async handleSend() {\r\n      const msg = this.userInput.trim()\r\n\r\n      // ‚úÖ Ê†°È™åËæìÂÖ•Âíå WebSocket Áä∂ÊÄÅ\r\n      if (!msg && !this.selectedFile) return\r\n      if (!this.socket || this.socket.readyState !== WebSocket.OPEN) {\r\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÂèëÈÄÅÂ§±Ë¥•')\r\n        return\r\n      }\r\n\r\n      // 1. ÊòæÁ§∫Áî®Êà∑Ê∂àÊÅØÔºàÊ∏≤Êüì markdownÔºåÂè™Ê∏≤ÊüìÊñáÊú¨ÈÉ®ÂàÜÔºâ\r\n      let displayContent = msg\r\n      if (this.selectedFile) {\r\n        displayContent += `\\nüìé ÈôÑ‰ª∂: ${this.selectedFile.name}`\r\n      }\r\n\r\n      this.chatMessages.push({\r\n        id: this.generateId(),\r\n        role: 'user',\r\n        content: displayContent,\r\n        renderedContent: marked.parse(msg || ''), // ÈôÑ‰ª∂‰∏çÂèÇ‰∏é markdown Ê∏≤Êüì\r\n      })\r\n\r\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°ÜÔºåÂºÄÂêØÂä†ËΩΩ\r\n      this.userInput = ''\r\n      this.loading = true\r\n      this.scrollToBottom()\r\n\r\n      // 2. ÁªÑË£Ö‰∏ä‰∏ãÊñáÔºàÊñπ‰æøÂêéÁ´Ø‰øùÊåÅ‰ºöËØùÔºâ\r\n      const context = this.chatMessages\r\n        .map((m) => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content)\r\n        .join('\\n')\r\n\r\n      try {\r\n        // 3. Âà§Êñ≠ÊòØÂèëÈÄÅÈôÑ‰ª∂ËøòÊòØÁ∫ØÊñáÊú¨\r\n        if (this.selectedFile) {\r\n          console.log('[Chat] üìé ÂèëÈÄÅÈôÑ‰ª∂:', this.selectedFile.name)\r\n          const base64 = await this.fileToBase64(this.selectedFile)\r\n\r\n          this.safeSend({\r\n            type: 'file',\r\n            name: this.selectedFile.name,\r\n            data: base64,\r\n            text: msg || '',\r\n            context\r\n          })\r\n\r\n          this.selectedFile = null\r\n        } else {\r\n          console.log('[Chat] üí¨ ÂèëÈÄÅÊñáÊú¨:', msg)\r\n          this.safeSend({\r\n            type: 'text',\r\n            text: msg,\r\n            context\r\n          })\r\n        }\r\n      } catch (err) {\r\n        console.error('[Chat] ‚ùå Ê∂àÊÅØÂèëÈÄÅÂ§±Ë¥•:', err)\r\n      }\r\n    },\r\n\r\n    /**\r\n     * ‚úÖ Â∞ÅË£ÖÂÆâÂÖ®ÁöÑ WebSocket ÂèëÈÄÅÊñπÊ≥ïÔºåÈÅøÂÖç TEXT_PARTIAL_WRITING ÈîôËØØ\r\n     */\r\n    safeSend(data) {\r\n      if (this.socket && this.socket.readyState === WebSocket.OPEN) {\r\n        this.socket.send(typeof data === 'string' ? data : JSON.stringify(data))\r\n      } else {\r\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÊú™ÂèëÈÄÅ')\r\n      }\r\n    },\r\n    // Êñá‰ª∂Â§ÑÁêÜ‰∏∫Base64\r\n    fileToBase64(file) {\r\n      return new Promise((resolve, reject) => {\r\n        const reader = new FileReader()\r\n        reader.readAsDataURL(file)\r\n        reader.onload = () => resolve(reader.result)\r\n        reader.onerror = reject\r\n      })\r\n    },\r\n    queueTextForTyping(text) {\r\n      const chars = Array.from(text)\r\n      this.typingQueue.push(...chars)\r\n      if (!this.isTyping) {\r\n        this.startTyping()\r\n      }\r\n    },\r\n    startTyping() {\r\n      this.isTyping = true\r\n\r\n      let last = this.chatMessages.find((msg) => msg.id === this.currentAiMessageId)\r\n      if (!last) {\r\n        last = { id: this.generateId(), role: 'ai', content: '', renderedContent: '' }\r\n        this.chatMessages.push(last)\r\n        this.currentAiMessageId = last.id\r\n      }\r\n\r\n      const typeNext = () => {\r\n        if (this.typingQueue.length === 0) {\r\n          this.isTyping = false\r\n          return\r\n        }\r\n\r\n        const nextChar = this.typingQueue.shift()\r\n        last.content += nextChar\r\n        last.renderedContent = marked.parse(last.content)\r\n        this.$forceUpdate()\r\n        this.scrollToBottom()\r\n\r\n        this.$nextTick(() => {\r\n          hljs.highlightAll() // ËØ≠Ê≥ïÈ´ò‰∫ÆÈáçÊñ∞ÊâßË°å\r\n        })\r\n\r\n        this.typingTimer = setTimeout(typeNext, 20)\r\n      }\r\n\r\n      typeNext()\r\n    },\r\n    scrollToBottom() {\r\n      this.$nextTick(() => {\r\n        const el = this.$refs.chatList\r\n        if (el) el.scrollTop = el.scrollHeight\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Ê∑ªÂä†ÈôÑ‰ª∂ */\r\n.attach-btn {\r\n  background: transparent;\r\n  border: none;\r\n  font-size: 20px;\r\n  cursor: pointer;\r\n  padding: 0 8px;\r\n}\r\n\r\n.file-preview {\r\n  display: flex;\r\n  align-items: center;\r\n  font-size: 14px;\r\n  background: #f4f6f8;\r\n  padding: 6px 10px;\r\n  border-radius: 6px;\r\n  margin: 6px 0 0 0;\r\n}\r\n\r\n.file-preview .remove-file {\r\n  background: transparent;\r\n  border: none;\r\n  margin-left: 8px;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n}\r\n\r\n/* ======================== */\r\npre code::before {\r\n  content: attr(class);\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  font-size: 12px;\r\n  color: #999;\r\n  background: #eee;\r\n  padding: 2px 8px;\r\n  border-radius: 0 0 0 10px;\r\n}\r\n\r\n/* ‰ª£Á†ÅÂùóÊ†∑ÂºèÔºåÊ∑°Á¥´Ëâ≤ËÉåÊôØ+ÂúÜËßí+Èò¥ÂΩ±+ËæπÊ°Ü */\r\n.msg-content pre {\r\n  background: #f6f7fb;\r\n  /* Ê∑°Á¥´ÁÅ∞Ëâ≤ËÉåÊôØ */\r\n  border-radius: 12px;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.06);\r\n  border: 1px solid #ececec;\r\n  margin: 18px 0;\r\n  position: relative;\r\n  overflow: auto;\r\n  padding: 0;\r\n}\r\n\r\n.msg-content pre code {\r\n  background: transparent;\r\n  padding: 18px;\r\n  font-size: 15px;\r\n  color: #2d2d2d;\r\n  /* Á®çÊ∑±Â≠ó‰ΩìÔºåÂíåÊ≠£ÊñáÂå∫ÂàÜ */\r\n  font-family: 'Fira Mono', 'Consolas', 'Menlo', monospace;\r\n  display: block;\r\n  white-space: pre;\r\n  line-height: 1.7;\r\n}\r\n\r\n/* ‰ª£Á†ÅÂùóÂ∑¶‰∏äËßíÊ†áÁ≠æÊ†∑Âºè */\r\n.msg-content pre code::before {\r\n  content: attr(class);\r\n  position: absolute;\r\n  top: 8px;\r\n  right: 16px;\r\n  font-size: 12px;\r\n  co