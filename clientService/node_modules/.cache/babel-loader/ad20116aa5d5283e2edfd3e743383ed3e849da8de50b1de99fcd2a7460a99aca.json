xtarea = this.$refs.textarea;\n      if (textarea) {\n        textarea.style.height = 'auto';\n        const maxHeight = 120;\n        textarea.style.height = Math.min(textarea.scrollHeight, maxHeight) + 'px';\n      }\n    },\n    scrollToBottom() {\n      this.$nextTick(() => {\n        const list = this.$refs.messageList;\n        if (list) list.scrollTop = list.scrollHeight;\n      });\n    },\n    onFocus() {\n      this.scrollToBottom();\n    }\n  }\n};","map":{"version":3,"names":["data","ws","messages","from","text","inputText","socketConnected","typingBuffer","isTyping","mounted","initWebSocket","beforeDestroy","close","methods","wsUrl","WebSocket","onopen","console","log","onmessage","event","JSON","parse","reply","message","push","scrollToBottom","onerror","err","error","onclose","warn","handleEnter","e","shiftKey","sendMessage","trim","autoResize","payload","cmd","send","stringify","textarea","$refs","style","height","maxHeight","Math","min","scrollHeight","$nextTick","list","messageList","scrollTop","onFocus"],"sources":["src/views/ChatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-container\">\r\n    <!-- ËÅäÂ§©Ê∂àÊÅØÂ±ïÁ§∫Âå∫ -->\r\n    <div class=\"chat-messages\" ref=\"messageList\">\r\n      <div\r\n        v-for=\"(msg, index) in messages\"\r\n        :key=\"index\"\r\n        :class=\"['chat-bubble', msg.from]\"\r\n      >\r\n        <div class=\"avatar\" v-if=\"msg.from === 'bot'\">ü§ñ</div>\r\n        <div class=\"bubble-content\">{{ msg.text }}</div>\r\n        <div class=\"avatar\" v-if=\"msg.from === 'user'\">üßë</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ËæìÂÖ•Âå∫Âüü -->\r\n    <div class=\"chat-input\">\r\n      <textarea\r\n        v-model=\"inputText\"\r\n        ref=\"textarea\"\r\n        @focus=\"onFocus\"\r\n        @input=\"autoResize\"\r\n        @keydown.enter.prevent=\"handleEnter\"\r\n        placeholder=\"ÂèëÈÄÅ‰∏ÄÊù°Ê∂àÊÅØ...\"\r\n        rows=\"1\"\r\n      ></textarea>\r\n    </div>\r\n  </div>\r\