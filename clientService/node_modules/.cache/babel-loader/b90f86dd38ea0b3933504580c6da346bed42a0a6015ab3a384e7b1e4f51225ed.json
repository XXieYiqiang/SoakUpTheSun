r\n                    lastUpdate: new Date(Date.now() - 60 * 60 * 1000), // 1Â∞èÊó∂Ââç\r\n                    items: [\r\n                        // \"Áü≠ÂâßÊí≠ÊîæÈáèÁ™ÅÁ†¥2000‰∫øÊ¨°\", \"DIYÂª∫ÊàøÊåëÊàòËµõÁàÜÁÅ´\", \"Âπ¥Â∫¶ÊúÄÁÅ´BGMÂêàÈõÜ\",\r\n                        // \"ÊäñÈü≥ÁîµÂïÜÊàê‰∫§È¢ùÂàõÊñ∞È´ò\", \"ÁΩëÂèãËá™Âà∂AIÁü≠ÁâáÁôªÁÉ≠Ê¶ú\", \"ÊóÖË°åvlogÊúÄÂèóÊ¨¢ËøéÊ¶úÂçï\",\r\n                        // \"Áôæ‰∏áÁ≤â‰∏ùÂàõ‰ΩúËÄÖËÆøË∞à\", \"ÊåëÊàò24Â∞èÊó∂Êó†ÊâãÊú∫ÁîüÊ¥ª\", \"AIÂèòËÑ∏ÊäÄÊúØÂºïÂèëÁÉ≠ËÆÆ\", \"ÂÆ†Áâ©Áü≠ËßÜÈ¢ë‰∫∫Ê∞îÊ¶úTOP10\"\r\n                    ],\r\n                    loading: false\r\n                },\r\n                {\r\n                    title: \"Êñ∞Êµ™ÂæÆÂçö\",\r\n                    tag: \"ÁÉ≠Êêú\",\r\n                    icon: \"/upload/xlwb.png\",\r\n                    lastUpdate: new Date(Date.now() - 3 * 60 * 60 * 1000), // 3Â∞èÊó∂Ââç\r\n                    items: [\r\n                        // \"2025Âπ¥È´òËÄÉÊä•Âêç‰∫∫Êï∞ÂÜçÂàõÊñ∞È´ò\", \"ÂØíÊΩÆÊù•Ë¢≠ÂÖ®ÂõΩÊ∞îÊ∏©È™§Èôç\", \"ÂàòÂæ∑ÂçéÊºîÂî±‰ºöÂÖ®Á®ãÂõûÊîæ\",\r\n                        // \"Êñ∞ËÉΩÊ∫êËΩ¶ÈîÄÈáèÁ™ÅÁ†¥Êñ∞Á∫™ÂΩï\", \"‰∏≠ÁæéÁßëÊäÄ‰ºÅ‰∏öÂêà‰ΩúÊ∂àÊÅØ\", \"Âπ¥Â∫¶ÂçÅÂ§ßÁÉ≠ÊêúÊ¶úÂçïÂèëÂ∏É\",\r\n                        // \"‰∏ñÁïåÊùØÈ¢ÑÈÄâËµõÊúÄÊñ∞ÊàòÂÜµ\", \"AI ÊêúÁ¥¢ÂäüËÉΩ‰∏äÁ∫øÂÖ¨Êµã\", \"ÊµÅÊÑüÁñ´ËãóÊé•ÁßçÊèêÈÜí\", \"Êò•ËøêË¥≠Á•®Á≥ªÁªüÂçáÁ∫ßÂÆåÊàê\"\r\n                    ],\r\n                    loading: false\r\n                },\r\n                {\r\n                    title: \"ÊæéÊπÉÊñ∞Èóª\",\r\n                    tag: \"ÁÉ≠Ê¶ú\",\r\n                    icon: \"/upload/pp.png\",\r\n                    lastUpdate: new Date(Date.now() - 2 * 60 * 60 * 1000),\r\n                    items: [\r\n                        // \"AIÂàõ‰∏öÂÖ¨Âè∏ËûçËµÑÊΩÆÂÜçËµ∑\", \"Â≠óËäÇË∑≥Âä®Êñ∞‰∏öÂä°ÊõùÂÖâ\", \"SaaSË°å‰∏öËøéÊù•Ê¥óÁâåÊúü\",\r\n                        // \"OpenAI ÊúÄÊñ∞Âä®ÊÄÅÊ±áÊÄª\", \"Ê∂àË¥πÁßëÊäÄÊäïËûçËµÑÂë®Êä•\", \"ÁîµÂïÜÊñ∞Ë∂ãÂäøÔºöÂÖ¥Ë∂£Êé®Ëçê\",\r\n                        // \"Âàõ‰∏öËÄÖËØ•Â¶Ç‰ΩïÊäì‰ΩèAIÁ∫¢Âà©\", \"ÂõΩÂÜÖÂàõÊäïÂ∏ÇÂú∫ÂõûÊöñ‰ø°Âè∑\", \"Web3ÁîüÊÄÅÂ∏ÉÂ±ÄËøõÂ±ï\", \"Ëá™Âä®È©æÈ©∂Ë°å‰∏öËßÇÂØü\"\r\n                    ],\r\n                    loading: false\r\n                },\r\n                {\r\n                    title: \"ÁΩëÊòìÊñ∞Èóª\",\r\n                    tag: \"ÁÉ≠Ê¶ú\",\r\n                    icon: \"/upload/wy.png\",\r\n                    lastUpdate: new Date(Date.now() - 30 * 60 * 1000),\r\n                    items: [\r\n                        // \"Êú¨Âë®ÂÄºÂæóÂÖ≥Ê≥®ÁöÑÊïàÁéáÂ∫îÁî®\", \"Android 15 Beta ‰ΩìÈ™åÊä•Âëä\", \"macOS 15 ÂÆûÁî®ÊäÄÂ∑ßÂêàÈõÜ\",\r\n                        // \"App Store Âπ¥Â∫¶Ê¶úÂçïÈ¢ÑÊµã\", \"Notion Ê®°ÊùøÂàÜ‰∫´\", \"Êï∞Â≠óÁîüÊ¥ªË∂ãÂäøËßÇÂØü\",\r\n                        // \"RSS Â∑•ÂÖ∑Êé®ËçêÂêàÈõÜ\", \"Â¶Ç‰ΩïÊâìÈÄ†È´òÊïàÂ∑•‰ΩúÁéØÂ¢É\", \"Â∞ëÊï∞Ê¥æÁºñËæëÁ≤æÈÄâÊñáÁ´†\", \"ËÆæËÆ°Â∏àÂøÖÂ§áÂ∞èÂ∑•ÂÖ∑Ê∏ÖÂçï\"\r\n                    ],\r\n                    loading: false\r\n                },\r\n                {\r\n                    title: \"CSDN\",\r\n                    tag: \"ÁÉ≠Ê¶ú\",\r\n                    icon: \"/upload/csdn.png\",\r\n                    lastUpdate: new Date(Date.now() - 10 * 60 * 1000),\r\n                    items: [\r\n                        // \"Â∞èÁ±≥15Á≥ªÂàóÊ≠£ÂºèÂèëÂ∏É\", \"ËãπÊûúM4ËäØÁâáÊÄßËÉΩÊõùÂÖâ\", \"Redmi K70 ÂºÄÂîÆÂç≥ÂîÆÁΩÑ\",\r\n                        // \"WiFi 7 ËÆæÂ§áÂÖ®Èù¢ÊôÆÂèä\", \"Âçé‰∏∫Êñ∞Á≥ªÁªüBetaÊµãËØï\", \"Ëç£ËÄÄÂèëÂ∏ÉÂÖ®Êñ∞Âπ≥ÊùøÁ≥ªÂàó\",\r\n                        // \"Ëã±‰ºüËææAIÊòæÂç°‰æõ‰∏çÂ∫îÊ±Ç\", \"‰∏âÊòüS25ËÆæËÆ°ÊõùÂÖâ\", \"AIÊâãÊú∫ÊãçÁÖßÂäüËÉΩÂØπÊØî\", \"ÂõΩÂÜÖËäØÁâáÈáè‰∫ßËøõÂ±ï\"\r\n                    ],\r\n                    loading: false\r\n                }\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n        getData() {\r\n            // È°µÈù¢ÂàùÂßãÂä†ËΩΩÊó∂ÔºåÂÖ®ÈÉ®Âç°ÁâáÈÉΩÊòæÁ§∫ loading\r\n            this.boardList.forEach(board => board.loading = true);\r\n\r\n            let param = {}\r\n            getNewsTop10(param).then(res => {\r\n                if (res && res.data.code === 200 && res.data.data) {\r\n                    const dataMap = {\r\n                        0: 'XINLANGB',\r\n                        1: 'DOUYIN',\r\n                        2: 'XINLANGNEWS',\r\n                        3: 'PENGPAI',\r\n                        4: 'WANGYIXINWEN',\r\n                        5: 'CSDN'\r\n                    }\r\n\r\n                    // ÈÅçÂéÜÊØè‰∏™ÊùøÂùóÔºåËµãÂÄºÊï∞ÊçÆÂπ∂ÂÖ≥Èó≠ loading\r\n                    Object.keys(dataMap).forEach(index => {\r\n                        const i = Number(index)\r\n                        this.boardList[i].items = res.data.data[dataMap[i]] || []\r\n                        this.boardList[i].loading = false\r\n                        this.boardList[i].lastUpdate = new Date()\r\n                    })\r\n                } else {\r\n                    this.$message.error(res.data.message)\r\n                    // Â¶ÇÊûúÊé•Âè£Â§±Ë¥•Ôºå‰πüÂÖ≥Èó≠ loading ÈÅøÂÖç‰∏ÄÁõ¥ÊòæÁ§∫\r\n                    this.boardList.forEach(board => board.loading = false)\r\n                }\r\n            }).catch(err => {\r\n                console.error(err)\r\n                this.$message.error('Êï∞ÊçÆÂä†ËΩΩÂ§±Ë¥•')\r\n                this.boardList.forEach(board => board.loading = false)\r\n            })\r\n        },\r\n        toNewsDetails() {\r\n            this.$router.push({\r\n                name: 'newDetails'\r\n            })\r\n        },\r\n        getRelativeTime(time) {\r\n            const diff = (Date.now() - new Date(time)) / 1000\r\n            if (diff < 60) return \"ÂàöÂàöÊõ¥Êñ∞\"\r\n            if (diff < 3600) return Math.floor(diff / 60) + \"ÂàÜÈíüÂâçÊõ¥Êñ∞\"\r\n            if (diff < 86400) return Math.floor(diff / 3600) + \"Â∞èÊó∂ÂâçÊõ¥Êñ∞\"\r\n            return Math.floor(diff / 86400) + \"Â§©ÂâçÊõ¥Êñ∞\"\r\n        },\r\n        getRelativeTime(time) {\r\n            const diff = (Date.now() - new Date(time)) / 1000\r\n            if (diff < 60) return \"ÂàöÂàöÊõ¥Êñ∞\"\r\n            if (diff < 3600) return Math.floor(diff / 60) + \"ÂàÜÈíüÂâçÊõ¥Êñ∞\"\r\n            if (diff < 86400) return Math.floor(diff / 3600) + \"Â∞èÊó∂ÂâçÊõ¥Êñ∞\"\r\n            return Math.floor(diff / 86400) + \"Â§©ÂâçÊõ¥Êñ∞\"\r\n        },\r\n        async refreshBoard(index) {\r\n\r\n            // ËÆæÁΩÆÂç°Áâá loading\r\n            this.boardList[index].loading = true;\r\n\r\n            try {\r\n                const res = await getNewsTop10({}); // ‰Ω†Ëá™Â∑±ÁöÑÊé•Âè£\r\n                if (res && res.data.code === 200 && res.data.data) {\r\n                    // Ê†πÊçÆ index Êõ¥Êñ∞ÂØπÂ∫îÊùøÂùó\r\n                    switch (index) {\r\n                        case 0: this.boardList[0].items = res.data.data.XINLANGB || []; break;\r\n                        case 1: this.boardList[1].items = res.data.data.DOUYIN || []; break;\r\n                        case 2: this.boardList[2].items = res.data.data.XINLANGNEWS || []; break;\r\n                        case 3: this.boardList[3].items = res.data.data.PENGPAI || []; break;\r\n                        case 4: this.boardList[4].items = res.data.data.WANGYIXINWEN || []; break;\r\n                        case 5: this.boardList[5].items = res.data.data.CSDN || []; break;\r\n                    }\r\n                    this.boardList[index].lastUpdate = new Date();\r\n                } else {\r\n                    this.$message.error(res.data.message)\r\n                }\r\n            } catch (error) {\r\n                console.error(error);\r\n                this.$message.error('Âà∑Êñ∞Â§±Ë¥•')\r\n            } finally {\r\n                // ÂÖ≥Èó≠ loading\r\n                this.boardList[index].loading = false;\r\n            }\r\n        },\r\n        updateTime() {\r\n            const now = new Date()\r\n            const pad = n => n.toString().padStart(2, '0')\r\n            const week = ['ÊòüÊúüÊó•', 'ÊòüÊúü‰∏Ä', 'ÊòüÊúü‰∫å', 'Êòü