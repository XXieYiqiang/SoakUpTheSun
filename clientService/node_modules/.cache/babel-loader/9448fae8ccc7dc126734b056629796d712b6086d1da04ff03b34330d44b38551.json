{"ast":null,"code":"\"use strict\";\n\nvar _interopRequireDefault = require(\"C:/Users/Administrator/Desktop/GimChat/gimChatVue/node_modules/@babel/runtime/helpers/interopRequireDefault.js\").default;\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\nrequire(\"core-js/modules/es.array.push.js\");\nrequire(\"core-js/modules/es.iterator.constructor.js\");\nrequire(\"core-js/modules/es.iterator.find.js\");\nrequire(\"core-js/modules/es.iterator.for-each.js\");\nrequire(\"core-js/modules/es.iterator.map.js\");\nvar _marked = require(\"marked\");\nvar _file = require(\"@/api/file\");\nvar _vue = require(\"vue\");\nvar _highlight = _interopRequireDefault(require(\"highlight.js\"));\nrequire(\"highlight.js/styles/github.css\");\n// ‰Ω†ÂèØ‰ª•Êç¢ÊàêÂÖ∂‰ªñ‰∏ªÈ¢òÊ†∑Âºè\n\n_marked.marked.setOptions({\n  breaks: true,\n  gfm: true,\n  highlight(code, lang) {\n    return lang && _highlight.default.getLanguage(lang) ? _highlight.default.highlight(code, {\n      language: lang\n    }).value : _highlight.default.highlightAuto(code).value;\n  }\n});\nvar _default = exports.default = {\n  data() {\n    return {\n      selectedModel: 'gpt-4.1',\n      //ÊâÄÈÄâÊ®°Âûã\n      selectedModelOptions: [{\n        label: 'üöÄ GPT-4.1ÔºàÊé®ËçêÊñáÊú¨Ê®°ÂûãÔºâ',\n        value: 'gpt-4.1'\n      }, {\n        label: 'üåå GPT-4.1-2025-04-14ÔºàÊúÄÊñ∞ÁâàÔºâ',\n        value: 'gpt-4.1-2025-04-14'\n      }, {\n        label: 'üîÆ GPT-4oÔºàÂ§öÊ®°ÊÄÅÊóóËà∞Ôºâ',\n        value: 'gpt-4o'\n      }, {\n        label: 'üß† O3ÔºàÊé®ÁêÜ‰ºòÂåñÁâàÔºâ',\n        value: 'o3'\n      }, {\n        label: 'üìò GPT-4ÔºàÊóßÁâàÔºâ',\n        value: 'gpt-4'\n      }, {\n        label: 'üí∏ GPT-3.5 TurboÔºà‰ΩéÊàêÊú¨Ôºâ',\n        value: 'gpt-3.5-turbo'\n      }],\n      selectedKB: '',\n      selectedKBOptions: [{\n        label: 'üìö Êú™‰ΩøÁî®Áü•ËØÜÂ∫ì',\n        value: 'üìö Êú™‰ΩøÁî®Áü•ËØÜÂ∫ì'\n      }, {\n        label: 'üìò ‰∫ßÂìÅÊñáÊ°£Â∫ì',\n        value: 'üìò ‰∫ßÂìÅÊñáÊ°£Â∫ì'\n      }, {\n        label: 'üìí FAQ Áü•ËØÜÂ∫ì',\n        value: 'üìí FAQ Áü•ËØÜÂ∫ì'\n      }, {\n        label: 'üìô ÂÜÖÈÉ®ËµÑÊñôÂ∫ì',\n        value: 'üìô ÂÜÖÈÉ®ËµÑÊñôÂ∫ì'\n      }],\n      //ÊâÄÈÄâÁü•ËØÜÂ∫ì\n      socketStatus: 'connecting',\n      // ÂàùÂßãÁä∂ÊÄÅ\n      socket: null,\n      reconnectAttempts: 0,\n      // ÂΩìÂâçÈáçËØïÊ¨°Êï∞\n      maxReconnectAttempts: 20,\n      // ÊúÄÂ§ßÈáçËØïÊ¨°Êï∞\n      reconnectDelay: 2000,\n      // ÂàùÂßãÈáçËøûÂª∂Ëøü(ms)\n      reconnectTimer: null,\n      selectedFiles: [],\n      // ÊîπÊàêÊï∞ÁªÑÊîØÊåÅÂ§öÊñá‰ª∂\n\n      selectedFile: null,\n      // Êñ∞Â¢ûÔºöÂ∑≤ÈÄâÊñá‰ª∂\n      chatMessages: [{\n        id: Date.now(),\n        role: 'ai',\n        content: 'ÂòøÔºÅÊàëÊòØ **AI Â∞èÂä©Êâã**ÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû',\n        renderedContent: ''\n      }],\n      userInput: '',\n      loading: false,\n      currentAiMessageId: null,\n      typingQueue: [],\n      typingTimer: null,\n      isTyping: false\n    };\n  },\n  mounted() {\n    // ÂàùÂßãÂåñ Markdown Ê∏≤Êüì\n    this.chatMessages.forEach(msg => {\n      msg.renderedContent = _marked.marked.parse(msg.content);\n    });\n    this.initSocket();\n    this.adjustInputHeight(); // ÂàùÂßãÂåñ‰∏ÄÊ¨°\n  },\n  watch: {\n    userInput() {\n      this.adjustInputHeight();\n    }\n  },\n  beforeDestroy() {\n    if (this.typingTimer) {\n      clearTimeout(this.typingTimer);\n    }\n    if (this.reconnectTimer) {\n      clearTimeout(this.reconnectTimer);\n    }\n    if (this.socket) {\n      this.socket.close();\n    }\n  },\n  methods: {\n    adjustInputHeight() {\n      (0, _vue.nextTick)(() => {\n        const el = this.$refs.inputBox;\n        if (!el) return;\n        el.style.height = 'auto'; // ÈáçÁΩÆÈ´òÂ∫¶\n        const lineHeight = parseInt(getComputedStyle(el).lineHeight) || 24;\n        const maxLines = 3;\n        const maxHeight = lineHeight * maxLines + 20; // 20px padding È¢ÑÁïô\n        el.style.height = Math.min(el.scrollHeight, maxHeight) + 'px';\n      });\n    },\n    /**\r\n     * Â§çÂà∂Á≤òË¥¥Êñá‰ª∂‰∏ä‰º†\r\n     */\n    handlePaste(e) {\n      const clipboardData = e.clipboardData;\n      if (!clipboardData) return;\n      const files = [];\n      for (let item of clipboardData.items) {\n        if (item.kind === 'file') {\n          const file = item.getAsFile();\n          if (file) files.push(file);\n        }\n      }\n      if (files.length > 0) {\n        // ËøΩÂä†Êñá‰ª∂ÔºåÈÅøÂÖçË¶ÜÁõñ\n        this.selectedFiles.push(...files);\n        console.log(\"Á≤òË¥¥Ê∑ªÂä†Êñá‰ª∂Ôºö\");\n        console.log(this.selectedFiles);\n        e.preventDefault(); // ÈòªÊ≠¢Á≤òË¥¥Âà∞ËæìÂÖ•Ê°Ü\n      }\n    },\n    /**\r\n     * ÊâãÂä®ÈáçËøû\r\n     */\n    manualReconnect() {\n      console.log('[WebSocket] Áî®Êà∑ÊâãÂä®Ëß¶ÂèëÈáçËøû');\n      // Ê∏ÖÁêÜÂ∑≤ÊúâËá™Âä®ÈáçËøûÂÆöÊó∂Âô®ÔºåÈÅøÂÖçÂÜ≤Á™Å\n      if (this.reconnectTimer) {\n        clearTimeout(this.reconnectTimer);\n        this.reconnectTimer = null;\n      }\n      this.reconnectAttempts = 0; // ÈáçÁΩÆÈáçËØïËÆ°Êï∞\n      this.initSocket(); // Áõ¥Êé•ÈáçÊñ∞ËøûÊé•\n    },\n    // Â§ÑÁêÜÊñá‰ª∂ÈÄâÊã©ÔºàÊîØÊåÅÂ§öÈÄâÔºâ\n    handleFileSelect(e) {\n      const files = Array.from(e.target.files);\n      if (!files.length) return;\n      // ËøΩÂä†Êñá‰ª∂ÔºåÈÅøÂÖçË¶ÜÁõñ\n      this.selectedFiles.push(...files);\n      console.log(\"Â∑≤ÈÄâÊã©Êñá‰ª∂Ôºö\");\n      console.log(this.selectedFiles);\n      // Ê∏ÖÁ©∫ input ‰ª•‰æøÂêåÂêçÊñá‰ª∂‰πüËÉΩË¢´ÈÄâ‰∏≠\n      e.target.value = null;\n    },\n    // Âà†Èô§ÈôÑ‰ª∂\n    removeFile(index) {\n      this.selectedFiles.splice(index, 1);\n    },\n    /**\r\n     * Ê∂àÊÅØId ÁîüÊàê\r\n     */\n    generateId() {\n      return Date.now() + Math.random();\n    },\n    /**\r\n     * ÂàùÂßãÂåñ Web Socket ÊúçÂä°\r\n     */\n    initSocket() {\n      if (this.socket) {\n        this.socket.close();\n        this.socket = null;\n      }\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws');\n      this.socketStatus = 'connecting';\n      this.socket.addEventListener('open', () => {\n        console.log('WebSocket Â∑≤ËøûÊé•');\n        this.socketStatus = 'open';\n        this.reconnectAttempts = 0; // ÊàêÂäüËøûÊé•ÂêéÈáçÁΩÆËÆ°Êï∞\n        if (this.reconnectTimer) {\n          clearTimeout(this.reconnectTimer);\n          this.reconnectTimer = null;\n        }\n      });\n      this.socket.addEventListener('message', event => {\n        if (event.data === '[DONE]') {\n          this.loading = false;\n          this.currentAiMessageId = null;\n          return;\n        }\n        this.queueTextForTyping(event.data);\n      });\n      this.socket.addEventListener('close', () => {\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠');\n        this.socketStatus = 'closed';\n        this.tryReconnect();\n      });\n      this.socket.addEventListener('error', error => {\n        console.error('WebSocket ÈîôËØØ:', error);\n        this.socketStatus = 'error';\n        this.tryReconnect();\n      });\n    },\n    /** WebSocket ÈáçËøû ÈÄªËæë */\n    tryReconnect() {\n      if (this.reconnectAttempts >= this.maxReconnectAttempts) {\n        console.warn('[WebSocket] Â∑≤ËææÂà∞ÊúÄÂ§ßÈáçËøûÊ¨°Êï∞Ôºå‰∏çÂÜçÈáçËØï');\n        return;\n      }\n      if (this.reconnectTimer) return; // Â∑≤ÊúâÈáçËøûËÆ°ÂàíÔºåÈÅøÂÖçÈáçÂ§ç\n\n      const delay = this.reconnectDelay * (this.reconnectAttempts + 1); // ÁÆÄÂçïÊåáÊï∞ÈÄÄÈÅø\n      console.log(`[WebSocket] ${delay}ms ÂêéÂ∞ùËØïÁ¨¨ ${this.reconnectAttempts + 1} Ê¨°ÈáçËøû`);\n      this.reconnectTimer = setTimeout(() => {\n        console.log('[WebSocket] ÂºÄÂßãÈáçËøû...');\n        this.reconnectAttempts++;\n        this.initSocket();\n      }, delay);\n    },\n    // Â§ÑÁêÜÊ∂àÊÅØÂèëÈÄÅ\n    async handleSend() {\n      const msg = this.userInput.trim();\n\n      // ‚úÖ Ê†°È™åËæìÂÖ•Âíå WebSocket Áä∂ÊÄÅ\n      if (!msg && this.selectedFiles.length === 0) return;\n      if (!this.socket || this.socket.readyState !== WebSocket.OPEN) {\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÂèëÈÄÅÂ§±Ë¥•');\n        return;\n      }\n      let contentWithFiles = msg;\n      if (this.selectedFiles.length) {\n        const names = this.selectedFiles.map(f => f.name).join(', ');\n        contentWithFiles += `\\n\\nüìé ÈôÑ‰ª∂: ${names}`;\n      }\n      this.chatMessages.push({\n        id: this.generateId(),\n        role: 'user',\n        content: _marked.marked.parse(contentWithFiles),\n        renderedContent: _marked.marked.parse(contentWithFiles) // ‚úÖ ÈôÑ‰ª∂Âêç‰πü‰ºöÊîØÊåÅ Markdown Ê∏≤Êüì\n      });\n\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°ÜÔºåÂºÄÂêØÂä†ËΩΩ\n      this.userInput = '';\n      this.loading = true;\n      this.scrollToBottom();\n\n      // 2. ÁªÑË£Ö‰∏ä‰∏ãÊñáÔºàÊñπ‰æøÂêéÁ´Ø‰øùÊåÅ‰ºöËØùÔºâ\n      const context = this.chatMessages.map(m => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content).join('\\n');\n      try {\n        // 3. Âà§Êñ≠ÊòØÂèëÈÄÅÈôÑ‰ª∂ËøòÊòØÁ∫ØÊñáÊú¨\n        if (this.selectedFiles.length) {\n          console.log('[Chat] üìé Ê≠£Âú®‰∏ä‰º†ÈôÑ‰ª∂:');\n          const fileUrlList = [];\n          for (let i = 0; i < this.selectedFiles.length; i++) {\n            const resp = await this.uploadFile(this.selectedFiles[i]);\n            console.log('[Chat] üìé ‰∏ä‰º†ÈôÑ‰ª∂ÊàêÂäü:', resp);\n            for (let j = 0; j < resp.length; j++) {\n              fileUrlList.push(resp[j]);\n            }\n          }\n          console.log('fileUrl');\n          console.log(fileUrlList);\n          this.safeSend({\n            type: 'file',\n            name: 'ÈôÑ‰ª∂',\n            modelName: this.selectedModel,\n            data: fileUrlList,\n            text: msg || '',\n            context\n          });\n          this.selectedFiles = [];\n        } else {\n          console.log('[Chat] üí¨ ÂèëÈÄÅÊñáÊú¨:', msg);\n          this.safeSend({\n            type: 'text',\n            modelName: this.selectedModel,\n            text: msg,\n            context\n          });\n        }\n      } catch (err) {\n        console.error('[Chat] ‚ùå Ê∂àÊÅØÂèëÈÄÅÂ§±Ë¥•:', err);\n      }\n    },\n    /**\r\n     * ‚úÖ Â∞ÅË£ÖÂÆâÂÖ®ÁöÑ WebSocket ÂèëÈÄÅÊñπÊ≥ïÔºåÈÅøÂÖç TEXT_PARTIAL_WRITING ÈîôËØØ\r\n     */\n    safeSend(data) {\n      if (this.socket && this.socket.readyState === WebSocket.OPEN) {\n        this.socket.send(typeof data === 'string' ? data : JSON.stringify(data));\n      } else {\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÊú™ÂèëÈÄÅ');\n      }\n    },\n    // Êñá‰ª∂Â§ÑÁêÜ‰∏∫Base64\n    fileToBase64(file) {\n      return new Promise((resolve, reject) => {\n        const reader = new FileReader();\n        reader.readAsDataURL(file);\n        reader.onload = () => resolve(reader.result);\n        reader.onerror = reject;\n      });\n    },\n    queueTextForTyping(text) {\n      const chars = Array.from(text);\n      this.typingQueue.push(...chars);\n      if (!this.isTyping) {\n        this.startTyping();\n      }\n    },\n    startTyping() {\n      this.isTyping = true;\n      let last = this.chatMessages.find(msg => msg.id === this.currentAiMessageId);\n      if (!last) {\n        last = {\n          id: this.generateId(),\n          role: 'ai',\n          content: '',\n          renderedContent: ''\n        };\n        this.chatMessages.push(last);\n        this.currentAiMessageId = last.id;\n      }\n      const typeNext = () => {\n        if (this.typingQueue.length === 0) {\n          this.isTyping = false;\n          return;\n        }\n        const nextChar = this.typingQueue.shift();\n        last.content += nextChar;\n        last.renderedContent = _marked.marked.parse(last.content);\n        this.$forceUpdate();\n        this.scrollToBottom();\n        this.$nextTick(() => {\n          _highlight.default.highlightAll(); // ËØ≠Ê≥ïÈ´ò‰∫ÆÈáçÊñ∞ÊâßË°å\n        });\n        this.typingTimer = setTimeout(typeNext, 20);\n      };\n      typeNext();\n    },\n    scrollToBottom() {\n      this.$nextTick(() => {\n        const el = this.$refs.chatList;\n        if (el) el.scrollTop = el.scrollHeight;\n      });\n    },\n    /**\r\n     * Êñá‰ª∂‰∏ä‰º†\r\n     */\n    async uploadFile(file) {\n      const formData = new FormData();\n      formData.append('file', file); // ÂÅáËÆæÂêéÁ´ØÂ≠óÊÆµÊòØ filesÔºåÂ§ö‰∏™ÂêåÂêçÂ≠óÊÆµ\n\n      const res = await (0, _file.uploadFile)(formData);\n      return res.data.data || [];\n    }\n  }\n};","map":{"version":3,"names":["_marked","require","_file","_vue","_highlight","_interopRequireDefault","marked","setOptions","breaks","gfm","highlight","code","lang","hljs","getLanguage","language","value","highlightAuto","_default","exports","default","data","selectedModel","selectedModelOptions","label","selectedKB","selectedKBOptions","socketStatus","socket","reconnectAttempts","maxReconnectAttempts","reconnectDelay","reconnectTimer","selectedFiles","selectedFile","chatMessages","id","Date","now","role","content","renderedContent","userInput","loading","currentAiMessageId","typingQueue","typingTimer","isTyping","mounted","forEach","msg","parse","initSocket","adjustInputHeight","watch","beforeDestroy","clearTimeout","close","methods","nextTick","el","$refs","inputBox","style","height","lineHeight","parseInt","getComputedStyle","maxLines","maxHeight","Math","min","scrollHeight","handlePaste","e","clipboardData","files","item","items","kind","file","getAsFile","push","length","console","log","preventDefault","manualReconnect","handleFileSelect","Array","from","target","removeFile","index","splice","generateId","random","WebSocket","addEventListener","event","queueTextForTyping","tryReconnect","error","warn","delay","setTimeout","handleSend","trim","readyState","OPEN","contentWithFiles","names","map","f","name","join","scrollToBottom","context","m","fileUrlList","i","resp","uploadFile","j","safeSend","type","modelName","text","err","send","JSON","stringify","fileToBase64","Promise","resolve","reject","reader","FileReader","readAsDataURL","onload","result","onerror","chars","startTyping","last","find","typeNext","nextChar","shift","$forceUpdate","$nextTick","highlightAll","chatList","scrollTop","formData","FormData","append","res"],"sources":["src/views/AIView.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-container\">\r\n\r\n    <!-- WebSocket Áä∂ÊÄÅÊèêÁ§∫ -->\r\n    <div class=\"socket-status\" :class=\"socketStatus\">\r\n      <span v-if=\"socketStatus === 'connecting'\">üü† ËøûÊé•‰∏≠...</span>\r\n      <span v-else-if=\"socketStatus === 'open'\">üü¢ Â∑≤ËøûÊé•</span>\r\n      <span v-else-if=\"socketStatus === 'closed'\">\r\n        üî¥ Â∑≤Êñ≠ÂºÄ\r\n        <button @click=\"manualReconnect\" class=\"reconnect-btn\">ÈáçËøû</button>\r\n      </span>\r\n      <span v-else-if=\"socketStatus === 'error'\">\r\n        ‚ö†Ô∏è ËøûÊé•Âá∫Èîô\r\n        <button @click=\"manualReconnect\" class=\"reconnect-btn\">ÈáçËøû</button>\r\n      </span>\r\n    </div>\r\n\r\n\r\n    <!-- ËÅäÂ§©Ê∂àÊÅØÂå∫ -->\r\n    <div class=\"msg-list\" ref=\"chatList\">\r\n      <div v-for=\"msg in chatMessages\" :key=\"msg.id\" :class=\"['msg', msg.role === 'user' ? 'msg-user' : 'msg-ai']\">\r\n        <!-- <div class=\"avatar\" v-if=\"msg.role === 'ai'\">üí°</div> -->\r\n        <div class=\"avatar\" v-if=\"msg.role === 'ai'\">\r\n          <img src=\"@/assets/images/avatar/openai.svg\" alt=\"AI Avatar\" class=\"avatar-img\" />\r\n        </div>\r\n        <div class=\"msg-content\" v-html=\"msg.renderedContent\"></div>\r\n        <div class=\"avatar\" v-if=\"msg.role === 'user'\">üßë</div>\r\n      </div>\r\n\r\n      <!-- AI ÊâìÂ≠ó‰∏≠... -->\r\n      <div class=\"ai-loading\" v-if=\"loading\">\r\n        <span style=\"margin-right: 10px;\">AI Ê≠£Âú®ËæìÂÖ•</span>\r\n        <div class=\"dot-flash\">\r\n          <div class=\"dot\" v-for=\"i in 3\" :key=\"i\" :style=\"{ animationDelay: i * 0.2 + 's' }\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <!-- Â∫ïÈÉ®ËæìÂÖ•Ê†è -->\r\n\r\n    <div class=\"input-bar1\" :class=\"{ 'input-bottom': chatMessages.length > 0 }\">\r\n      <!-- ÈôÑ‰ª∂ÂàóË°®Â±ïÁ§∫ÔºåÊîæÂú®ËæìÂÖ•Ê°Ü‰∏äÊñπ -->\r\n      <div v-if=\"selectedFiles.length > 0\" class=\"file-list\">\r\n        <div class=\"file-item\" v-for=\"(file, index) in selectedFiles\" :key=\"file.name + index\">\r\n          <span>{{ file.name }}</span>\r\n          <button class=\"remove-file\" @click=\"removeFile(index)\">‚ùå</button>\r\n        </div>\r\n      </div>\r\n      <div class=\"input-bar\">\r\n\r\n        <!-- ÈôÑ‰ª∂ÊåâÈíÆ -->\r\n        <input ref=\"fileInput\" type=\"file\" accept=\".txt,.pdf,.doc,.docx,image/*\" multiple style=\"display:none\"\r\n          @change=\"handleFileSelect\" />\r\n        <button class=\"attach-btn\" @click=\"$refs.fileInput.click()\">üìé</button>\r\n\r\n        <div class=\"input-wrapper\">\r\n          <textarea ref=\"inputBox\" v-model=\"userInput\" @keyup.enter=\"handleSend\" @paste=\"handlePaste\"\r\n            placeholder=\"ËØ∑ËæìÂÖ•ÈóÆÈ¢ò...\" class=\"input-box\" rows=\"1\"></textarea>\r\n        </div>\r\n\r\n        <!-- Êñ∞Â¢ûÔºöÊ®°ÂûãÈÄâÊã© -->\r\n        <el-select v-model=\"selectedModel\" placeholder=\"Ê®°ÂûãÈÄâÊã©\" style=\"margin-top: 10px;\">\r\n          <el-option v-for=\"item in selectedModelOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n          </el-option>\r\n        </el-select>\r\n        <!-- Êñ∞Â¢ûÔºöÁü•ËØÜÂ∫ìÈÄâÊã© -->\r\n        <el-select v-model=\"selectedKB\" placeholder=\"Áü•ËØÜÂ∫ìÈÄâÊã©\" style=\"margin-top: 10px;\">\r\n          <el-option v-for=\"item in selectedKBOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n          </el-option>\r\n        </el-select>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { marked } from 'marked'\r\nimport { uploadFile } from '@/api/file'\r\nimport { nextTick } from 'vue'\r\nimport hljs from 'highlight.js'\r\nimport 'highlight.js/styles/github.css' // ‰Ω†ÂèØ‰ª•Êç¢ÊàêÂÖ∂‰ªñ‰∏ªÈ¢òÊ†∑Âºè\r\n\r\nmarked.setOptions({\r\n  breaks: true,\r\n  gfm: true,\r\n  highlight(code, lang) {\r\n    return lang && hljs.getLanguage(lang)\r\n      ? hljs.highlight(code, { language: lang }).value\r\n      : hljs.highlightAuto(code).value;\r\n  }\r\n})\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedModel: 'gpt-4.1',//ÊâÄÈÄâÊ®°Âûã\r\n      selectedModelOptions: [\r\n        {\r\n          label: 'üöÄ GPT-4.1ÔºàÊé®ËçêÊñáÊú¨Ê®°ÂûãÔºâ',\r\n          value: 'gpt-4.1'\r\n        },\r\n        {\r\n          label: 'üåå GPT-4.1-2025-04-14ÔºàÊúÄÊñ∞ÁâàÔºâ',\r\n          value: 'gpt-4.1-2025-04-14'\r\n        },\r\n        {\r\n          label: 'üîÆ GPT-4oÔºàÂ§öÊ®°ÊÄÅÊóóËà∞Ôºâ',\r\n          value: 'gpt-4o'\r\n        },\r\n        {\r\n          label: 'üß† O3ÔºàÊé®ÁêÜ‰ºòÂåñÁâàÔºâ',\r\n          value: 'o3'\r\n        },\r\n        {\r\n          label: 'üìò GPT-4ÔºàÊóßÁâàÔºâ',\r\n          value: 'gpt-4'\r\n        },\r\n        {\r\n          label: 'üí∏ GPT-3.5 TurboÔºà‰ΩéÊàêÊú¨Ôºâ',\r\n          value: 'gpt-3.5-turbo'\r\n        }\r\n      ],\r\n      selectedKB: '',\r\n      selectedKBOptions: [\r\n        {\r\n          label: 'üìö Êú™‰ΩøÁî®Áü•ËØÜÂ∫ì',\r\n          value: 'üìö Êú™‰ΩøÁî®Áü•ËØÜÂ∫ì'\r\n        }, {\r\n          label: 'üìò ‰∫ßÂìÅÊñáÊ°£Â∫ì',\r\n          value: 'üìò ‰∫ßÂìÅÊñáÊ°£Â∫ì'\r\n        }, {\r\n          label: 'üìí FAQ Áü•ËØÜÂ∫ì',\r\n          value: 'üìí FAQ Áü•ËØÜÂ∫ì'\r\n        }, {\r\n          label: 'üìô ÂÜÖÈÉ®ËµÑÊñôÂ∫ì',\r\n          value: 'üìô ÂÜÖÈÉ®ËµÑÊñôÂ∫ì'\r\n        }\r\n      ],//ÊâÄÈÄâÁü•ËØÜÂ∫ì\r\n      socketStatus: 'connecting', // ÂàùÂßãÁä∂ÊÄÅ\r\n      socket: null,\r\n      reconnectAttempts: 0,         // ÂΩìÂâçÈáçËØïÊ¨°Êï∞\r\n      maxReconnectAttempts: 20,      // ÊúÄÂ§ßÈáçËØïÊ¨°Êï∞\r\n      reconnectDelay: 2000,         // ÂàùÂßãÈáçËøûÂª∂Ëøü(ms)\r\n      reconnectTimer: null,\r\n      selectedFiles: [],  // ÊîπÊàêÊï∞ÁªÑÊîØÊåÅÂ§öÊñá‰ª∂\r\n\r\n      selectedFile: null, // Êñ∞Â¢ûÔºöÂ∑≤ÈÄâÊñá‰ª∂\r\n      chatMessages: [\r\n        {\r\n          id: Date.now(),\r\n          role: 'ai',\r\n          content: 'ÂòøÔºÅÊàëÊòØ **AI Â∞èÂä©Êâã**ÔºåÊúâ‰ªª‰ΩïÈóÆÈ¢òÈÉΩÂèØ‰ª•ÈóÆÊàëÂì¶ÔΩû',\r\n          renderedContent: '',\r\n        },\r\n      ],\r\n      userInput: '',\r\n      loading: false,\r\n      currentAiMessageId: null,\r\n      typingQueue: [],\r\n      typingTimer: null,\r\n      isTyping: false,\r\n    }\r\n  },\r\n  mounted() {\r\n    // ÂàùÂßãÂåñ Markdown Ê∏≤Êüì\r\n    this.chatMessages.forEach((msg) => {\r\n      msg.renderedContent = marked.parse(msg.content)\r\n    })\r\n    this.initSocket()\r\n\r\n    this.adjustInputHeight() // ÂàùÂßãÂåñ‰∏ÄÊ¨°\r\n  },\r\n  watch: {\r\n    userInput() {\r\n      this.adjustInputHeight()\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    if (this.typingTimer) {\r\n      clearTimeout(this.typingTimer)\r\n    }\r\n    if (this.reconnectTimer) {\r\n      clearTimeout(this.reconnectTimer)\r\n    }\r\n    if (this.socket) {\r\n      this.socket.close()\r\n    }\r\n  },\r\n  methods: {\r\n    adjustInputHeight() {\r\n      nextTick(() => {\r\n        const el = this.$refs.inputBox\r\n        if (!el) return\r\n        el.style.height = 'auto' // ÈáçÁΩÆÈ´òÂ∫¶\r\n        const lineHeight = parseInt(getComputedStyle(el).lineHeight) || 24\r\n        const maxLines = 3\r\n        const maxHeight = lineHeight * maxLines + 20 // 20px padding È¢ÑÁïô\r\n        el.style.height = Math.min(el.scrollHeight, maxHeight) + 'px'\r\n      })\r\n    },\r\n    /**\r\n     * Â§çÂà∂Á≤òË¥¥Êñá‰ª∂‰∏ä‰º†\r\n     */\r\n    handlePaste(e) {\r\n      const clipboardData = e.clipboardData\r\n      if (!clipboardData) return\r\n\r\n      const files = []\r\n      for (let item of clipboardData.items) {\r\n        if (item.kind === 'file') {\r\n          const file = item.getAsFile()\r\n          if (file) files.push(file)\r\n        }\r\n      }\r\n\r\n      if (files.length > 0) {\r\n        // ËøΩÂä†Êñá‰ª∂ÔºåÈÅøÂÖçË¶ÜÁõñ\r\n        this.selectedFiles.push(...files)\r\n        console.log(\"Á≤òË¥¥Ê∑ªÂä†Êñá‰ª∂Ôºö\")\r\n        console.log(this.selectedFiles)\r\n        e.preventDefault() // ÈòªÊ≠¢Á≤òË¥¥Âà∞ËæìÂÖ•Ê°Ü\r\n      }\r\n    },\r\n    /**\r\n     * ÊâãÂä®ÈáçËøû\r\n     */\r\n    manualReconnect() {\r\n      console.log('[WebSocket] Áî®Êà∑ÊâãÂä®Ëß¶ÂèëÈáçËøû')\r\n      // Ê∏ÖÁêÜÂ∑≤ÊúâËá™Âä®ÈáçËøûÂÆöÊó∂Âô®ÔºåÈÅøÂÖçÂÜ≤Á™Å\r\n      if (this.reconnectTimer) {\r\n        clearTimeout(this.reconnectTimer)\r\n        this.reconnectTimer = null\r\n      }\r\n      this.reconnectAttempts = 0 // ÈáçÁΩÆÈáçËØïËÆ°Êï∞\r\n      this.initSocket()          // Áõ¥Êé•ÈáçÊñ∞ËøûÊé•\r\n\r\n    },\r\n    // Â§ÑÁêÜÊñá‰ª∂ÈÄâÊã©ÔºàÊîØÊåÅÂ§öÈÄâÔºâ\r\n    handleFileSelect(e) {\r\n      const files = Array.from(e.target.files)\r\n      if (!files.length) return\r\n      // ËøΩÂä†Êñá‰ª∂ÔºåÈÅøÂÖçË¶ÜÁõñ\r\n      this.selectedFiles.push(...files)\r\n      console.log(\"Â∑≤ÈÄâÊã©Êñá‰ª∂Ôºö\")\r\n      console.log(this.selectedFiles)\r\n      // Ê∏ÖÁ©∫ input ‰ª•‰æøÂêåÂêçÊñá‰ª∂‰πüËÉΩË¢´ÈÄâ‰∏≠\r\n      e.target.value = null\r\n    },\r\n    // Âà†Èô§ÈôÑ‰ª∂\r\n    removeFile(index) {\r\n      this.selectedFiles.splice(index, 1)\r\n    },\r\n    /**\r\n     * Ê∂àÊÅØId ÁîüÊàê\r\n     */\r\n    generateId() {\r\n      return Date.now() + Math.random()\r\n    },\r\n    /**\r\n     * ÂàùÂßãÂåñ Web Socket ÊúçÂä°\r\n     */\r\n    initSocket() {\r\n\r\n      if (this.socket) {\r\n        this.socket.close()\r\n        this.socket = null\r\n      }\r\n\r\n      this.socket = new WebSocket('ws://127.0.0.1:9876/chat-ws')\r\n\r\n      this.socketStatus = 'connecting'\r\n\r\n      this.socket.addEventListener('open', () => {\r\n        console.log('WebSocket Â∑≤ËøûÊé•')\r\n        this.socketStatus = 'open'\r\n        this.reconnectAttempts = 0  // ÊàêÂäüËøûÊé•ÂêéÈáçÁΩÆËÆ°Êï∞\r\n        if (this.reconnectTimer) {\r\n          clearTimeout(this.reconnectTimer)\r\n          this.reconnectTimer = null\r\n        }\r\n      })\r\n\r\n      this.socket.addEventListener('message', (event) => {\r\n        if (event.data === '[DONE]') {\r\n          this.loading = false\r\n          this.currentAiMessageId = null\r\n          return\r\n        }\r\n        this.queueTextForTyping(event.data)\r\n      })\r\n\r\n      this.socket.addEventListener('close', () => {\r\n        console.log('WebSocket Â∑≤ÂÖ≥Èó≠')\r\n        this.socketStatus = 'closed'\r\n        this.tryReconnect()\r\n      })\r\n\r\n      this.socket.addEventListener('error', (error) => {\r\n        console.error('WebSocket ÈîôËØØ:', error)\r\n        this.socketStatus = 'error'\r\n        this.tryReconnect()\r\n      })\r\n    },\r\n    /** WebSocket ÈáçËøû ÈÄªËæë */\r\n    tryReconnect() {\r\n      if (this.reconnectAttempts >= this.maxReconnectAttempts) {\r\n        console.warn('[WebSocket] Â∑≤ËææÂà∞ÊúÄÂ§ßÈáçËøûÊ¨°Êï∞Ôºå‰∏çÂÜçÈáçËØï')\r\n        return\r\n      }\r\n\r\n      if (this.reconnectTimer) return // Â∑≤ÊúâÈáçËøûËÆ°ÂàíÔºåÈÅøÂÖçÈáçÂ§ç\r\n\r\n      const delay = this.reconnectDelay * (this.reconnectAttempts + 1) // ÁÆÄÂçïÊåáÊï∞ÈÄÄÈÅø\r\n      console.log(`[WebSocket] ${delay}ms ÂêéÂ∞ùËØïÁ¨¨ ${this.reconnectAttempts + 1} Ê¨°ÈáçËøû`)\r\n\r\n      this.reconnectTimer = setTimeout(() => {\r\n        console.log('[WebSocket] ÂºÄÂßãÈáçËøû...')\r\n        this.reconnectAttempts++\r\n        this.initSocket()\r\n      }, delay)\r\n    },\r\n    // Â§ÑÁêÜÊ∂àÊÅØÂèëÈÄÅ\r\n    async handleSend() {\r\n      const msg = this.userInput.trim()\r\n\r\n      // ‚úÖ Ê†°È™åËæìÂÖ•Âíå WebSocket Áä∂ÊÄÅ\r\n      if (!msg && this.selectedFiles.length === 0) return\r\n      if (!this.socket || this.socket.readyState !== WebSocket.OPEN) {\r\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÂèëÈÄÅÂ§±Ë¥•')\r\n        return\r\n      }\r\n\r\n      let contentWithFiles = msg\r\n      if (this.selectedFiles.length) {\r\n        const names = this.selectedFiles.map(f => f.name).join(', ')\r\n        contentWithFiles += `\\n\\nüìé ÈôÑ‰ª∂: ${names}`\r\n      }\r\n\r\n      this.chatMessages.push({\r\n        id: this.generateId(),\r\n        role: 'user',\r\n        content: marked.parse(contentWithFiles),\r\n        renderedContent: marked.parse(contentWithFiles), // ‚úÖ ÈôÑ‰ª∂Âêç‰πü‰ºöÊîØÊåÅ Markdown Ê∏≤Êüì\r\n      })\r\n\r\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°ÜÔºåÂºÄÂêØÂä†ËΩΩ\r\n      this.userInput = ''\r\n      this.loading = true\r\n      this.scrollToBottom()\r\n\r\n      // 2. ÁªÑË£Ö‰∏ä‰∏ãÊñáÔºàÊñπ‰æøÂêéÁ´Ø‰øùÊåÅ‰ºöËØùÔºâ\r\n      const context = this.chatMessages\r\n        .map((m) => (m.role === 'user' ? 'Áî®Êà∑Ôºö' : 'AIÔºö') + m.content)\r\n        .join('\\n')\r\n\r\n      try {\r\n        // 3. Âà§Êñ≠ÊòØÂèëÈÄÅÈôÑ‰ª∂ËøòÊòØÁ∫ØÊñáÊú¨\r\n        if (this.selectedFiles.length) {\r\n\r\n          console.log('[Chat] üìé Ê≠£Âú®‰∏ä‰º†ÈôÑ‰ª∂:')\r\n\r\n          const fileUrlList = []\r\n\r\n          for (let i = 0; i < this.selectedFiles.length; i++) {\r\n\r\n            const resp = await this.uploadFile(this.selectedFiles[i])\r\n\r\n            console.log('[Chat] üìé ‰∏ä‰º†ÈôÑ‰ª∂ÊàêÂäü:', resp)\r\n\r\n            for (let j = 0; j < resp.length; j++) {\r\n              fileUrlList.push(resp[j])\r\n            }\r\n\r\n          }\r\n\r\n          console.log('fileUrl')\r\n          console.log(fileUrlList)\r\n\r\n          this.safeSend({\r\n            type: 'file',\r\n            name: 'ÈôÑ‰ª∂',\r\n            modelName: this.selectedModel,\r\n            data: fileUrlList,\r\n            text: msg || '',\r\n            context\r\n          })\r\n\r\n          this.selectedFiles = []\r\n        } else {\r\n          console.log('[Chat] üí¨ ÂèëÈÄÅÊñáÊú¨:', msg)\r\n          this.safeSend({\r\n            type: 'text',\r\n            modelName: this.selectedModel,\r\n            text: msg,\r\n            context\r\n          })\r\n        }\r\n      } catch (err) {\r\n        console.error('[Chat] ‚ùå Ê∂àÊÅØÂèëÈÄÅÂ§±Ë¥•:', err)\r\n      }\r\n    },\r\n\r\n    /**\r\n     * ‚úÖ Â∞ÅË£ÖÂÆâÂÖ®ÁöÑ WebSocket ÂèëÈÄÅÊñπÊ≥ïÔºåÈÅøÂÖç TEXT_PARTIAL_WRITING ÈîôËØØ\r\n     */\r\n    safeSend(data) {\r\n      if (this.socket && this.socket.readyState === WebSocket.OPEN) {\r\n        this.socket.send(typeof data === 'string' ? data : JSON.stringify(data))\r\n      } else {\r\n        console.warn('[Chat] üö´ WebSocket Êú™ËøûÊé•ÔºåÊ∂àÊÅØÊú™ÂèëÈÄÅ')\r\n      }\r\n    },\r\n    // Êñá‰ª∂Â§ÑÁêÜ‰∏∫Base64\r\n    fileToBase64(file) {\r\n      return new Promise((resolve, reject) => {\r\n        const reader = new FileReader()\r\n        reader.readAsDataURL(file)\r\n        reader.onload = () => resolve(reader.result)\r\n        reader.onerror = reject\r\n      })\r\n    },\r\n    queueTextForTyping(text) {\r\n      const chars = Array.from(text)\r\n      this.typingQueue.push(...chars)\r\n      if (!this.isTyping) {\r\n        this.startTyping()\r\n      }\r\n    },\r\n    startTyping() {\r\n      this.isTyping = true\r\n\r\n      let last = this.chatMessages.find((msg) => msg.id === this.currentAiMessageId)\r\n      if (!last) {\r\n        last = { id: this.generateId(), role: 'ai', content: '', renderedContent: '' }\r\n        this.chatMessages.push(last)\r\n        this.currentAiMessageId = last.id\r\n      }\r\n\r\n      const typeNext = () => {\r\n        if (this.typingQueue.length === 0) {\r\n          this.isTyping = false\r\n          return\r\n        }\r\n\r\n        const nextChar = this.typingQueue.shift()\r\n        last.content += nextChar\r\n        last.renderedContent = marked.parse(last.content)\r\n        this.$forceUpdate()\r\n        this.scrollToBottom()\r\n\r\n        this.$nextTick(() => {\r\n          hljs.highlightAll() // ËØ≠Ê≥ïÈ´ò‰∫ÆÈáçÊñ∞ÊâßË°å\r\n        })\r\n\r\n        this.typingTimer = setTimeout(typeNext, 20)\r\n      }\r\n\r\n      typeNext()\r\n    },\r\n    scrollToBottom() {\r\n      this.$nextTick(() => {\r\n        const el = this.$refs.chatList\r\n        if (el) el.scrollTop = el.scrollHeight\r\n      })\r\n    },\r\n    /**\r\n     * Êñá‰ª∂‰∏ä‰º†\r\n     */\r\n    async uploadFile(file) {\r\n      c