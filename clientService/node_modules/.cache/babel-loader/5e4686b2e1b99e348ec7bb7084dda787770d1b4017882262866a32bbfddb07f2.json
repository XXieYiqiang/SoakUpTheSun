ass=\"card-icon\"></i>\r\n    </div>\r\n\r\n    <!-- 卡片内容 -->\r\n    <div class=\"card-content\">\r\n      <div class=\"card-title\">\r\n        <span>{{ card.name }}</span>\r\n        <el-dropdown trigger=\"click\" class=\"dropdown\">\r\n          <i class=\"el-icon-more\"></i>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item>重命名</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"handleDelete\">删除</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n      <p class=\"card-desc\">{{ card.description }}</p>\r\n      <div class=\"card-meta\">\r\n\r\n        <span><i style=\"margin-right: 5px;\" class=\"el-icon-document\" />{{ card.fileCount }} 个文件</span>\r\n        <span>更新于 {{ card.updateTimeStr }}</span>\r\n      </div>\r\n    </div>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport { deleteKnowledge } from '@/api/knowledge'\r\nexport default {\r\n  name: 'KnowledgeCard',\r\n  props: {\r\n    card: {\r\n      type: Object,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n    }\r\n  },\r\n  methods: {\r\n    /**\r\n     * 删除知识库\r\n     */\r\n    handleDelete() {\r\n      this.$confirm('此操作将永久删除该知识库, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        const param = {\r\n          knowledgeCode: this.card.knowledgeCode\r\n        }\r\n        deleteKnowledge(param).then(res => {\r\n          if (res.data.code === 200) {\r\n            this.$message.success('知识库删除成功')\r\n            this.$emit('loadding') // 通知父组件刷新列表\r\n          } else {\r\n            this.$message.error('知识库删除失败，请稍后重试')\r\n          }\r\n        }).catch(err => {\r\n          console.error('删除知识库失败:', err)\r\n          this.$message.error('网络错误，请稍后重试')\r\n        })\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功!'\r\n        });\r\n      }).c