rning('âš ï¸ æœ€å¤šåªèƒ½ä¸Šä¼  1 ä¸ªæ–‡ä»¶');\n    },\n    // å…³é—­ loadingï¼ˆç¡®ä¿ä¸€å®šä¼šè¢«è°ƒç”¨ï¼‰\n    closeLoading() {\n      if (this.loadingInstance) {\n        this.loadingInstance.close();\n        this.loadingInstance = null;\n      }\n    },\n    /**\r\n     * * @param fileType è·å–æ–‡ä»¶ icon å›¾æ ‡\r\n     */\n    getFileIcon(fileType) {\n      switch (fileType) {\n        case 'pdf':\n          return 'el-icon-document';\n        case 'docx':\n          return 'el-icon-document';\n        case 'xlsx':\n          return 'el-icon-s-data';\n        case 'pptx':\n          return 'el-icon-s-marketing';\n        default:\n          return 'el-icon-document';\n      }\n    },\n    /** æ–‡ä»¶é¢„è§ˆ */\n    handlePreview(file) {\n      this.$message(`é¢„è§ˆæ–‡ä»¶ï¼š${file.fileName}`);\n      const fileSuffix = (file.extendName || '').toLowerCase();\n\n      // æ‹¦æˆªå›¾ç‰‡ã€éŸ³é¢‘ã€è§†é¢‘ç±»å‹ï¼Œä¸å¤„ç†\n      const IMAGE_TYPES = ['png', 'jpg', 'jpeg', 'gif', 'svg'];\n      const VIDEO_TYPES = ['mp4'];\n      const AUDIO_TYPES = ['mp3', 'flac'];\n      if (IMAGE_TYPES.includes(fileSuffix) || VIDEO_TYPES.includes(fileSuffix) || AUDIO_TYPES.includes(fileSuffix)) {\n        this.$message.warning('è¯¥æ–‡ä»¶ç±»å‹ä¸æ”¯æŒé¢„è§ˆ');\n        return;\n      }\n\n      // Office æ–‡ä»¶åœ¨çº¿é¢„è§ˆ\n      if ([...officeFileType].includes(fileSuffix)) {\n        this.getFileOnlineViewPathByOffice(file);\n        return;\n      }\n\n      // PDF æ–‡ä»¶æ–°æ ‡ç­¾é¡µæ‰“å¼€\n      if (fileSuffix === 'pdf') {\n        window.open(this.getViewFilePath(file), '_blank');\n        return;\n      }\n\n      // ç»Ÿä¸€ yaml => yml\n      let codeSuffix = fileSuffix === 'yaml' ? 'yml' : fileSuffix;\n\n      // Markdown æ–‡æ¡£\n      if (markdownFileType.includes(codeSuffix)) {\n        Vue.prototype.$openBox.markdownPreview({\n          fileInfo: file,\n          editable: false\n        });\n        return;\n      }\n\n      // æ–‡æœ¬/ä»£ç ç±»æ–‡ä»¶ æˆ– æ— æ‰©å±•åæ–‡ä»¶\n      if (fileSuffixCodeModeMap.has(codeSuffix) || file.isDir === 0 && !file.extendName) {\n        Vue.prototype.$openBox.codePreview({\n          fileInfo: file,\n          isEdit: false\n        });\n        return;\n      }\n\n      // å…¶ä»–ä¸æ”¯æŒé¢„è§ˆ\n      this.$message.warning('æš‚ä¸æ”¯æŒè¯¥ç±»å‹æ–‡ä»¶é¢„è§ˆ');\n    },\n    /**\r\n     * * @param file æ–‡ä»¶åˆ é™¤æ“ä½œ\r\n     */\n    handleDelete(file) {\n      this.$confirm(`ç¡®å®šåˆ é™¤ ${file.fileName} å—ï¼Ÿ`, 'åˆ é™¤ç¡®è®¤').then(() => {\n        this.$emit('fileDelete', file);\n      }).catch(() => {});\n    },\n    handleViewMore() {\n      this.$message('åŠ è½½æ›´å¤šæ–‡ä»¶...');\n    }\n  }\n};","map":{"version":3,"names":["_default","exports","default","name","props","title","type","String","files","Array","required","knowledgeCode","data","fileList","allowedTypes","loadingInstance","methods","beforeUpload","file","isAllowed","includes","isLt5M","size","$message","error","$loading","lock","text","spinner","background","handleSuccess","response","closeLoading","$emit","success","handleError","err","handleExceed","warning","close","getFileIcon","fileType","handlePreview","fileName","fileSuffix","extendName","toLowerCase","IMAGE_TYPES","VIDEO_TYPES","AUDIO_TYPES","officeFileType","getFileOnlineViewPathByOffice","window","open","getViewFilePath","codeSuffix","markdownFileType","Vue","prototype","$openBox","markdownPreview","fileInfo","editable","fileSuffixCodeModeMap","has","isDir","codePreview","isEdit","handleDelete","$confirm","then","catch","handleViewMore"],"sources":["src/views/knowledge/components/FileList.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-list tech-card\">\r\n    <div class=\"file-list-header\">\r\n      <div>\r\n        <h3 class=\"neon-title-small\">{{ title }} <span class=\"sub-text\">// DOCUMENT LOG</span></h3>\r\n        <p class=\"sub-text\">æœ€è¿‘è®¿é—® Â· {{ files.length }} ä¸ªæ–‡ä»¶</p>\r\n      </div>\r\n      <div class=\"btn-group\">\r\n        <el-upload class=\"up