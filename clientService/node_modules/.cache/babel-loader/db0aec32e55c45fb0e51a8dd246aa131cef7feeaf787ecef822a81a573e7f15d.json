r\n      });\r\n      return true;\r\n    },\r\n    handleSuccess(response, file) {\r\n      this.closeLoading();\r\n      this.$emit('dataLoadding');\r\n      this.$message.success(`âœ… \"${file.name}\" ä¸Šä¼ æˆåŠŸ`);\r\n      this.fileList = []\r\n    },\r\n    handleError(err, file) {\r\n      this.closeLoading();\r\n      this.$message.error(`âŒ \"${file.name}\" ä¸Šä¼ å¤±è´¥`);\r\n      this.fileList = []\r\n    },\r\n    handleExceed(files, fileList) {\r\n      this.$message.warning('âš ï¸ æœ€å¤šåªèƒ½ä¸Šä¼  1 ä¸ªæ–‡ä»¶');\r\n    },\r\n    // å…³é—­ loadingï¼ˆç¡®ä¿ä¸€å®šä¼šè¢«è°ƒç”¨ï¼‰\r\n    closeLoading() {\r\n      if (this.loadingInstance) {\r\n        this.loadingInstance.close();\r\n        this.loadingInstance = null;\r\n      }\r\n    },\r\n    /**\r\n     * * @param fileType è·å–æ–‡ä»¶ icon å›¾æ ‡\r\n     */\r\n    getFileIcon(fileType) {\r\n      switch (fileType) {\r\n        case 'pdf':\r\n          return 'el-icon-document';\r\n        case 'docx':\r\n          return 'el-icon-document';\r\n        case 'xlsx':\r\n          return 'el-icon-s-data';\r\n        case 'pptx':\r\n          return 'el-icon-s-marketing';\r\n        default:\r\n          return 'el-icon-document';\r\n      }\r\n    },\r\n    /** æ–‡ä»¶é¢„è§ˆ */\r\n    handlePreview(file) {\r\n      this.$message(`é¢„è§ˆæ–‡ä»¶ï¼š${file.fileName}`)\r\n\r\n      const fileSuffix = (file.extendName || '').toLowerCase()\r\n\r\n      // æ‹¦æˆªå›¾ç‰‡ã€éŸ³é¢‘ã€è§†é¢‘ç±»å‹ï¼Œä¸å¤„ç†\r\n      const IMAGE_TYPES = ['png', 'jpg', 'jpeg', 'gif', 'svg']\r\n      const VIDEO_TYPES = ['mp4']\r\n      const AUDIO_TYPES = ['mp3', 'flac']\r\n\r\n      if (\r\n        IMAGE_TYPES.includes(fileSuffix) ||\r\n        VIDEO_TYPES.includes(fileSuffix) ||\r\n        AUDIO_TYPES.includes(fileSuffix)\r\n      ) {\r\n        this.$message.warning('è¯¥æ–‡ä»¶ç±»å‹ä¸æ”¯æŒé¢„è§ˆ')\r\n        return\r\n      }\r\n\r\n      // Office æ–‡ä»¶åœ¨çº¿é¢„è§ˆ\r\n      if ([...officeFileType].includes(fileSuffix)) {\r\n        this.getFileOnlineViewPathByOffice(file)\r\n        return\r\n      }\r\n\r\n      // PDF æ–‡ä»¶æ–°æ ‡ç­¾é¡µæ‰“å¼€\r\n      if (fileSuffix === 'pdf') {\r\n        window.open(this.getViewFilePath(file), '_blank')\r\n        return\r\n      }\r\n\r\n      // ç»Ÿä¸€ yaml => yml\r\n      let codeSuffix = fileSuffix === 'yaml' ? 'yml' : fileSuffix\r\n\r\n      // Markdown æ–‡æ¡£\r\n      if (markdownFileType.includes(codeSuffix)) {\r\n        Vue.prototype.$openBox.markdownPreview({\r\n          fileInfo: file,\r\n          editable: false\r\n        })\r\n        return\r\n      }\r\n\r\n      // æ–‡æœ¬/ä»£ç ç±»æ–‡ä»¶ æˆ– æ— æ‰©å±•åæ–‡ä»¶\r\n      if (\r\n        fileSuffixCodeModeMap.has(codeSuffix) ||\r\n        (file.isDir === 0 && !file.extendName)\r\n      ) {\r\n        Vue.prototype.$openBox.codePreview({ fileInfo: file, isEdit: false })\r\n        return\r\n      }\r\n\r\n      // å…¶ä»–ä¸æ”¯æŒé¢„è§ˆ\r\n      this.$message.warning('æš‚ä¸æ”¯æŒè¯¥ç±»å‹æ–‡ä»¶é¢„è§ˆ')\r\n    },\r\n    /**\r\n     * * @param file æ–‡ä»¶åˆ é™¤æ“ä½œ\r\n     */\r\n    handleDelete(file) {\r\n      this.$confirm(`ç¡®å®šåˆ é™¤ ${file.fileName} å—ï¼Ÿ`, 'åˆ é™¤ç¡®è®¤')\r\n        .then(() => {\r\n          this.$emit('fileDelete', file);\r\n        })\r\n        .catch(() => { });\r\n    },\r\n    handleViewMore() {\r\n      this.$message('åŠ è½½æ›´å¤šæ–‡ä»¶...');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n/* ================= èµ›åšæœ‹å…‹ä¸»é¢˜å˜é‡ (ç»§æ‰¿è‡ªçˆ¶ç»„ä»¶ï¼Œè¿™é‡Œé‡å¤ä»¥ä¾¿ç‹¬ç«‹è°ƒè¯•) ================= */\r\n@bg-dark: #0f1219;\r\n@bg-panel: rgba(26, 31, 44, 0.7);\r\n@primary-color: #00f2ff; /* Cyber Cyan */\r\n@accent-color: #7d2ae8;  /* Neon Purple */\r\n@text-main: #e2e8f0; // æµ…è‰²ä¸»æ–‡æœ¬\r\n@text-sub: #94a3b8;  // è¾…åŠ©æ–‡æœ¬\r\n@border-color: rgba(255, 255, 255, 0.08);\r\n\r\n/* ================= å®¹å™¨ (Card) æ ·å¼é‡å†™ ================= */\r\n.file-list {\r\n  /* æ›¿æ¢åŸæœ‰çš„ç™½è‰²èƒŒæ™¯ä¸ºç§‘æŠ€å¡ç‰‡é£æ ¼ */\r\n  background: @bg-panel;\r\