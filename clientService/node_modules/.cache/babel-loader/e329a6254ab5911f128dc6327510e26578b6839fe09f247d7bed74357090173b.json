,\n    onWindowMouseUp() {\n      this.isPanning = false;\n      this.draggingNode = null;\n      this.resizingNode = null;\n    },\n    onWheel(e) {\n      const zoomIntensity = 0.1;\n      const delta = e.deltaY > 0 ? -zoomIntensity : zoomIntensity;\n      const newScale = Math.min(5, Math.max(0.1, this.scale + delta));\n      const rect = this.$refs.canvas.getBoundingClientRect();\n      const mouseX = e.clientX - rect.left;\n      const mouseY = e.clientY - rect.top;\n      const worldPos = this.screenToWorld(mouseX, mouseY);\n      this.scale = newScale;\n      this.offsetX = mouseX / this.scale - worldPos.x;\n      this.offsetY = mouseY / this.scale - worldPos.y;\n    },\n    addNode(type = 'text') {\n      // Âú®ËßÜÂè£‰∏≠ÂøÉÁîüÊàê\n      const center = this.screenToWorld(this.$refs.canvas.clientWidth / 2, this.$refs.canvas.clientHeight / 2);\n      this.nodes.push({\n        id: genId('n'),\n        x: center.x - 100,\n        y: center.y - 75,\n        width: 200,\n        height: 150,\n        title: 'Êñ∞ËäÇÁÇπ',\n        contentType: type,\n        content: '',\n        src: ''\n      });\n    },\n    changeNodeType(node, type) {\n      node.contentType = type;\n    },\n    deleteNode(id) {\n      this.nodes = this.nodes.filter(n => n.id !== id);\n      this.edges = this.edges.filter(e => e.fromId !== id && e.toId !== id);\n    },\n    // --- Êñ∞Â¢ûÔºöÂºπÁ™óÁõ∏ÂÖ≥ÈÄªËæë ---\n    openAddChildModal(parent) {\n      this.pendingParentNode = parent;\n      this.newChildType = parent.contentType; // ÈªòËÆ§ÁªßÊâøÁà∂ËäÇÁÇπÁ±ªÂûã\n      this.newChildText = '';\n      this.newChildSrc = '';\n      this.isModalVisible = true;\n\n      // ËÅöÁÑ¶ËæìÂÖ•Ê°Ü (Âú® Vue $nextTick ‰∏≠ÊâßË°åÔºåÁ°Æ‰øùÂºπÁ™óÂ∑≤Ê∏≤Êüì)\n      this.$nextTick(() => {\n        const input = this.$el.querySelector('.modal-content textarea, .modal-content input');\n        if (input) input.focus();\n      });\n    },\n    closeModal() {\n      this.isModalVisible = false;\n      this.pendingParentNode = null;\n      this.newChildText = '';\n      this.newChildSrc = '';\n    },\n    confirmAddChild() {\n      if (!this.isContentValid) return;\n      const parent = this.pendingParentNode;\n      if (!parent) return;\n      const child = {\n        id: genId('n'),\n        // Ëá™Âä®ÂÆö‰ΩçÂú®Áà∂ËäÇÁÇπÂè≥‰æß\n        x: parent.x + parent.width + 100,\n        y: parent.y,\n        width: 200,\n        height: 150,\n        title: 'Â≠êËäÇÁÇπ',\n        contentType: this.newChildType,\n        content: this.newChildType === 'text' ? this.newChildText : '',\n        src: this.newChildType !== 'text' ? this.newChildSrc : ''\n      };\n      this.nodes.push(child);\n      this.edges.push({\n        id: genId('e'),\n        fromId: parent.id,\n        toId: child.id\n      });\n      this.closeModal(); // ÂÆåÊàêÊìç‰ΩúÂêéÂÖ≥Èó≠ÂºπÁ™ó\n    }\n  }\n};","map":{"version":3,"names":["idCounter","genId","prefix","_default","exports","default","name","data","offsetX","offsetY","scale","isPanning","panStart","x","y","panOrigin","draggingNode","nodeDragStart","nodeOrigin","resizingNode","resizeStart","resizeOriginSize","w","h","selectedNodeId","isModalVisible","pendingParentNode","newChildType","newChildText","newChildSrc","nodes","id","width","height","title","contentType","content","src","edges","fromId","toId","computed","isContentValid","trim","length","mounted","window","addEventListener","onWindowMouseMove","onWindowMouseUp","beforeDestroy","removeEventListener","methods","worldToScreen","screenToWorld","getNodeStyle","node","pos","transform","zIndex","getEdgePath","edge","from","find","n","to","start","end","dist","Math","abs","cp1","cp2","onCanvasMouseDown","e","button","clientX","clientY","onNodeMouseDown","onNodeResizeMouseDown","dx","dy","max","onWheel","zoomIntensity","delta","deltaY","newScale","min","rect","$refs","canvas","getBoundingClientRect","mouseX","left","mouseY","top","worldPos","addNode","type","center","clientWidth","clientHeight","push","changeNodeType","d