{"ast":null,"code":"\"use strict\";\n\nObject.defineProperty(exports, \"__esModule\", {\n  value: true\n});\nexports.default = void 0;\nrequire(\"core-js/modules/es.array.push.js\");\nrequire(\"core-js/modules/es.iterator.constructor.js\");\nrequire(\"core-js/modules/es.iterator.find.js\");\nlet idCounter = 1;\nfunction genNodeId() {\n  return 'n' + idCounter++;\n}\nfunction genEdgeId() {\n  return 'e' + idCounter++;\n}\nvar _default = exports.default = {\n  name: 'InfiniteCanvasCurved',\n  data() {\n    return {\n      // 画布变换\n      offsetX: 0,\n      offsetY: 0,\n      scale: 1,\n      // 画布拖拽\n      isPanning: false,\n      panStart: {\n        x: 0,\n        y: 0\n      },\n      panOrigin: {\n        x: 0,\n        y: 0\n      },\n      // 节点拖拽\n      draggingNode: null,\n      nodeDragStart: {\n        x: 0,\n        y: 0\n      },\n      nodeOrigin: {\n        x: 0,\n        y: 0\n      },\n      // 节点缩放\n      resizingNode: null,\n      resizeStartMouse: {\n        x: 0,\n        y: 0\n      },\n      resizeOriginSize: {\n        w: 0,\n        h: 0\n      },\n      nodes: [],\n      edges: []\n    };\n  },\n  created() {\n    // 初始化一个根节点\n    const root = {\n      id: genNodeId(),\n      x: -120,\n      y: -60,\n      width: 240,\n      height: 140,\n      title: '根节点示例',\n      text: '点击右下角 + 可以衍生子节点，并用曲线相连。'\n    };\n    this.nodes.push(root);\n  },\n  methods: {\n    /* 坐标转换：世界 -> 屏幕 */\n    worldToScreenX(x) {\n      return (x + this.offsetX) * this.scale;\n    },\n    worldToScreenY(y) {\n      return (y + this.offsetY) * this.scale;\n    },\n    /* 屏幕 -> 世界 */\n    screenToWorldX(x) {\n      return x / this.scale - this.offsetX;\n    },\n    screenToWorldY(y) {\n      return y / this.scale - this.offsetY;\n    },\n    /* 节点中心点（世界坐标） */\n    getNodeCenter(nodeId) {\n      const node = this.nodes.find(n => n.id === nodeId);\n      if (!node) return {\n        x: 0,\n        y: 0\n      };\n      return {\n        x: node.x + node.width / 2,\n        y: node.y + node.height / 2\n      };\n    },\n    /* 节点的样式（屏幕坐标） */\n    getNodeStyle(node) {\n      const left = this.worldToScreenX(node.x);\n      const top = this.worldToScreenY(node.y);\n      const width = node.width * this.scale;\n      const height = node.height * this.scale;\n      return {\n        position: 'absolute',\n        left: left + 'px',\n        top: top + 'px',\n        width: width + 'px',\n        height: height + 'px'\n      };\n    },\n    /* 曲线连线的 path（使用三次贝塞尔 C） */\n    getEdgePath(edge) {\n      const fromCenter = this.getNodeCenter(edge.fromId);\n      const toCenter = this.getNodeCenter(edge.toId);\n\n      // 世界坐标 -> 屏幕坐标\n      const x1 = this.worldToScreenX(fromCenter.x);\n      const y1 = this.worldToScreenY(fromCenter.y);\n      const x2 = this.worldToScreenX(toCenter.x);\n      const y2 = this.worldToScreenY(toCenter.y);\n\n      // 控制点：简单做一个左右弯曲的 S 型曲线\n      const dx = (x2 - x1) * 0.5;\n      const cx1 = x1 + dx;\n      const cy1 = y1;\n      const cx2 = x2 - dx;\n      const cy2 = y2;\n\n      // C x1 y1, x2 y2, x y\n      return `M ${x1} ${y1} C ${cx1} ${cy1} ${cx2} ${cy2} ${x2} ${y2}`;\n    },\n    /* 画布拖拽 */\n    onCanvasMouseDown(e) {\n      // 只在点击到画布空白区域时开始平移\n      if (e.target !== this.$refs.canvas) return;\n      this.isPanning = true;\n      this.panStart = {\n        x: e.clientX,\n        y: e.clientY\n      };\n      this.panOrigin = {\n        x: this.offsetX,\n        y: this.offsetY\n      };\n    },\n    onCanvasMouseMove(e) {\n      // 缩放节点\n      if (this.resizingNode) {\n        const dx = e.clientX - this.resizeStartMouse.x;\n        const dy = e.clientY - this.resizeStartMouse.y;\n        const node = this.resizingNode;\n        const minSize = 60;\n        node.width = Math.max(this.resizeOriginSize.w + dx / this.scale, minSize);\n        node.height = Math.max(this.resizeOriginSize.h + dy / this.scale, minSize);\n        return;\n      }\n\n      // 拖拽节点\n      if (this.draggingNode) {\n        const dx = e.clientX - this.nodeDragStart.x;\n        const dy = e.clientY - this.nodeDragStart.y;\n        const node = this.draggingNode;\n        node.x = this.nodeOrigin.x + dx / this.scale;\n        node.y = this.nodeOrigin.y + dy / this.scale;\n        return;\n      }\n\n      // 拖动画布\n      if (this.isPanning) {\n        const dx = e.clientX - this.panStart.x;\n        const dy = e.clientY - this.panStart.y;\n        this.offsetX = this.panOrigin.x + dx / this.scale;\n        this.offsetY = this.panOrigin.y + dy / this.scale;\n      }\n    },\n    onCanvasMouseUp() {\n      this.isPanning = false;\n      this.draggingNode = null;\n      this.resizingNode = null;\n    },\n    /* 鼠标滚轮缩放（以鼠标位置为缩放中心） */\n    onWheel(e) {\n      const delta = e.deltaY > 0 ? -0.1 : 0.1;\n      const newScale = Math.min(3, Math.max(0.2, this.scale + delta));\n      if (newScale === this.scale) return;\n      const rect = this.$refs.canvas.getBoundingClientRect();\n      const mouseX = e.clientX - rect.left;\n      const mouseY = e.clientY - rect.top;\n      const worldBeforeX = this.screenToWorldX(mouseX);\n      const worldBeforeY = this.screenToWorldY(mouseY);\n      this.scale = newScale;\n      const worldAfterX = this.screenToWorldX(mouseX);\n      const worldAfterY = this.screenToWorldY(mouseY);\n      this.offsetX += worldBeforeX - worldAfterX;\n      this.offsetY += worldBeforeY - worldAfterY;\n    },\n    /* 节点拖拽 */\n    onNodeMouseDown(node, e) {\n      this.draggingNode = node;\n      this.nodeDragStart = {\n        x: e.clientX,\n        y: e.clientY\n      };\n      this.nodeOrigin = {\n        x: node.x,\n        y: node.y\n      };\n    },\n    /* 节点缩放 */\n    onNodeResizeMouseDown(node, e) {\n      this.resizingNode = node;\n      this.resizeStartMouse = {\n        x: e.clientX,\n        y: e.clientY\n      };\n      this.resizeOriginSize = {\n        w: node.width,\n        h: node.height\n      };\n    },\n    /* 添加根节点（可多棵树） */\n    addRootNode() {\n      const node = {\n        id: genNodeId(),\n        x: -80 + Math.random() * 160,\n        y: -40 + Math.random() * 80,\n        width: 220,\n        height: 130,\n        title: '新根节点',\n        text: '这是新的根节点，你可以继续点 + 创建子节点。'\n      };\n      this.nodes.push(node);\n    },\n    /* 在当前节点上衍生一个子节点，并用曲线相连 */\n    addChildNode(parent) {\n      const child = {\n        id: genNodeId(),\n        x: parent.x + parent.width + 100,\n        y: parent.y,\n        width: 220,\n        height: 130,\n        title: '子节点 ' + parent.id,\n        text: '这是从节点 ' + parent.id + ' 衍生出来的子节点。'\n      };\n      this.nodes.push(child);\n      this.edges.push({\n        id: genEdgeId(),\n        fromId: parent.id,\n        toId: child.id\n      });\n    }\n  }\n};","map":{"version":3,"names":["idCounter","genNodeId","genEdgeId","_default","exports","default","name","data","offsetX","offsetY","scale","isPanning","panStart","x","y","panOrigin","draggingNode","nodeDragStart","nodeOrigin","resizingNode","resizeStartMouse","resizeOriginSize","w","h","nodes","edges","created","root","id","width","height","title","text","push","methods","worldToScreenX","worldToScreenY","screenToWorldX","screenToWorldY","getNodeCenter","nodeId","node","find","n","getNodeStyle","left","top","position","getEdgePath","edge","fromCenter","fromId","toCenter","toId","x1","y1","x2","y2","dx","cx1","cy1","cx2","cy2","onCanvasMouseDown","e","target","$refs","canvas","clientX","clientY","onCanvasMouseMove","dy","minSize","Math","max","onCanvasMouseUp","onWheel","delta","deltaY","newScale","min","rect","getBoundingClientRect","mouseX","mouseY","worldBeforeX","worldBeforeY","worldAfterX","worldAfterY","onNodeMouseDown","onNodeResizeMouseDown","addRootNode","random","addChildNode","parent","child"],"sources":["src/views/GroupView.vue"],"sourcesContent":["<template>\r\n  <div class=\"ice-wrapper\">\r\n    <!-- 顶部工具栏 -->\r\n    <div class=\"ice-toolbar\">\r\n      <button @click=\"addRootNode\">添加根节点</button>\r\n      <span class=\"info\">\r\n        拖动画布：按住空白处拖动 | 缩放：鼠标滚轮 | 拖动节点：按住节点移动 | 子节点：节点右下 + 号\r\n      </span>\r\n    </div>\r\n\r\n    <!-- 无限画布 -->\r\n    <div\r\n      class=\"ice-canvas\"\r\n      ref=\"canvas\"\r\n      @mousedown=\"onCanvasMouseDown\"\r\n      @mousemove=\"onCanvasMouseMove\"\r\n      @mouseup=\"onCanvasMouseUp\"\r\n      @mouseleave=\"onCanvasMouseUp\"\r\n      @wheel.prevent=\"onWheel\"\r\n    >\r\n      <!-- 连线层：使用曲线 path -->\r\n      <svg class=\"ice-edges-layer\">\r\n        <path\r\n          v-for=\"edge in edges\"\r\n          :key=\"edge.id\"\r\n          :d=\"getEdgePath(edge)\"\r\n          fill=\"none\"\r\n          stroke=\"#9ca3af\"\r\n          stroke-width=\"2\"\r\n        />\r\n      </svg>\r\n\r\n      <!-- 节点层 -->\r\n      <div\r\n        v-for=\"node in nodes\"\r\n        :key=\"node.id\"\r\n        class=\"ice-node\"\r\n        :style=\"getNodeStyle(node)\"\r\n        @mousedown.stop=\"onNodeMouseDown(node, $event)\"\r\n      >\r\n        <!-- 内容区域（这里用文本演示，你可以换成图片/视频） -->\r\n        <div class=\"ice-node-content\">\r\n          <div class=\"ice-node-title\">{{ node.title || ('节点 ' + node.id) }}</div>\r\n          <textarea\r\n            v-model=\"node.text\"\r\n            class=\"ice-node-textarea\"\r\n            placeholder=\"在这里输入描述...\"\r\n          />\r\n        </div>\r\n\r\n        <!-- 节点底部工具条 -->\r\n        <div class=\"ice-node-footer\">\r\n          <span class=\"ice-node-type-tag\">node</span>\r\n          <button class=\"ice-node-add\" @click.stop=\"addChildNode(node)\">+</button>\r\n        </div>\r\n\r\n        <!-- 右下角缩放句柄 -->\r\n        <div\r\n          class=\"ice-resize-handle\"\r\n          @mousedown.stop=\"onNodeResizeMouseDown(node, $event)\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nlet idCounter = 1;\r\nfunction genNodeId() {\r\n  return 'n' + idCounter++;\r\n}\r\nfunction genEdgeId() {\r\n  return 'e' + idCounter++;\r\n}\r\n\r\nexport default {\r\n  name: 'InfiniteCanvasCurved',\r\n  data() {\r\n    return {\r\n      // 画布变换\r\n      offsetX: 0,\r\n      offsetY: 0,\r\n      scale: 1,\r\n\r\n      // 画布拖拽\r\n      isPanning: false,\r\n      panStart: { x: 0, y: 0 },\r\n      panOrigin: { x: 0, y: 0 },\r\n\r\n      // 节点拖拽\r\n      draggingNode: null,\r\n      nodeDragStart: { x: 0, y: 0 },\r\n      nodeOrigin: { x: 0, y: 0 },\r\n\r\n      // 节点缩放\r\n      resizingNode: null,\r\n      resizeStartMouse: { x: 0, y: 0 },\r\n      resizeOriginSize: { w: 0, h: 0 },\r\n\r\n      nodes: [],\r\n      edges: []\r\n    };\r\n  },\r\n  created() {\r\n    // 初始化一个根节点\r\n    const root = {\r\n      id: genNodeId(),\r\n      x: -120,\r\n      y: -60,\r\n      width: 240,\r\n      height: 140,\r\n      title: '根节点示例',\r\n      text: '点击右下角 + 可以衍生子节点，并用曲线相连。'\r\n    };\r\n    this.nodes.push(root);\r\n  },\r\n  methods: {\r\n    /* 坐标转换：世界 -> 屏幕 */\r\n    worldToScreenX(x) {\r\n      return (x + this.offsetX) * this.scale;\r\n    },\r\n    worldToScreenY(y) {\r\n      return (y + this.offsetY) * this.scale;\r\n    },\r\n    /* 屏幕 -> 世界 */\r\n    screenToWorldX(x) {\r\n      return x / this.scale - this.offsetX;\r\n    },\r\n    screenToWorldY(y) {\r\n      return y / this.scale - this.offsetY;\r\n    },\r\n\r\n    /* 节点中心点（世界坐标） */\r\n    getNodeCenter(nodeId) {\r\n      const node = this.nodes.find(n => n.id === nodeId);\r\n      if (!node) return { x: 0, y: 0 };\r\n      return {\r\n        x: node.x + node.width / 2,\r\n        y: node.y + node.height / 2\r\n      };\r\n    },\r\n\r\n    /* 节点的样式（屏幕坐标） */\r\n    getNodeStyle(node) {\r\n      const left = this.worldToScreenX(node.x);\r\n      const top = this.worldToScreenY(node.y);\r\n      const width = node.width * this.scale;\r\n      const height = node.height * this.scale;\r\n      return {\r\n        position: 'absolute',\r\n        left: left + 'px',\r\n        top: top + 'px',\r\n        width: width + 'px',\r\n        height: height + 'px'\r\n      };\r\n    },\r\n\r\n    /* 曲线连线的 path（使用三次贝塞尔 C） */\r\n    getEdgePath(edge) {\r\n      const fromCenter = this.getNodeCenter(edge.fromId);\r\n      const toCenter = this.getNodeCenter(edge.toId);\r\n\r\n      // 世界坐标 -> 屏幕坐标\r\n      const x1 = this.worldToScreenX(fromCenter.x);\r\n      const y1 = this.worldToScreenY(fromCenter.y);\r\n      const x2 = this.worldToScreenX(toCenter.x);\r\n      const y2 = this.worldToScreenY(toCenter.y);\r\n\r\n      // 控制点：简单做一个左右弯曲的 S 型曲线\r\n      const dx = (x2 - x1) * 0.5;\r\n      const cx1 = x1 + dx;\r\n      const cy1 = y1;\r\n      const cx2 = x2 - dx;\r\n      const cy2 = y2;\r\n\r\n      // C x1 y1, x2 y2, x y\r\n      return `M ${x1} ${y1} C ${cx1} ${cy1} ${cx2} ${cy2} ${x2} ${y2}`;\r\n    },\r\n\r\n    /* 画布拖拽 */\r\n    onCanvasMouseDown(e) {\r\n      // 只在点击到画布空白区域时开始平移\r\n      if (e.target !== this.$refs.canvas) return;\r\n      this.isPanning = true;\r\n      this.panStart = { x: e.clientX, y: e.clientY };\r\n      this.panOrigin = { x: this.offsetX, y: this.offsetY };\r\n    },\r\n    onCanvasMouseMove(e) {\r\n      // 缩放节点\r\n      if (this.resizingNode) {\r\n        const dx = e.clientX - this.resizeStartMouse.x;\r\n        const dy = e.clientY - this.resizeStartMouse.y;\r\n        const node = this.resizingNode;\r\n        const minSize = 60;\r\n        node.width = Math.max(this.resizeOriginSize.w + dx / this.scale, minSize);\r\n        node.height = Math.max(this.resizeOriginSize.h + dy / this.scale, minSize);\r\n        return;\r\n      }\r\n\r\n      // 拖拽节点\r\n      if (this.draggingNode) {\r\n        const dx = e.clientX - this.nodeDragStart.x;\r\n        const dy = e.clientY - this.nodeDragStart.y;\r\n        const node = this.draggingNode;\r\n        node.x = this.nodeOrigin.x + dx / this.scale;\r\n        node.y = this.nodeOrigin.y + dy / this.scale;\r\n        return;\r\n      }\r\n\r\n      // 拖动画布\r\n      if (this.isPanning) {\r\n        const dx = e.clientX - this.panStart.x;\r\n        const dy = e.clientY - this.panStart.y;\r\n        this.offsetX